ription,Orden,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT VTWEG AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,600 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVTW.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVTW&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVTW.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM accounts_array_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE accounts_array_tr 600'&#xA;UPDATE accounts_array_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT VTWEG AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,600 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVTW.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVTW&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVTW.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND accounts_array_tr.Code = A.Code&#xA;AND accounts_array_tr.Type = A.Type&#xA;AND accounts_array_tr.Language_Tr = A.l&#xA;/*&#xA;ER