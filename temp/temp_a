        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 720_type_unit.sql 4.0.0&#xA;&#xA;/*&#xA;DELETE FROM type_unit&#xA;SELECT * FROM type_unit&#xA;*/&#xA;&#xA;/*&#xA;+--------+-------+------------------------------+&#xA;| tabla  | campo |         descripcion          |&#xA;+--------+-------+------------------------------+&#xA;| FARTER | MEINS | Unidad de medida             |&#xA;| MARA   | MEINS | Unidad de medida de venta    |&#xA;| STPO   | MEINS | Unidad de medida de la lista |&#xA;+--------+-------+------------------------------+&#xA;*/&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;--CONVERT(VARCHAR(4),ERP_MARA.MEINS) AS Unit_Type_Sales, -- Unidad de medida de venta &#xA;SELECT DISTINCT Unit_Type_Sales,SUBSTRING('MARA.MEINS ('+Unit_Type_Sales+')',1,20),'100'&#xA;FROM ERP_IMP_products&#xA;WHERE Unit_Type_Sales NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT Unit_Type_Sales,Unit_Type_Sales,110&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT RTRIM(LTRIM(MEINS))AS Unit_Type_Sales FROM ERP_STPO  --ST&#xA;        UNION&#xA;    SELECT DISTINCT RTRIM(LTRIM(MEINS)) FROM ERP_MARA  &#xA;        UNION&#xA;    SELECT DISTINCT RTRIM(LTRIM(MEINS)) FROM ERP_FARTER&#xA;) A&#xA;WHERE Unit_Type_Sales NOT IN&#xA;(&#xA;    SELECT Unit_Type_Sales FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;/*&#xA;+-------+-------+---------------------------+&#xA;| tabla | campo |        descripcion        |&#xA;+-------+-------+---------------------------+&#xA;| MARA  | GEWEI | Unidad de medida del peso |&#xA;| MARM  | GEWEI | Unidad de peso            |&#xA;+-------+-------+---------------------------+&#xA;*/&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;-- esto se guarda en products.Unit_type_Inv&#xA;-- CONVERT(VARCHAR(4),ERP_MARA.GEWEI) AS Unit_Type_Weight&#xA;SELECT DISTINCT Unit_Type_Weight,SUBSTRING('MARA.GEWEI ('+Unit_Type_Weight+')',1,20),'200'&#xA;FROM ERP_IMP_products&#xA;WHERE Unit_Type_Weight NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT Unit_Type_Weight,Unit_Type_Weight,210&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT RTRIM(LTRIM(GEWEI))AS Unit_Type_Weight FROM ERP_MARA  &#xA;        UNION&#xA;    SELECT DISTINCT RTRIM(LTRIM(GEWEI)) FROM ERP_MARM  &#xA;) A&#xA;WHERE Unit_Type_Weight NOT IN&#xA;(&#xA;    SELECT Unit_Type_Weight FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;/*&#xA;+-------+-------+------------------------------+&#xA;| tabla | campo |         descripcion          |&#xA;+-------+-------+------------------------------+&#xA;| MARA  | VOLEH | Unidad de medida del volumen |&#xA;| MARM  | VOLEH | Unidad de volumen            |&#xA;+-------+-------+------------------------------+&#xA;*/&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT DISTINCT Unit_Type_Volumen,SUBSTRING('MARA.VOLEH ('+Unit_Type_Volumen+')',1,20),'300'&#xA;FROM ERP_IMP_products&#xA;WHERE Unit_Type_Volumen NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT Unit_Type_Volumen,Unit_Type_Volumen,310&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT RTRIM(LTRIM(VOLEH))AS Unit_Type_Volumen FROM ERP_MARA &#xA;        UNION&#xA;    SELECT DISTINCT RTRIM(LTRIM(VOLEH)) FROM ERP_MARM  &#xA;) A&#xA;WHERE Unit_Type_Volumen NOT IN&#xA;(&#xA;    SELECT Unit_Type_Volumen FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;&#xA;--  ERP_MARM.MEINH&#x9;Unidad de medida alternativa&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT DISTINCT MEINH,SUBSTRING('MARM.MEINH ('+MEINH+')',1,20),'400'&#xA;FROM ERP_MARM&#xA;WHERE MEINH NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;--  ERP_FCOND1.KMEIN&#x9;Unidad de medida para la condición&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT DISTINCT KMEIN,SUBSTRING('ERP_FCOND1.KMEIN ('+KMEIN+')',1,20),'500'&#xA;FROM ERP_FCOND1 -- tabla de precios&#xA;WHERE 1=1&#xA;AND LTRIM(RTRIM(KMEIN))!=''&#xA;AND KMEIN NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;ORDER BY 1&#xA;&#xA;INSERT INTO type_unit (Code,Description,Orden)&#xA;SELECT DISTINCT Unit_Measure, Unit_Measure,600&#xA;FROM ERP_IMP_pricing_tables&#xA;WHERE 1=1&#xA;AND Unit_Measure NOT IN&#xA;(&#xA;    SELECT Code FROM type_unit&#xA;)&#xA;&#xA;DELETE FROM type_unit WHERE Code=''&#xA;&#xA;exec prc_set_sysvars 'type_unit'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
