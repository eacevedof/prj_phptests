 --relacion unitorg/salesorg&#xA;            ON s.VKORG = souo.VKORG&#xA;            AND souo.SPRAS = 'S'&#xA;&#xA;            UNION&#xA;&#xA;            SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;            FROM accounts_organization&#xA;            WHERE 1=1&#xA;            AND Delete_Date IS NULL&#xA;        ) AS orgs&#xA;        INNER JOIN ERP_FCPAVEN&#xA;        ON orgs.Code_Unit_Org = ERP_FCPAVEN.BUKRS&#xA;    ) AS orgs&#xA;    ON ERP_TVZBT.ZTERM = orgs.Code&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_payment_terms.Code = A.Code&#xA;AND prj_payment_terms.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_payment_terms.Code_Sales_Org = A.Code_Sales_Org&#xA;AND A.l='S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE prj_payment_terms_tr'&#xA;UPDATE prj_payment_terms_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZTERM AS Code&#xA;    ,orgs.Code_Unit_Org&#xA;    ,orgs.Code_Sales_Org&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVZBT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVZBT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVZBT.SPRAS&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,ZTERM AS Code&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT u.VKORG+u.VTWE