          SQLTask:SqlStatementSource="-- 804_prj_price_list.sql 3.0.0&#xA;&#xA;/*&#xA;TRUNCATE TABLE prj_price_list --no va&#xA;&#xA;DELETE FROM prj_price_list --no va, hay fks apuntando a esta tabla&#xA;-- Instrucción DELETE en conflicto con la restricción REFERENCE &quot;pjodr_ie1716_FK7&quot;. El conflicto ha aparecido en la base de datos &quot;crm3_flamagas&quot;&#xA;--, tabla &quot;dbo.prj_orders_lines&quot;, column 'Code_Price_List_default'.&#xA;&#xA;SELECT * FROM prj_price_list&#xA;*/&#xA;&#xA;PRINT 'UPDATE prj_price_list'&#xA;UPDATE prj_price_list&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KONDA AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_T188T.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T188T&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_price_list.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_price_list'&#xA;INSERT INTO prj_price_list (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KONDA AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_T188T.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T188T&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code&#xA;    FROM prj_price_list o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code  &#xA;)&#xA;&#xA;PRINT 'DELETE prj_price_list'&#xA;UPDATE prj_price_list&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KONDA AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_T188T.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T188T&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_price_list.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE prj_price_list_tr'&#xA;UPDATE prj_price_list_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KONDA AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T188T.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T188T&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_T188T.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_price_list_tr.Code = A.Code&#xA;AND prj_price_list_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT prj_price_list_tr'&#xA;INSERT INTO prj_price_list_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Tr