        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 501_prj_des_array_org_501.sql 4.0.0&#xA;&#xA;/*&#xA;prod_org.Code_ClassificationE&#xA;&#xA;Almacen --&gt; SELECT * FROM ERP_T001LPJ  &#xA;Almacén: prj_orders_lines.Code_Warehouse. Se relación con la tabla &#xA;“prj_des_array_org para type = 501” para sacar la descripción&#xA;*/&#xA;&#xA;-- ALMACEN: -- !!! no tiene traducciones&#xA;-- DELETE FROM prj_des_array_org WHERE Type='501'&#xA;-- SELECT * FROM prj_des_array_org WHERE Type='501'&#xA;-- prj_orders_lines.Code_Warehouse&#xA;&#xA;-- 22/02/2018 concatenar para formar Code&#xA;PRINT 'UPDATE prj_des_array_org 501'&#xA;UPDATE prj_des_array_org&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden,&#xA;Extra_Type = A.Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_T001LPJ.LGORT+ERP_T001LPJ.WERKS AS Code&#xA;    ,'501' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001LPJ.LGOBE))) AS Description&#xA;    -- 22/02/2018 antes el orden tenia q ser 1, ahora me tengo que traer todos&#xA;    ,CASE WHEN VALIDO = 'S' THEN &#xA;        CASE WHEN DEFECTO = 'S' THEN '1' ELSE '2' &#xA;        END ELSE '0' &#xA;    END AS Orden&#xA;    ,'S' AS l&#xA;    ,ERP_T001LPJ.WERKS AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001LPJ.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T001LPJ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso     &#xA;    ON ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG = uso.Code_Sales_Org &#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_des_array_org.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_des_array_org.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_des_array_org.Code = A.Code&#xA;AND prj_des_array_org.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_des_array_org 501'&#xA;INSERT INTO prj_des_array_org (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code_Unit_Org,Code_Sales_Org,Code,Type,Description,Orden,Extra_Type)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code_Unit_Org,Code_Sales_Org,Code,Type,Description,Orden,Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_T001LPJ.LGORT+ERP_T001LPJ.WERKS AS Code&#xA;    ,'501' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001LPJ.LGOBE))) AS Description&#xA;    -- 22/02/2018 antes el orden tenia q ser 1, ahora me tengo que traer todos&#xA;    ,CASE WHEN VALIDO = 'S' THEN &#xA;        CASE WHEN DEFECTO = 'S' THEN '1' ELSE '2' &#xA;        END ELSE '0' &#xA;    END AS Orden&#xA;    ,'S' AS l&#xA;    ,ERP_T001LPJ.WERKS AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001LPJ.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T001LPJ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso     &#xA;    ON ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG = uso.Code_Sales_Org &#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT DISTINCT Code,Type,Code_Unit_Org,Code_Sales_Org&#xA;    FROM prj_des_array_org o&#xA;    WHERE 1=1&#xA;    AND o.Code_Unit_Org = A.Code_Unit_Org&#xA;    AND o.Code_Sales_Org = A.Code_Sales_Org&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE prj_des_array_org 501'&#xA;UPDATE prj_des_array_org&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_T001LPJ.LGORT+ERP_T001LPJ.WERKS AS Code&#xA;    ,'501' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001LPJ.LGOBE))) AS Description&#xA;    -- 22/02/2018 antes el orden tenia q ser 1, ahora me tengo que traer todos&#xA;    ,CASE WHEN VALIDO = 'S' THEN &#xA;        CASE WHEN DEFECTO = 'S' THEN '1' ELSE '2' &#xA;        END ELSE '0' &#xA;    END AS Orden&#xA;    ,'S' AS l&#xA;    ,ERP_T001LPJ.WERKS AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001LPJ.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T001LPJ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso     &#xA;    ON ERP_T001LPJ.VKORG + ERP_T001LPJ.VTWEG = uso.Code_Sales_Org &#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_des_array_org.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_des_array_org.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_des_array_org.Code = A.Code&#xA;AND prj_des_array_org.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'NO TIENE TRADUCCIONES...'&#xA;&#xA;/*&#xA;prj_des_array_org&#xA;+----------------+------+-----------+----------+&#xA;|   fieldname    | ispk | fieldtype | fieldlen |&#xA;+----------------+------+-----------+----------+&#xA;| Code_Unit_Org  | Y    | varchar   |       12 |&#xA;| Code_Sales_Org | Y    | varchar   |       12 |&#xA;| Code           | Y    | varchar   |       12 |&#xA;| Type           | Y    | numeric   |        5 |&#xA;| Description    |      | nvarchar  |       50 |&#xA;| Orden          |      | int       |        4 |&#xA;| Extra_Type     |      | varchar   |       12 |&#xA;+----------------+------+-----------+----------+&#xA;&#xA;ERP_T001LPJ     CENTROS/ALMACENES POR ORGANIZACION/CANAL&#xA;+---------+-------------------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;|  campo  |                         descripcion                         | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+---------+-------------------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| WERKS   | Centro de expedición por defecto                            |     1 | CHAR       |        4 |         0 | T001W          |&#xA;| LGORT   | Código de almacén                                           |     2 | CHAR       |        4 |         0 |                |&#xA;| LGOBE   | Denominación de almacén                                     |       | CHAR       |       16 |         0 |                |&#xA;| VALIDO  | Indica si el almacén es válido para Org./Canal/Centro       |       | CHAR       |        1 |         0 |                |&#xA;| DEFECTO | Indica que es el almacén por defecto para Org./Canal/Centro |       | CHAR       |        1 |         0 |                |&#xA;| CODEST  | No se usa                                                   |       | CHAR       |        1 |         0 |                |&#xA;| VKORG   | Organización de ventas                                      |     3 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG   | Canal de distribución                                       |     4 | CHAR       |        2 |         0 | TVTW           |&#xA;+---------+-------------------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;*/&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
