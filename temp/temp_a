    <DTS:Executable
      DTS:refId="Package\805_prj_visit_type   CODIGOS DE GESTION PARA VISITAS"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{d578f80c-e4b6-4611-9e69-a79396dfb239}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="805_prj_visit_type   CODIGOS DE GESTION PARA VISITAS"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="3">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 805_prj_visit_type 6.1.0&#xA;/*&#xA;CODIGOS DE GESTION PARA VISITAS&#xA;&#xA;PEDIDO (ZDETALLE de SAP) [char](1),  Indicador que implica pedido&#xA;REAL (ZVISITA de SAP) [Char](1),  Indicador de visita real &#xA;&#xA;-- REVISAR&#xA;-- Esta se creo con este nombre pero se baja como paquete prj_visits_type&#xA;PATRI: otra cosa que me he dado cuenta ahora. Para poder hacer un pedido en una visita necesitamos que el campo &#xA;&quot;pedido&quot; de la tabla PRJ_VISITS_TYPE esté a uno en alguno de los tipos de visita que hay, &#xA;ahora mismo este campo está vacío para los dos tipos que hay en la tabla. &#xA;&#xA;TRUNCATE TABLE prj_visit_type  --ok funciona&#xA;&#xA;-- ORIGINAL -&gt; TRUNCATE TABLE prj_visits_type &quot;pjvst_a1504_FK0&quot;. El conflicto ha aparecido en fk &quot;dbo.prj_visits_cap&quot;, column 'Code_Type'.&#xA;&#xA;DELETE FROM prj_visit_type&#xA;SELECT * FROM prj_visit_type&#xA;-- SELECT * FROM prj_visit_type_tr  -- NO ESTA CREADA, pero tampoco ERP_FGESTI tiene columna de idioma&#xA;*/&#xA;&#xA;PRINT 'UPDATE prj_visit_type'&#xA;UPDATE prj_visit_type&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;--Code_Unit_Org = A.Code_Unit_Org,&#xA;--Code_Sales_Org = A.Code_Sales_Org,&#xA;--Code = A.Code,&#xA;Description = A.Description,&#xA;PEDIDO = A.PEDIDO,&#xA;REAL = A.REAL,&#xA;Orden = A.Orden,&#xA;Active = A.Active&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZCODGES AS Code,Code_Unit_Org,Code_Sales_Org&#xA;    ,ZDETALLE AS PEDIDO&#xA;    ,ZVISITA AS REAL&#xA;    ,ZNGESTI+'-'+Code_Sales_Org AS Description&#xA;    ,100 AS Orden&#xA;    ,1 AS Active&#xA;    ,'S' AS l&#xA;    ,Transfer_Status&#xA;    ,Transfer_Date&#xA;    FROM &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uos&#xA;    CROSS JOIN &#xA;    (&#xA;        -- real=zvisita,pedido=zdetalle&#xA;        SELECT DISTINCT ZCODGES&#xA;        ,CASE ZDETALLE&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZDETALLE&#xA;        ,CASE ZVISITA&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZVISITA&#xA;        ,ZNGESTI&#xA;        ,IIF(ISNULL(ERP_FGESTI.STATUS,'')='',0,9) AS Transfer_Status&#xA;        ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;        FROM ERP_FGESTI&#xA;    ) AS ERP_FGESTI&#xA;) AS A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_visit_type.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_visit_type.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_visit_type.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_visit_type'&#xA;INSERT INTO prj_visit_type (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code_Unit_Org, Code_Sales_Org,Code,Description,PEDIDO,REAL,Orden,Active)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code_Unit_Org, Code_Sales_Org,Code,Description,PEDIDO,REAL,Orden,Active&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZCODGES AS Code,Code_Unit_Org,Code_Sales_Org&#xA;    ,ZDETALLE AS PEDIDO&#xA;    ,ZVISITA AS REAL&#xA;    ,ZNGESTI+'-'+Code_Sales_Org AS Description&#xA;    ,100 AS Orden&#xA;    ,1 AS Active&#xA;    ,'S' AS l&#xA;    ,Transfer_Status&#xA;    ,Transfer_Date&#xA;    FROM &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uos&#xA;    CROSS JOIN &#xA;    (&#xA;        -- real=zvisita,pedido=zdetalle&#xA;        SELECT DISTINCT ZCODGES&#xA;        ,CASE ZDETALLE&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZDETALLE&#xA;        ,CASE ZVISITA&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZVISITA&#xA;        ,ZNGESTI&#xA;        ,IIF(ISNULL(ERP_FGESTI.STATUS,'')='',0,9) AS Transfer_Status&#xA;        ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;        FROM ERP_FGESTI&#xA;    ) AS ERP_FGESTI&#xA;) AS A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code_Unit_Org,Code_Sales_Org,Code&#xA;    FROM prj_visit_type o&#xA;    WHERE 1=1&#xA;    AND o.Code_Unit_Org = A.Code_Unit_Org&#xA;    AND o.Code_Sales_Org = A.Code_Sales_Org&#xA;    AND o.Code = A.Code&#xA;)&#xA;&#xA;PRINT 'DELETE prj_visit_type'&#xA;UPDATE prj_visit_type&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZCODGES AS Code,Code_Unit_Org,Code_Sales_Org&#xA;    ,ZDETALLE AS PEDIDO&#xA;    ,ZVISITA AS REAL&#xA;    ,ZNGESTI+'-'+Code_Sales_Org AS Description&#xA;    ,100 AS Orden&#xA;    ,1 AS Active&#xA;    ,'S' AS l&#xA;    ,Transfer_Status&#xA;    ,Transfer_Date&#xA;    FROM &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uos&#xA;    CROSS JOIN &#xA;    (&#xA;        -- real=zvisita,pedido=zdetalle&#xA;        SELECT DISTINCT ZCODGES&#xA;        ,CASE ZDETALLE&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZDETALLE&#xA;        ,CASE ZVISITA&#xA;            WHEN 'S' THEN '1'&#xA;            ELSE '0'&#xA;        END AS ZVISITA&#xA;        ,ZNGESTI&#xA;        ,IIF(ISNULL(ERP_FGESTI.STATUS,'')='',0,9) AS Transfer_Status&#xA;        ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;        FROM ERP_FGESTI&#xA;    ) AS ERP_FGESTI&#xA;) AS A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_visit_type.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_visit_type.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_visit_type.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;-- se necesita los codigos pq en prj_visit_cap hay una fk a esta tabla&#xA;PRINT 'INTO prj_visits_type'&#xA;INSERT INTO prj_visits_type(Code,Description)&#xA;SELECT DISTINCT Code,Code AS Description &#xA;FROM prj_visit_type&#xA;WHERE Code NOT IN&#xA;(&#xA;    SELECT Code &#xA;    FROM prj_visits_type&#xA;)&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;/*&#xA;PRINT 'UPDATE prj_visit_type_tr'&#xA;UPDATE prj_visit_type_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,Code&#xA;    ,UPPER(LTRIM(RTRIM(Description))) AS Description&#xA;    ,'P' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FGASOL&#xA;    CROSS JOIN&#xA;    (&#xA;        -- según la carga de erp_imp_sales_organization&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR --AS Code_Seller&#xA;        --,'' AS status&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;    ) orgs&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_visit_type_tr.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_visit_type_t