        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 500_prj_des_array_org_500.sql 2.2.0&#xA;&#xA;/*&#xA;prod_org.Code_ClassificationE&#xA;Centro: Prj_orders_lines. Code_Lock. Se relación con la tabla &#xA;“prj_des_array_org para type = 500” para sacar la descripción&#xA;*/&#xA;&#xA;-- CENTROS !!! no tiene traducciones&#xA;/*&#xA;prj_orders_lines.Code_Lock&#xA;prod_org.Code_ClassificationE&#xA;Centros --&gt; select distinct werks  from MVKE where vkorg= '&quot; &amp; gl_orgven_ped &amp; &quot;'&#xA;and vtweg='&quot; &amp; gl_canal_ped &amp; &quot;' and MVKE.MATNR='&quot; &amp; table2.Columns(1).Text &amp; &quot;'&#xA;and  '' &amp; werks &lt;&gt; ''  &quot;&#xA;*/&#xA;-- SELECT * FROM prj_des_array_org WHERE Type='500'&#xA;-- DELETE FROM prj_des_array_org WHERE Type='500'&#xA;&#xA;PRINT 'UPDATE prj_des_array_org 500'&#xA;UPDATE prj_des_array_org&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden,&#xA;Extra_Type = A.Extra_Type&#xA;FROM&#xA;(&#xA;    -- Esto carga centros. No lleva Extra_Type&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_TVKWZ.WERKS AS Code&#xA;    ,'500' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001W.NAME1))) AS Description&#xA;    ,500 AS Orden&#xA;    ,'S' AS l&#xA;    ,NULL AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001W.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVKWZ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN ERP_T001W  --CENTROS DE EXPEDICION&#xA;    ON ERP_TVKWZ.WERKS = ERP_T001W.WERKS &#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso &#xA;    ON ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG = uso.Code_Sales_Org&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_des_array_org.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_des_array_org.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_des_array_org.Code = A.Code&#xA;AND prj_des_array_org.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_des_array_org 500'&#xA;INSERT INTO prj_des_array_org (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code_Unit_Org,Code_Sales_Org,Code,Type,Description,Orden,Extra_Type)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code_Unit_Org,Code_Sales_Org,Code,Type,Description,Orden,Extra_Type&#xA;FROM&#xA;(&#xA;    -- Esto carga centros. No lleva Extra_Type&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_TVKWZ.WERKS AS Code&#xA;    ,'500' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001W.NAME1))) AS Description&#xA;    ,500 AS Orden&#xA;    ,'S' AS l&#xA;    ,NULL AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001W.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVKWZ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN ERP_T001W  --CENTROS DE EXPEDICION&#xA;    ON ERP_TVKWZ.WERKS = ERP_T001W.WERKS &#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso &#xA;    ON ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG = uso.Code_Sales_Org&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT DISTINCT Code,Type,Code_Unit_Org,Code_Sales_Org&#xA;    FROM prj_des_array_org o&#xA;    WHERE 1=1&#xA;    AND o.Code_Unit_Org = A.Code_Unit_Org&#xA;    AND o.Code_Sales_Org = A.Code_Sales_Org&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE prj_des_array_org 500'&#xA;UPDATE prj_des_array_org&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    -- Esto carga centros. No lleva Extra_Type&#xA;    SELECT DISTINCT uso.Code_Unit_Org AS Code_Unit_Org&#xA;    ,ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG AS Code_Sales_Org&#xA;    ,ERP_TVKWZ.WERKS AS Code&#xA;    ,'500' AS Type&#xA;    ,LTRIM(RTRIM(UPPER(ERP_T001W.NAME1))) AS Description&#xA;    ,500 AS Orden&#xA;    ,'S' AS l&#xA;    ,NULL AS Extra_Type&#xA;    ,IIF(ISNULL(ERP_T001W.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVKWZ -- CENTROS POR ORGANIZACION/CANAL&#xA;    INNER JOIN ERP_T001W  --CENTROS DE EXPEDICION&#xA;    ON ERP_TVKWZ.WERKS = ERP_T001W.WERKS &#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR AS Code_Seller&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) AS uso &#xA;    ON ERP_TVKWZ.VKORG + ERP_TVKWZ.VTWEG = uso.Code_Sales_Org&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_des_array_org.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_des_array_org.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_des_array_org.Code = A.Code&#xA;AND prj_des_array_org.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'NO TIENE TRADUCCIONES...'&#xA;&#xA;/*&#xA;prj_des_array_org&#xA;+----------------+------+-----------+----------+&#xA;|   fieldname    | ispk | fieldtype | fieldlen |&#xA;+----------------+------+-----------+----------+&#xA;| Code_Unit_Org  | Y    | varchar   |       12 |&#xA;| Code_Sales_Org | Y    | varchar   |       12 |&#xA;| Code           | Y    | varchar   |       12 |&#xA;| Type           | Y    | numeric   |        5 |&#xA;| Description    |      | nvarchar  |       50 |&#xA;| Orden          |      | int       |        4 |&#xA;| Extra_Type     |      | varchar   |       12 |&#xA;+----------------+------+-----------+----------+&#xA;&#xA;ERP_TVKWZ&#x9;ZSDTV658&#x9;CENTROS POR ORGANIZACION/CANAL&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |      descripcion       | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| VKORG | Organización de ventas |     1 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG | Canal de distribución  |     2 | CHAR       |        2 |         0 | TVTW           |&#xA;| WERKS | Centro de expedición   |     3 | CHAR       |        4 |         0 | T001W          |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_T001W&#x9;ZSDTV648&#x9;CENTROS DE EXPEDICION&#xA;+-------+----------------------------------+-------+------------+----------+-----------+&#xA;| campo |           descripcion            | clave | tipo_datos | longitud | decimales |&#xA;+-------+----------------------------------+-------+------------+----------+-----------+&#xA;| WERKS | Centro de expedición por defecto |     1 | CHAR       |        4 |         0 |&#xA;| NAME1 | Nombre del centro de expedición  |       | CHAR       |       30 |         0 |&#xA;+-------+----------------------------------+-------+------------+----------+-----------+&#xA;&#xA;*/&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
