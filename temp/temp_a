          SQLTask:SqlStatementSource="-- 102_accounts_array_503.sql &#xA;-- ZONAS DE TRANSPORTE&#xA;-- accounts.Code_ClassificationB&#xA;&#xA;/*&#xA;SELECT DISTINCT Code_ClassificationB FROM ERP_IMP_accounts --acc_array(503)&#xA;-- TRUNCATE TABLE accounts_array&#xA;&#xA;DELETE FROM accounts_array WHERE Type='503'&#xA;SELECT * FROM accounts_array WHERE Type='503'&#xA;SELECT * FROM accounts_array_tr WHERE Type='503'&#xA;&#xA;Problema, mismo codigo distinto pais. Solo cargo españa&#xA;+-------+-------+------------+------------------+&#xA;| SPRAS | LAND1 |   ZONE1    |      VTEXT       |&#xA;+-------+-------+------------+------------------+&#xA;| S     | ES    | 0000000001 | Alava            |&#xA;| P     | PT    | 0000000001 | VIANA DO CASTELO |&#xA;| S     | PT    | 0000000001 | portugal         |&#xA;+-------+-------+------------+------------------+&#xA;*/&#xA;&#xA;PRINT 'UPDATE accounts_array 503'&#xA;UPDATE accounts_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    WHERE 1=1&#xA;    AND LAND1='ES' --zonas de españa&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND accounts_array.Code = A.Code&#xA;AND accounts_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT accounts_array 503'&#xA;INSERT INTO accounts_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    WHERE 1=1&#xA;    AND LAND1='ES'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM accounts_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE accounts_array 503'&#xA;UPDATE accounts_array&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    WHERE 1=1&#xA;    AND LAND1='ES'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND accounts_array.Code = A.Code&#xA;AND accounts_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE accounts_array_tr 503'&#xA;UPDATE accounts_array_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TZONE.SPRAS&#xA;    WHERE 1=1&#xA;    AND LAND1!='ES'&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND accounts_array_tr.Code = A.Code&#xA;AND accounts_array_tr.Type = A.Type&#xA;AND accounts_array_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT accounts_array_tr 503'&#xA;INSERT INTO accounts_array_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TZONE.SPRAS&#xA;    WHERE 1=1&#xA;    AND LAND1!='ES'  &#xA;    --PARCHE por repetidos&#xA;    AND (ZONE1!='0000000001' AND VTEXT!='PORTUGAL')&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM accounts_array_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE accounts_array_tr 503'&#xA;UPDATE accounts_array_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZONE1 AS Code,'503' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,503 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TZONE.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TZONE&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TZONE.SPRAS&#xA;    WHERE 1=1&#xA;    AND LAND1!='ES' -- las zonas que no son de españa&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND accounts_array_tr.Code = A.Code&#xA;AND accounts_array_tr.Type = A.Type&#xA;AND accounts_array_tr.Language_Tr = A.l&#xA;&#xA;/*&#xA;ERP_TZONE  ZONAS DE TRANSPORTE&#xA;+-------+--------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |       descripcion        | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+--------------------------+-------+------------+----------+-----------+----------------+&#xA;| SPRAS | Idioma de la descripción |     1 | CHAR       |        1 |         0 |                |&#xA;| LAND1 | Código de país           |     2 | CHAR       |        3 |         0 | T005T          |&#xA;| ZONE1 | Zona de transporte       |     3 | CHAR       |       10 |         0 |                |&#xA;| VTEXT | Descripción              |       | CHAR       |       20 |         0 |                |&#xA;+-------+--------------------------+-------+------------+----------+-----------+----------------+&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
