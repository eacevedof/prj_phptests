&#xA;&#xA;PRINT 'DELETE populations'&#xA;UPDATE populations&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CITYC AS Code,REGIO AS Code_Province,LAND1 AS Code_Country&#xA;    ,UPPER(LTRIM(RTRIM(BEZEI))) AS Description&#xA;    ,500 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_T005H.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T005H&#xA;    WHERE 1=1&#xA;    AND CITYC != ''&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND populations.Code = A.Code&#xA;AND populations.Code_Country = A.Code_Country&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE populations_tr'&#xA;UPDATE populations_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CITYC AS Code,REGIO AS Code_Province,LAND1 AS Code_Country&#xA;    ,UPPER(LTRIM(RTRIM(BEZEI))) AS Description&#xA;    ,500 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T005H.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T005H&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_T005H.SPRAS&#xA;    WHERE 1=1&#xA;    AND CITYC != ''&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND populations_tr.Code = A.Code&#xA;AND populations_tr.Code_Country = A.Code_Country&#xA;AND populations_tr.Language_Tr = A.l&#xA;&#xA;&#xA;PRINT 'INSERT populations_tr'&#xA;INSERT INTO populations_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Code_Province,Code_Country,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Code_Province,Code_Country,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CITYC AS Code,REGIO AS Code_Province,LAND1 AS Code_Country&#xA;    ,UPPER(LTRIM(RTRIM(BEZEI))) AS Description&#xA;    ,500 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T005H.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T005H&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_T005H.SPRAS&#xA;    WHERE 1=1&#xA;    AND CITYC != ''&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Code_Country&#xA;    FROM populations_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Code_Country = A.Code_Country&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE populations_tr'&#xA;UPDATE populations_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CITYC AS Code,REGIO AS Code_Province,LAND1 AS Code_Country&#xA;    ,UPPER(LTRIM(RTRIM(BEZEI))) AS Description&#xA;    ,500 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T005H.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T005H&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#x