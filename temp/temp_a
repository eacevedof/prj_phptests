rden,Code_Unit_Org,Code_Sales_Org)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,Days,Orden,Code_Unit_Org,Code_Sales_Org&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT erp.*&#xA;    ,orgs.Code_Unit_Org ,orgs.Code_Sales_Org&#xA;    FROM &#xA;    (&#xA;        SELECT DISTINCT ZTERM AS Code&#xA;        ,LTRIM(RTRIM(UPPER(VTEXT))) AS Description&#xA;        ,100 AS Orden&#xA;        ,Language_Tr AS l&#xA;        ,CONVERT(NUMERIC(4,0),REPLACE(REPLACE(LTRIM(RTRIM(PPAGO)),'.',''),',','.')) AS Days&#xA;        ,PPAGO&#xA;        ,IIF(ISNULL(ERP_TVZBT.STATUS,'')='',0,9) AS Transfer_Status&#xA;        ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;        FROM ERP_TVZBT&#xA;        INNER JOIN &#xA;        (&#xA;            SELECT ClientCode,Code AS Language_Tr&#xA;            FROM core_users_languages&#xA;            WHERE 1=1&#xA;            AND ClientCode IS NOT NULL&#xA;            AND Code!='ES'&#xA;        ) AS l&#xA;        ON l.ClientCode = ERP_TVZBT.SPRAS&#xA;        WHERE 1=1&#xA;        --AND SPRAS='S'&#xA;    ) AS erp&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,ZTERM AS Code&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;            --,s.SPART --sector se quita&#xA;            ,souo.BUKRS AS Code_Unit_Org&#xA;            --,u.ZCEMPLE AS Code_User&#xA;            --,s.PERNR AS Code_Seller&#xA;            FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;            INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;            ON u.VKORG = s.VKORG --code_unit_org&#xA;            AND u.VTWEG = s.VTWEG --canal dist&#xA;            AND u.PERNR = s.PERNR  --code_seller&#xA;            INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;            ON s.VKORG = souo.VKORG&#xA;            AND souo.SPRAS = 'S'&#xA;&#xA;            UNION&#x