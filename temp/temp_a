          SQLTask:SqlStatementSource="-- 602_products_array_502.sql 1.0.0&#xA;-- TIPO DE MATERIAL  no hay tabla descriptora&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE products_array&#xA;&#xA;DELETE FROM products_array WHERE Type='502'&#xA;SELECT * FROM products_array WHERE Type='502'&#xA;SELECT * FROM products_array_tr WHERE Type='502'&#xA;*/&#xA;&#xA;PRINT 'UPDATE products_array 502'&#xA;UPDATE products_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT SUBSTRING(MTART,1,12) AS Code,'502' AS Type&#xA;    ,UPPER(LTRIM(RTRIM('TIP. MAT.'+MTART))) AS Description&#xA;    ,100 AS Orden&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_MARA&#xA;    WHERE 1=1&#xA;    AND ISNULL(MTART,'')!=''&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND products_array.Code = A.Code&#xA;AND products_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT products_array 502'&#xA;INSERT INTO products_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden)&#xA;&#xA;SELECT DISTINCT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT SUBSTRING(MTART,1,12) AS Code,'502' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(MTART))) AS Description&#xA;    ,100 AS Orden&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_MARA&#xA;    WHERE 1=1&#xA;    AND ISNULL(MTART,'')!=''&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM products_array o&#xA;