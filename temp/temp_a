          SQLTask:SqlStatementSource="-- 611_products_hierarchy1.sql 3.0.0&#xA;-- MARCAS ESTADISTICAS DE LOS MATERIALES&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE products_hierarchy1&#xA;&#xA;SELECT * FROM products_hierarchy1 &#xA;SELECT * FROM products_hierarchy1_tr&#xA;*/&#xA;&#xA;--PRINT 'TRUNCATE TABLE products_hierarchy1'&#xA;-- TRUNCATE TABLE products_hierarchy1 no vale pq hay fks &#xA;&#xA;exec prc_cleancrlf 'ERP_FMARCA','CMARES'&#xA;exec prc_cleancrlf 'ERP_FMARCA','VTEXT'&#xA;&#xA;PRINT 'UPDATE products_hierarchy1'&#xA;UPDATE products_hierarchy1&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND products_hierarchy1.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT products_hierarchy1'&#xA;INSERT INTO products_hierarchy1 (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code&#xA;    FROM products_hierarchy1 o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code  &#xA;)&#xA;&#xA;PRINT 'DELETE products_hierarchy1'&#xA;UPDATE products_hierarchy1&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND products_hierarchy1.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE products_hierarchy1_tr'&#xA;UPDATE products_hierarchy1_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_FMARCA.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND products_hierarchy1_tr.Code = A.Code&#xA;AND products_hierarchy1_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT products_hierarchy1_tr'&#xA;INSERT INTO products_hierarchy1_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_FMARCA.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code&#xA;    FROM products_hierarchy1_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE products_hierarchy1_tr'&#xA;UPDATE products_hierarchy1_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT CMARES AS Code&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_FMARCA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FMARCA&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_FMARCA.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND products_hierarchy1_tr.Code = A.Code&#xA;AND products_hierarchy1_tr.Language_Tr = A.l&#xA;&#xA;/*&#xA;ERP_FMARCA      MARCAS ESTADISTICAS DE LOS MATERIALES&#xA;&#xA;+--------+-