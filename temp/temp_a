          SQLTask:SqlStatementSource="-- 108_accounts_type_functions.sql 2.2.0&#xA;/*&#xA;SELECT * FROM accounts_type_functions&#xA;SELECT * FROM ERP_KNVP -- CLIENTES - INTERLOCUTORES&#xA;TRUNCATE TABLE accounts_type_functions&#xA;DELETE FROM accounts_type_functions&#xA;*/&#xA;&#xA;-- exec prc_cleancrlf 'ERP_KNVP','PARVW'-- CLIENTES - INTERLOCUTORES&#xA;-- exec prc_cleancrlf 'ERP_TPAER','PARVW'-- FUNCIONES DEL GRUPO DE CUENTAS&#xA;&#xA;--TRUNCATE TABLE accounts_type_functions&#xA;--TRUNCATE TABLE accounts_type_functions_tr&#xA;&#xA;-- PARVW Función de interlocutor WE=Destinatario mercancía, RE=Destinatario factura, RG=Responsable pago&#xA;INSERT INTO accounts_type_functions(Code,Description,Orden,Type)&#xA;&#xA;SELECT Code, &#xA;CASE Code&#xA;    WHEN 'RE' THEN 'DESTINARIO FACTURA'&#xA;    WHEN 'RG' THEN 'RESPONSABLE PAGO'&#xA;    WHEN 'TR' THEN 'TRANSPORTISTA'&#xA;    WHEN 'WE' THEN 'DESTINATARIO DE MERCANCIA'&#xA;    WHEN 'AG' THEN 'SOLICITANTE DE PEDIDO'&#xA;    ELSE Code&#xA;END AS Description,&#xA;100 AS Orden,&#xA;1 AS Type&#xA;FROM &#xA;(&#xA;    SELECT DISTINCT PARVW AS Code FROM ERP_KNVP&#xA;        UNION&#xA;    SELECT DISTINCT PARVW AS Code FROM ERP_TPAER&#xA;)AS A&#xA;WHERE 1=1&#xA;AND NOT EXISTS&#xA;(&#xA;    SELECT Code,Type&#xA;    FROM accounts_type_functions AS atf&#xA;    WHERE 1=1&#xA;    AND atf.Code = A.Code&#xA;    AND atf.Type = 1&#xA;)&#xA;&#xA;exec prc_set_sysvars 'accounts_type_functions'&#xA;/*&#xA;accounts_type_functions&#xA;+-------------+------+-----------+----------+&#xA;|  fieldname  | ispk | fieldtype | fieldlen |&#xA;+-------------+------+-----------+----------+&#xA;| Code        | Y    | varchar   |       12 |&#xA;| Description |      | nvarchar  |       50 |&#xA;| Orden       |      | int       |        4 |&#xA;| Type        |      | char      |        1 |&#xA;+-------------+------+-----------+----------+&#xA;&#xA;+-------+&#xA;| PARVW |&#xA;+-------+&#xA;| AG    |   SOLICITANTE DE PEDIDO&#xA;| RE    |   Destenitario factura&#xA;| RG    |   Responsable pago&#xA;| TR    |   Transportista&#xA;| WE    |   Destinatario de mercancia &#xA;+-------+&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
