          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 200_contacts_array_1.sql 2.0.0&#xA;-- DEPARTAMENTOS PERSONAS DE CONTACTO&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE contacts_array&#xA;&#xA;DELETE FROM contacts_array WHERE Type='1'&#xA;SELECT * FROM contacts_array WHERE Type='1'&#xA;SELECT * FROM contacts_array_tr WHERE Type='1'&#xA;*/&#xA;&#xA;PRINT 'UPDATE contacts_array 1'&#xA;UPDATE contacts_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND contacts_array.Code = A.Code&#xA;AND contacts_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;&#xA;PRINT 'INSERT contacts_array 1'&#xA;INSERT INTO contacts_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM contacts_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE contacts_array 1'&#xA;UPDATE contacts_array&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND contacts_array.Code = A.Code&#xA;AND contacts_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE contacts_array_tr 1'&#xA;UPDATE contacts_array_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TSABT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND contacts_array_tr.Code = A.Code&#xA;AND contacts_array_tr.Type = A.Type&#xA;AND contacts_array_tr.Language_Tr = A.l&#xA;&#xA;&#xA;PRINT 'INSERT contacts_array_tr 1'&#xA;INSERT INTO contacts_array_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TSABT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM contacts_array_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE contacts_array_tr 1'&#xA;UPDATE contacts_array_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ABTNR AS Code,'1' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,1 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TSABT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TSABT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TSABT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND contacts_array_tr.Code = A.Code&#xA;AND contacts_array_tr.Type = A.Type&#xA;AND contacts_array_tr.Language_Tr = A.l&#xA;&#xA;/*&#xA;ERP_TSABT: DEPARTAMENTOS PERSON