RE 1=1&#xA;        --AND LAND1='S'&#xA;    ) AS erp&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,ZLSCH AS Code&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;            --,s.SPART --sector se quita&#xA;            ,souo.BUKRS AS Code_Unit_Org&#xA;            --,u.ZCEMPLE AS Code_User&#xA;            --,s.PERNR AS Code_Seller&#xA;            FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;            INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;            ON u.VKORG = s.VKORG --code_unit_org&#xA;            AND u.VTWEG = s.VTWEG --canal dist&#xA;            AND u.PERNR = s.PERNR  --code_seller&#xA;            INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;            ON s.VKORG = souo.VKORG&#xA;            AND souo.SPRAS = 'S'&#xA;&#xA;            UNION&#xA;&#xA;            SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;            FROM accounts_organization&#xA;            WHERE 1=1&#xA;            AND Delete_Date IS NULL&#xA;        ) AS orgs&#xA;        INNER JOIN ERP_FVPSOC  -- VIAS DE PAGO POR ORGANIZACION&#xA;        ON orgs.Code_Unit_Org = ERP_FVPSOC.BUKRS&#xA;    ) AS orgs&#xA;    ON orgs.Code = erp.Code&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_payment_ways.Code = A.Code&#xA;AND prj_payment_ways.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_payment_ways.Code_Sales_Org = A.Code_Sales_Org&#xA;AND A.l='S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE prj_payment_ways_tr'&#xA;UPDATE prj_payment_ways_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZLSCH AS Code&#xA;    ,orgs.Code_Unit_Org&#xA;    ,orgs.Code_Sales_Org&#xA;    ,UPPER(LTRIM(RTRIM(TEXT1))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T042Z.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T042Z&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.Language_Tr = ERP_T042Z.LAND1&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,ZLSCH AS Code&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;            --,s.SPART --sector se quita&#xA;            ,souo.BUKRS AS Code_Unit_Org&#xA;            --,u.ZCEMPLE AS Code_User&#xA;            --,s.PERNR AS Code_Seller&#xA;            FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;            INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;            ON u.VKORG = s.VKORG --code_unit_org&#xA;            AND u.VTWEG = s.VTWEG --canal dist&#xA;            AND u.PERNR = s.PERNR  --code_seller&#xA;            INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;            ON s.VKORG = souo.VKORG&#xA;            AND souo.SPRAS = 'S'&#xA;&#xA;            UNION&#xA;&#xA;            SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;            FROM accounts_organization&#xA;            WHERE 1=1&#xA;            AND Delete_Date IS NULL&#xA;        ) AS orgs&#xA;        INNER JOIN ERP_FVPSOC  -- VIAS DE PAGO POR ORGANIZACION&#xA;        ON orgs.Code_Unit_Org = ERP_FVPSOC.BUKRS&#xA;    ) AS orgs&#xA;    ON ERP_T042Z.ZLSCH = orgs.Code&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_payment_ways_tr.Code = A.Code&#xA;AND prj_payment_ways_tr.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_payment_ways_tr.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_payment_ways_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT prj_payment_ways_tr'&#xA;INSERT INTO prj_payment_ways_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Code_Unit_Org,Code_Sales_Org,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,Code_Unit_Org,Code_Sales_Org,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT ZLSCH AS Code&#xA;    ,orgs.Code_Unit_Org&#xA;    ,orgs.Code_Sales_Org&#xA;    ,UPPER(LTRIM(RTRIM(TEXT1))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_T042Z.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_T042Z&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.Language_Tr = ERP_T042Z.LAND1&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,ZLSCH AS Code&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;            --,s.SPART --sector se quita&#xA;            ,souo.BUKRS AS Code_Unit_Org&#xA;            --,u.ZCEMPLE AS Code_User&#xA;            --,s.PERNR AS Code_Seller&#xA;            FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;            INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;            ON u.VKORG = s.VKORG --code_unit_org&#xA;            AND u.VTWEG = s.VTWEG --canal dist&#xA;            AND u.PERNR = s.PERNR  --code_seller&#xA;            INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;            ON s.VKORG = souo.VKORG&#xA;            AND souo.SPRAS = 'S'&#xA;&#xA;            UNION&#xA;&#xA;            SEL