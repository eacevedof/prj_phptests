          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 801_prj_fgasol.sql 2.0.0&#xA;-- KILOMETRAJE - TIPOS DE GASOLINA&#xA;&#xA;/*&#xA;DELETE FROM prj_fgasol &#xA;SELECT * FROM prj_fgasol &#xA;*/&#xA;PRINT 'UPDATE prj_fgasol'&#xA;UPDATE prj_fgasol&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;--Code_Unit_Org = A.Code_Unit_Org,&#xA;--Code_Sales_Org = A.Code_Sales_Org,&#xA;--ZCGASOL = A.ZCGASOL,&#xA;ZNGASOL = A.ZNGASOL&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,ZCGASOL&#xA;    ,UPPER(LTRIM(RTRIM(ZNGASOL))) AS ZNGASOL&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FGASOL&#xA;    CROSS JOIN&#xA;    (&#xA;        -- según la carga de erp_imp_sales_organization&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR --AS Code_Seller&#xA;        --,'' AS status&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) orgs&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_fgasol.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_fgasol.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_fgasol.ZCGASOL = A.ZCGASOL&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_fgasol'&#xA;INSERT INTO prj_fgasol (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code_Unit_Org, Code_Sales_Org,ZCGASOL,ZNGASOL)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code_Unit_Org, Code_Sales_Org,ZCGASOL,ZNGASOL&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,ZCGASOL&#xA;    ,UPPER(LTRIM(RTRIM(ZNGASOL))) AS ZNGASOL&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FGASOL&#xA;    CROSS JOIN&#xA;    (&#xA;        -- según la carga de erp_imp_sales_organization&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR --AS Code_Seller&#xA;        --,'' AS status&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) orgs&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code_Unit_Org,Code_Sales_Org,ZCGASOL&#xA;    FROM prj_fgasol o&#xA;    WHERE 1=1&#xA;    AND o.Code_Unit_Org = A.Code_Unit_Org&#xA;    AND o.Code_Sales_Org = A.Code_Sales_Org&#xA;    AND o.ZCGASOL = A.ZCGASOL&#xA;)&#xA;&#xA;PRINT 'DELETE prj_fgasol'&#xA;UPDATE prj_fgasol&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,ZCGASOL&#xA;    ,UPPER(LTRIM(RTRIM(ZNGASOL))) AS ZNGASOL&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FGASOL&#xA;    CROSS JOIN&#xA;    (&#xA;        -- según la carga de erp_imp_sales_organization&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR --AS Code_Seller&#xA;        --,'' AS status&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;&#xA;        UNION&#xA;&#xA;        SELECT DISTINCT Code_Sales_Org,Code_Unit_Org&#xA;        FROM accounts_organization&#xA;        WHERE 1=1&#xA;        AND Delete_Date IS NULL&#xA;    ) orgs&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_fgasol.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_fgasol.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_fgasol.ZCGASOL = A.ZCGASOL&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;/*&#xA;PRINT 'UPDATE prj_fgasol_tr'&#xA;UPDATE prj_fgasol_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;ZNGASOL = A.ZNGASOL&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,ZCGASOL&#xA;    ,UPPER(LTRIM(RTRIM(ZNGASOL))) AS ZNGASOL&#xA;    ,'P' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_FGASOL&#xA;    CROSS JOIN&#xA;    (&#xA;        -- según la carga de erp_imp_sales_organization&#xA;        SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;        --,s.SPART --sector se quita&#xA;        ,souo.BUKRS AS Code_Unit_Org&#xA;        --,u.ZCEMPLE AS Code_User&#xA;        --,s.PERNR --AS Code_Seller&#xA;        --,'' AS status&#xA;        FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;        INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;        ON u.VKORG = s.VKORG --code_unit_org&#xA;        AND u.VTWEG = s.VTWEG --canal dist&#xA;        AND u.PERNR = s.PERNR  --code_seller&#xA;        INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;        ON s.VKORG = souo.VKORG&#xA;        AND souo.SPRAS = 'S'&#xA;    ) orgs&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_fgasol_tr.Code_Unit_Org = A.Code_Unit_Org&#xA;AND prj_fgasol_tr.Code_Sales_Org = A.Code_Sales_Org&#xA;AND prj_fgasol_tr.ZCGASOL = A.ZCGASOL&#xA;AND prj_fgasol_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT prj_fgasol_tr'&#xA;INSERT INTO prj_fgasol_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Code_Unit_Org, Code_Sales_Org&#xA;    ,ZCGASOL&#xA;    ,UPPER(LTRIM(RTRIM(ZNGASOL))) AS ZNGASOL&#xA;    ,'P' AS l&#xA;    ,IIF(ISNULL(ERP_FGASOL.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':','')