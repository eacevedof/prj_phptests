          SQLTask:SqlStatementSource="-- 104_accounts_array_600.sql &#xA;-- CANALES DE DISTRIBUCION&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE accounts_array&#xA;-- DELETE FROM accounts_array WHERE Type='600'&#xA;SELECT * FROM accounts_array WHERE Type='600'&#xA;SELECT * FROM accounts_array_tr WHERE Type='600'&#xA;*/&#xA;&#xA;PRINT 'UPDATE accounts_array 600'&#xA;UPDATE accounts_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT VTWEG AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,600 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TVTW.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVTW&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND accounts_array.Code = A.Code&#xA;AND accounts_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT accounts_array 600'&#xA;INSERT INTO accounts_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT VTWEG AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,600 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TVTW.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVTW&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM accounts_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE accounts_array 600'&#xA;UPDATE accounts_ar