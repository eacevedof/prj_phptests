          SQLTask:SqlStatementSource="-- 407_prj_des_array_512.sql 1.0.0&#xA;/*&#xA;-- TRUNCATE TABLE prj_des_array&#xA;DELETE FROM prj_des_array WHERE Type='512'&#xA;SELECT * FROM prj_des_array WHERE Type='512'&#xA;SELECT * FROM prj_des_array_tr WHERE Type='512'&#xA;&#xA;RE- Destinatario de Factura à prj_des_array para type 511&#xA;RG- Responsable de pago prj_des_array para type 512&#xA;WE- Destinatario de mercancia à prj_des_array para type 513&#xA;&#xA;No va por idioma&#xA;SELECT DISTINCT A.Create_User, A.Create_Date, A.Modify_User, A.Modify_Date, A.Platform, ERP_TPAER.KTOKD, '512' AS Type, ERP_TPAER.PARVW AS Description, '1' AS Orden, '0' AS Extra_Type, ERP_TPAER.PARVW&#xA;FROM ERP_TPAER &#xA;CROSS JOIN View_System_Fields_ERP AS A&#xA;WHERE (ERP_TPAER.PARVW = 'RG')&#xA;*/&#xA;&#xA;PRINT 'UPDATE prj_des_array 512'&#xA;UPDATE prj_des_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden,&#xA;Extra_Type = A.Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KTOKD AS Code,'512' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(PARVW))) AS Description&#xA;    ,1 AS Orden&#xA;    ,0 Extra_Type&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_TPAER.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TPAER&#xA;    WHERE 1=1&#xA;    AND PARVW = 'RG'&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_des_array.Code = A.Code&#xA;AND prj_des_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_des_array 512'&#xA;INSERT INTO prj_des_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden,Extra_Type)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden,Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KTOKD AS Code,'512' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(PARVW))) AS Description&#xA;    ,1 AS Orden&#xA;    ,0 Extra_Type&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_TPAER.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TPAER&#xA;    WHERE 1=1&#xA;    AND PARVW = 'RG'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM prj_des_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE prj_des_array 512'&#xA;UPDATE prj_des_array&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KTOKD AS Code,'512' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(PARVW))) AS Description&#xA;    ,1 AS Orden&#xA;    ,0 Extra_Type&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_TPAER.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TPAER&#xA;    WHERE 1=1&#xA;    AND PARVW = 'RG'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_des_array.Code = A.Code&#xA;AND prj_des_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;&#xA;/*&#xA;&#xA;TPAER&#x9;FUNCIONES DEL GRUPO DE CUENTAS&#xA;+-------+-------------------------+-------+------------+----------+-----------+&#xA;| campo |       descripcion       | clave | tipo_datos | longitud | decimales |&#xA;+-------+-------------------------+-------+------------+----------+-----------+&#xA;| KTOKD | Grupo de cuentas deudor |     1 | CHAR       |        4 |         0 |&#xA;| PARVW | Función de interlocutor |     2 | CHAR       |        2 |         0 |&#xA;+-------+-------------------------+-------+------------+----------+-----------+&#xA;&#xA;+------+---------------------------+---------+&#xA;| Code |        Description        | prjdesa |&#xA;+------+---------------------------+---------+&#xA;| RE   | DESTINARIO FACTURA        |   511   |&#xA;| RG   | RESPONSABLE PAGO          |   512   |&#xA;| WE   | DESTINATARIO DE MERCANCIA |   513   |&#xA;| AG   | SOLICITANTE DE PEDIDO     |   ???   |&#xA;+------+---------------------------+---------+&#xA;&#xA;&#xA;prj_des_array&#xA;+-------------+------+-----------+----------+&#xA;|  fieldname  | ispk | fieldtype | fieldlen |&#xA;+-------------+------+-----------+----------+&#xA;| Code        | Y    | varchar   |       12 |&#xA;| Type        | Y    | numeric   |        5 |&#xA;| Description |      | nvarchar  |       50 |&#xA;| Orden       |      | int       |        4 |&#xA;| Extra_Type  |      | varchar   |       12 |&#xA;+-------------+------+-----------+----------+&#xA;*/&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
