          SQLTask:SqlStatementSource="--800_prj_currency.sql 4.0.0&#xA;/*&#xA;-- TRUNCATE TABLE prj_currency --no va&#xA;-- DELETE FROM prj_currency&#xA;SELECT * FROM prj_currency&#xA;SELECT * FROM prj_currency_tr&#xA;*/&#xA;&#xA;-- INSERT INTO prj_currency (Code,Description) VALUES ('%','%')&#xA;PRINT 'UPDATE prj_currency'&#xA;UPDATE prj_currency&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_currency.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_currency'&#xA;INSERT INTO prj_currency (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Orden)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code&#xA;    FROM prj_currency o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code  &#xA;)&#xA;&#xA;PRINT 'DELETE prj_currency'&#xA;UPDATE prj_currency&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_currency.Code = A.Code&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE prj_currency_tr'&#xA;UPDATE prj_currency_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TCURT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_currency_tr.Code = A.Code&#xA;AND prj_currency_tr.Language_Tr = A.l&#xA;&#xA;PRINT 'INSERT prj_currency_tr'&#xA;INSERT INTO prj_currency_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TCURT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code&#xA;    FROM prj_currency_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE prj_currency_tr'&#xA;UPDATE prj_currency_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT WAERS AS Code&#xA;    ,UPPER(LTRIM(RTRIM(KTEXT))) AS Description&#xA;    ,900 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TCURT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TCURT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TCURT.SPRAS&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND prj_currency_tr.Code = A.Code&#xA;AND prj_currency_tr.Language_Tr = A.l&#xA;&#xA;/*&#xA;ERP_TCURT&#xA;+-------+--------------------------------------------+-------+------------+----------+&#xA;| campo |                descripcion                 | clave | tipo_datos | longitud |&#xA;+-------+--------------------------------------------+-------+------------+----------+&#xA;| SPRAS | Idioma de la descripción de la moneda      |     1 | CHAR       |        1 |&#xA;| WAERS | Código de moneda                           |     2 | CUKY       |        5 |&#xA;| KTEXT | Descripción de la moneda                   |       | CHAR       |       25 |&#xA;| AUT   | Indica si la moneda se puede utilizar o no |       | CHAR       |        1 |&#xA;+-------+--------------------------------------------+-------+------------+----------+&#xA;&#xA;SELECT * FROM ERP_TCURT&#xA;+-------+-------+-----------------+-----+&#xA;| SPRAS | WAERS |      KTEXT      | AUT |&#xA;+-------+-------+-----------------+-----+&#xA;| S     | ECS   | Sucre           | N   |&#xA;| S     | EEK   | Corona          | N   |&#xA;| S     | EGP   | Libra           | N   |&#xA;| S     | ESP   | Peseta          | N   |&#xA;| S     | ETB   | Birr            | N   |&#xA;| S     | EU    | EURO            | N   |&#xA;| S     | EUR   | EURO            | S   |&#xA;| S     | EUR3  | EURO 3 decimal. | N   |&#xA;....&#xA;&#xA;prj_currency&#xA;+-------------+------+-----------+----------+&#xA;|  fieldname  | ispk | fieldtype | fieldlen |&#xA;+-------------+------+-----------+----------+&#xA;| Code        | Y    | varchar   |       12 |&#xA;| Description |      | nvarchar  |       20 |&#xA;| Orden       |      | int       |        4 |&#xA;+-------------+------+-----------+----------+&#xA;&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
