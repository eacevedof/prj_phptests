          SQLTask:SqlStatementSource="-- 608_products_array_600.sql 1.0.0&#xA;-- UNIDAD DE VOLUMEN no hay tabla descriptora&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE products_array&#xA;&#xA;DELETE FROM products_array WHERE Type='600'&#xA;SELECT * FROM products_array WHERE Type='600'&#xA;SELECT * FROM products_array_tr WHERE Type='600'&#xA;*/&#xA;&#xA;PRINT 'UPDATE products_array 600'&#xA;UPDATE products_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT SUBSTRING(VOLEH,1,12) AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VOLEH))) AS Description&#xA;    ,600 AS Orden&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_MARA&#xA;    WHERE 1=1&#xA;    AND ISNULL(VOLEH,'')!=''&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND products_array.Code = A.Code&#xA;AND products_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT products_array 600'&#xA;INSERT INTO products_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden)&#xA;&#xA;SELECT DISTINCT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT SUBSTRING(VOLEH,1,12) AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VOLEH))) AS Description&#xA;    ,600 AS Orden&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_MARA&#xA;    WHERE 1=1&#xA;    AND ISNULL(VOLEH,'')!=''&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM products_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE products_array 600'&#xA;UPDATE products_array&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT SUBSTRING(VOLEH,1,12) AS Code,'600' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VOLEH))) AS Description&#xA;    ,600 AS Orden&#xA;    ,'S' AS l&#xA;    ,IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_MARA&#xA;    WHERE 1=1&#xA;    AND ISNULL(VOLEH,'')!=''&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND products_array.Code = A.Code&#xA;AND products_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'NO HAY TABLA DESCRIPTORA'&#xA;&#xA;/*&#xA;ERP_MARA   MATERIALES - DATOS GENERALES&#xA;+-------+---------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |           descripcion           | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+---------------------------------+-------+------------+----------+-----------+----------------+&#xA;| MATNR | Código del material             |     1 | CHAR       |       18 |         0 |                |&#xA;| MTART | Tipo de material                |       | CHAR       |        4 |         0 |                |&#xA;| PRDHA | Jerarquía del material          |       | CHAR       |       18 |         0 |                |&#xA;| MATKL | Grupo de artículos              |       | CHAR       |        9 |         0 |                |&#xA;| ERSDA | Fecha de creación               |       | DATS       |        8 |         0 |                |&#xA;| LAEDA | Fecha de la última modificación |    