          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 405_prj_des_array_510.sql 3.0.0&#xA;&#xA;/*&#xA;solo se cargan los validos&#xA;Hay que controlar los borrados. no activo -&gt; delete_date=fecha&#xA;&#xA;TIPOS DE POSICION SIN CARGO&#xA;SELECT * FROM ERP_TVAPT&#xA;&#xA;-- TRUNCATE TABLE prj_des_array&#xA;DELETE FROM prj_des_array WHERE Type='510'&#xA;SELECT * FROM prj_des_array WHERE Type='510'&#xA;SELECT * FROM prj_des_array_tr WHERE Type='510'&#xA;*/&#xA;&#xA;PRINT 'UPDATE prj_des_array 510'&#xA;UPDATE prj_des_array&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description,&#xA;Orden = A.Orden,&#xA;Extra_Type = A.Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,MTPOS AS Extra_Type&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    WHERE 1=1&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_des_array.Code = A.Code&#xA;AND prj_des_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;PRINT 'INSERT prj_des_array 510'&#xA;INSERT INTO prj_des_array (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Orden,Extra_Type)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,Orden,Extra_Type&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,MTPOS AS Extra_Type&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    WHERE 1=1&#xA;    AND VALID='S'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND A.l = 'S'&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM prj_des_array o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;)&#xA;&#xA;PRINT 'DELETE prj_des_array 510'&#xA;UPDATE prj_des_array&#xA;SET&#xA;Delete_User = 'DTSP',Delete_Date = A.Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,MTPOS AS Extra_Type&#xA;    ,SPRAS AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    WHERE 1=1&#xA;    AND VALID='S'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 9&#xA;AND prj_des_array.Code = A.Code&#xA;AND prj_des_array.Type = A.Type&#xA;AND A.l = 'S'&#xA;&#xA;&#xA;DECLARE @i INT=0&#xA;SET @i = (SELECT COUNT(*) FROM ERP_TVAPT WHERE 1=1 AND VALID='S' AND SPRAS='S')&#xA;-- si hay algo&#xA;IF(@i&gt;0)&#xA;BEGIN&#xA;    PRINT 'DELETE NO EXISTE prj_des_array 510'&#xA;    UPDATE prj_des_array&#xA;    SET&#xA;    Delete_User = 'DTSPNE'&#xA;    ,Delete_Date = REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','')&#xA;    WHERE 1=1&#xA;    AND Type = '510'&#xA;    AND NOT EXISTS &#xA;    (&#xA;        SELECT Code,Type&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;            ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;            FROM ERP_TVAPT&#xA;            WHERE 1=1&#xA;            AND VALID='S'&#xA;            AND SPRAS='S'&#xA;        ) A&#xA;        WHERE 1=1&#xA;        AND prj_des_array.Code = A.Code &#xA;        AND prj_des_array.Type = A.Type&#xA;    )&#xA;END&#xA;&#xA;-- =============================================================================&#xA;--                              TRADUCCIONES&#xA;-- =============================================================================&#xA;PRINT 'UPDATE prj_des_array_tr 510'&#xA;UPDATE prj_des_array_tr&#xA;SET&#xA;Delete_User = NULL,Delete_Date = NULL,&#xA;Modify_User = 'DTSP', Modify_Date = Transfer_Date,&#xA;Description = A.Description&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVAPT.SPRAS&#xA;    WHERE 1=1&#xA;    AND VALID='S'&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND prj_des_array_tr.Code = A.Code&#xA;AND prj_des_array_tr.Type = A.Type&#xA;AND prj_des_array_tr.Language_Tr = A.l&#xA;&#xA;&#xA;PRINT 'INSERT prj_des_array_tr 510'&#xA;INSERT INTO prj_des_array_tr (Create_User,Create_Date,Modify_User,Modify_Date&#xA;,Code,Type,Description,Language_Tr)&#xA;&#xA;SELECT 'DTSP' AS Create_User,Transfer_Date,'DTSP' AS Modify_User,Transfer_Date&#xA;,Code,Type,Description,l&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVAPT.SPRAS&#xA;    WHERE 1=1&#xA;    AND VALID='S'&#xA;) A&#xA;WHERE 1=1&#xA;AND A.Transfer_Status = 0&#xA;AND NOT EXISTS &#xA;(&#xA;    SELECT Code,Type&#xA;    FROM prj_des_array_tr o&#xA;    WHERE 1=1&#xA;    AND o.Code = A.Code&#xA;    AND o.Type = A.Type&#xA;    AND o.Language_Tr = A.l&#xA;)&#xA;&#xA;PRINT 'DELETE prj_des_array_tr 510'&#xA;UPDATE prj_des_array_tr&#xA;SET Delete_User = 'DTSP', Delete_Date = Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;    ,UPPER(LTRIM(RTRIM(VTEXT))) AS Description&#xA;    ,510 AS Orden&#xA;    ,Language_Tr AS l&#xA;    ,IIF(ISNULL(ERP_TVAPT.STATUS,'')='',0,9) AS Transfer_Status&#xA;    ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;    FROM ERP_TVAPT&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT ClientCode,Code AS Language_Tr&#xA;        FROM core_users_languages&#xA;        WHERE 1=1&#xA;        AND ClientCode IS NOT NULL&#xA;        AND Code!='ES'&#xA;    ) AS l&#xA;    ON l.ClientCode = ERP_TVAPT.SPRAS&#xA;    WHERE 1=1&#xA;    AND VALID='S'&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status = 9&#xA;AND prj_des_array_tr.Code = A.Code&#xA;AND prj_des_array_tr.Type = A.Type&#xA;AND prj_des_array_tr.Language_Tr = A.l&#xA;&#xA;SET @i = (SELECT COUNT(*) FROM ERP_TVAPT WHERE 1=1 AND VALID='S' AND SPRAS!='S')&#xA;-- si hay algo&#xA;IF(@i&gt;0)&#xA;BEGIN&#xA;    PRINT 'DELETE NO EXISTE prj_des_array_tr 510'&#xA;    UPDATE prj_des_array_tr&#xA;    SET&#xA;    Delete_User = 'DTSPNETR'&#xA;    ,Delete_Date = REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','')&#xA;    WHERE 1=1&#xA;    AND Type = '510'&#xA;    AND NOT EXISTS &#xA;    (&#xA;        SELECT Code,Type,Language_Tr&#xA;        FROM&#xA;        (&#xA;            SELECT DISTINCT PSTYV+MTPOS AS Code,'510' AS Type&#xA;            ,Language_Tr&#xA;            ,REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;            FROM ERP_TVAPT&#xA;            INNER JOIN &#xA;            (&#xA;                SELECT ClientCode,Code AS Language_Tr&#xA;                FROM core_users_languages&#xA;                WHERE 1=1&#xA;                AND ClientCode IS NOT NULL&#xA;                AND Code!='ES'&#xA;            ) AS l&#xA;            ON ERP_TVAPT.SPRAS = l.ClientCode&#xA;            WHERE 1=1&#xA;            AND VALID='S'&#xA;        ) A&#xA;        WHERE 1=1&#xA;        AND prj_des_array_tr.Code = A.Code &#xA;        AND prj_des_array_tr.Type = A.Type&#xA;        AND prj_des_array_tr.Language_Tr = A.Language_Tr&#xA;    )&#xA;END&#xA;/*&#xA;ERP_TVAPT - TIPOS DE POSICION SIN CARGO&#xA;+-------+------------------------------------------------------------------+-------+------------+----------+-----------+&#xA;| campo |                           descripcion                            | clave | tipo_datos | longitud | decimales |&#xA;+-------+------------------------------------------------------------------+-------+------------+----------+-----------+&#xA;| SPRAS | Idioma de la descripción                                         |     1 | CHAR       |        1 |         0 |&#xA;| PSTYV | Tipo de posición                                                 |     2 | CHAR       |        4 |         0 |&#xA;| MTPOS | Grupo de tipos de posición del maestro de material               |     3 | CHAR       |        4 |         0 |&#xA;| VTEXT | Descripción                                                      |       | CHAR       |       20 |         0 |&#xA;| VALID | Indica si es un tipo de posición sin cargo válido en el terminal |       | CHAR       |        1 |         0 |&#xA;+-------+------------------------------------------------------------------+-------+------------+----------+-----------+&#xA;&#xA;prj_des_array&#xA;+-------------+------+-----------+----------+&#xA;|  fieldname  | ispk | fieldtype | fieldlen |&#xA;+-------------+------+-----------+----------+&#xA;| Code        | Y    | varchar   |       12 |&#xA;| Type        | Y    | numeric   |        5 |&#xA;| Description |      | nvarchar  |       50 |&#xA;| Orden       |      | int       |        4 |&#xA;| Extra_Type  |      | varchar   |       12 |&#xA;+-------------+------+-----------+----------+&#xA;*/&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
