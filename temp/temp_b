                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- CONFIG
    SELECT * 
    FROM
    (
        SELECT SUBSTRING(linea,1,1) AS Status,
LTRIM(RTRIM(SUBSTRING(linea,2,5))) AS ZTERMIN,
LTRIM(RTRIM(SUBSTRING(linea,7,7))) AS ZCONTPED,
LTRIM(RTRIM(SUBSTRING(linea,14,3))) AS ZCONTCLI,
LTRIM(RTRIM(SUBSTRING(linea,17,8))) AS ZULTDIAT,
LTRIM(RTRIM(SUBSTRING(linea,25,3))) AS CODPAI,
LTRIM(RTRIM(SUBSTRING(linea,28,1))) AS CODEST,
LTRIM(RTRIM(SUBSTRING(linea,29,1))) AS ZTIPOPRI,
LTRIM(RTRIM(SUBSTRING(linea,30,1))) AS SPRAS
--,id
FROM ERP_auxiliar 
        ) AS t
        WHERE 1=1
        --AND</property>
