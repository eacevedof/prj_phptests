          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 275_erp_imp_collections_pending 4.0.0&#xA;/*&#xA;!!! falta trata deletes y status hay q preguntar sobre&#xA;las claves varia para vencidos y pagados&#xA;-- TRUNCATE TABLE ERP_IMP_collections_pending&#xA;SELECT * FROM ERP_IMP_collections_pending ORDER BY id DESC&#xA;UPDATE ERP_IMP_collections_pending SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_collections_pending insert' &#xA;&#xA;PRINT 'INSERT de ERP_FCLIVP'&#xA;INSERT INTO ERP_IMP_collections_pending &#xA;(Code_Unit_Org,Code_Sales_Org,Account_ID,Seller_ID,Code_Type,Total_Amount,Total_Pending,&#xA;Code_Payment_Term,Total_Pending_30,Total_Pending_60,Total_Pending_90,Total_Pending_up,&#xA;Total_Pending_Risk,Unpaid,Doc_Type,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;-- ERP_FCLIVP  CLIENTES - VENCIMIENTOS PENDIENTES&#xA;SELECT DISTINCT &#xA;-- CONVERT(NUMERIC(18,0),ERP_Automático.Automático) AS Num_Invoice,&#xA;--CONVERT(NUMERIC(18,0),ERP_Automático.Automático) AS NumDocum,&#xA;CONVERT(VARCHAR(12),ERP_FCLIVP.Code_Unit_Org) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),ERP_FCLIVP.Code_Sales_Org) AS Code_Sales_Org,&#xA;CONVERT(VARCHAR(24),ERP_FCLIVP.KUNNR) AS Account_ID,&#xA;CONVERT(VARCHAR(24),ERP_FCLIVP.Code_ofClientSeller) AS Seller_ID,&#xA;CONVERT(VARCHAR(12),ERP_FCLIVP.TIPOMV) AS Code_Type,&#xA;CONVERT(NUMERIC(11,3),ERP_FCLIVP.IVEN30D+IVEN60D+IVEN90D+IVENMAS+IVENRIE) AS Total_Amount,&#xA;CONVERT(NUMERIC(11,3),ERP_FCLIVP.IVEN30D+IVEN60D+IVEN90D+IVENMAS+IVENRIE) AS Total_Pending,&#xA;CONVERT(VARCHAR(12),ERP_FCLIVP.ZLSCH) AS Code_Payment_Term,&#xA;ERP_FCLIVP.IVEN30D AS Total_Pending_30,&#xA;ERP_FCLIVP.IVEN60D AS Total_Pending_60,&#xA;ERP_FCLIVP.IVEN90D AS Total_Pending_90,&#xA;ERP_FCLIVP.IVENMAS AS Total_Pending_up,&#xA;ERP_FCLIVP.IVENRIE AS Total_Pending_Risk,&#xA;CONVERT(VARCHAR(1),0) AS Unpaid,&#xA;CONVERT(VARCHAR(12),'VENCI') AS Doc_Type,&#xA;'0' AS  Source_Create,0 AS Transfer_status,IIF(ISNULL(ERP_FCLIVP.STATUS,'')='',0,9)  AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM &#xA;(&#xA;    --!!! no hay vkorg y vtweg&#xA;    SELECT STATUS,KUNNR,TIPOMV,ZLSCH,BUKRS,&#xA;    -- IIF(VKORG='',NULL,VKORG) AS VKORG,IIF(VTWEG='',NULL,VTWEG) AS VTWEG,&#xA;    ERP_ACCORG.Code_Unit_Org,ERP_ACCORG.Code_Sales_Org,ERP_ACCORG.Code_ofClientSeller,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IVEN30D,'.',''),',','.')) AS IVEN30D,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IVEN60D,'.',''),',','.')) AS IVEN60D,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IVEN90D,'.',''),',','.')) AS IVEN90D,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IVENMAS,'.',''),',','.')) AS IVENMAS,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IVENRIE,'.',''),',','.')) AS IVENRIE&#xA;    FROM ERP_FCLIVP&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,Code_ofClientAccount,Code_ofClientSeller&#xA;        FROM view_help_erpaccorg&#xA;    ) AS ERP_ACCORG&#xA;    ON ERP_FCLIVP.KUNNR = ERP_ACCORG.Code_ofClientAccount&#xA;    AND ERP_FCLIVP.BUKRS = ERP_ACCORG.Code_Unit_Org&#xA;) AS ERP_FCLIVP&#xA;WHERE 1=1&#xA;&#xA;PRINT 'INSERT de ERP_FCLIPV'&#xA;INSERT INTO ERP_IMP_collections_pending &#xA;(Date_Doc,Num_Doc_ID,Code_Unit_Org,Code_Sales_Org,Account_ID,Seller_ID,Code_Type,Total_Amount,&#xA;Total_Pending,Unpaid,Doc_Type,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;-- ERP_FCLIPV  CLIENTES - PAGOS VENCIDOS&#xA;SELECT DISTINCT &#xA;--CONVERT(NUMERIC(18,0),ERP_Automático.Automático) AS Num_Invoice,&#xA;--CONVERT(NUMERIC(18,0),ERP_Automático.Automático) AS NumDocum,&#xA;CONVERT(VARCHAR(8),ERP_FCLIPV.FECDOC) AS Date_Doc,&#xA;CONVERT(VARCHAR(24),ERP_FCLIPV.BELNR) AS Num_Doc_ID,&#xA;CONVERT(VARCHAR(12),ERP_FCLIPV.Code_Unit_Org) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),ERP_FCLIPV.Code_Sales_Org) AS Code_Sales_Org,&#xA;CONVERT(VARCHAR(24),ERP_FCLIPV.KUNNR) AS Account_ID,&#xA;CONVERT(VARCHAR(24),ERP_FCLIPV.Code_ofClientSeller) AS Seller_ID,&#xA;CONVERT(VARCHAR(12),ERP_FCLIPV.TIPOMV) AS Code_Type,&#xA;CONVERT(NUMERIC(11,3),ERP_FCLIPV.IMPNO) AS Total_Amount,&#xA;CONVERT(NUMERIC(11,3),ERP_FCLIPV.IMPNO) AS Total_Pending,&#xA;CONVERT(VARCHAR(1),0) AS Unpaid,&#xA;CONVERT(VARCHAR(12),'PAGOS') AS Doc_Type,&#xA;'0' AS  Source_Create,0 AS Transfer_status,IIF(ISNULL(ERP_FCLIPV.STATUS,'')='',0,9)  AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM &#xA;(&#xA;    --!!! no hay vkorg y vtweg&#xA;    SELECT STATUS,KUNNR,TIPOMV,BELNR,FECDOC,BUKRS,&#xA;    --IIF(VKORG='',NULL,VKORG) AS VKORG,IIF(VTWEG='',NULL,VTWEG) AS VTWEG,&#xA;    ERP_ACCORG.Code_Unit_Org,ERP_ACCORG.Code_Sales_Org,ERP_ACCORG.Code_ofClientSeller,&#xA;    CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IMPNO,'.',''),',','.')) AS IMPNO&#xA;    FROM ERP_FCLIPV&#xA;    INNER JOIN &#xA;    (&#xA;        SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,Code_ofClientAccount,Code_ofClientSeller&#xA;        FROM view_help_erpaccorg&#xA;    ) AS ERP_ACCORG&#xA;    ON ERP_FCLIPV.KUNNR = ERP_ACCORG.Code_ofClientAccount&#xA;    AND ERP_FCLIPV.BUKRS = ERP_ACCORG.Code_Unit_Org    &#xA;) AS ERP_FCLIPV&#xA;WHERE 1=1&#xA;&#xA;/*&#xA;ERP_FCLIVP  CLIENTES - VENCIMIENTOS PENDIENTES&#xA;+---------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;|  campo  |              descripcion              | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+---------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| BUKRS   | Código de la sociedad                 |     1 | CHAR       |        4 |         0 |                |&#xA;| KUNNR   | Código del cliente                    |     2 | CHAR       |       10 |         0 | KNA1           |&#xA;| TIPOMV  | Tipo de movimiento                    |     3 | CHAR       |        1 |         0 |                |&#xA;| VKORG   | Organización de ventas                |     4 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG   | Canal de distribución                 |     5 | CHAR       |        2 |         0 | TVTW           |&#xA;| ZLSCH   | Vía de pago                           |     6 | CHAR       |        1 |         0 | T042Z          |&#xA;| IVEN30D | Importe vencimientos a 30 días        |       | DEC        |       14 |         2 |                |&#xA;| IVEN60D | Importe vencimientos a 60 días        |       | DEC        |       14 |         2 |                |&#xA;| IVEN90D | Importe vencimientos a 90 días        |       | DEC        |       14 |         2 |                |&#xA;| IVENMAS | Importe vencimientos a más de 90 días |       | DEC        |       14 |         2 |                |&#xA;| IVENRIE | Importe vencimientos en riesgo        |       | DEC        |       14 |         2 |                |&#xA;+---------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_FCLIPV  CLIENTES - PAGOS VENCIDOS&#xA;+--------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo  |          descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+--------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| BUKRS  | Código de la sociedad         |     1 | CHAR       |        4 |         0 |                |&#xA;| KUNNR  | Código del cliente            |     2 | CHAR       |       10 |         0 | KNA1           |&#xA;| TIPOMV | Tipo de movimiento            |     3 | CHAR       |        1 |         0 |                |&#xA;| VKORG  | Organización de ventas        |     4 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG  | Canal de distribución         |     5 | CHAR       |        2 |         0 | TVTW           |&#xA;| BELNR  | Número del documento contable |     6 | CHAR       |       10 |         0 |                |&#xA;| FECDOC | Fecha del documentos          |     7 | DATS       |        8 |         0 |                |&#xA;| IMPNO  | Importe del documento         |       | CURR       |       15 |         2 |                |&#xA;+--------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_IMP_collections_pending&#xA;+----------------------+------+-----------+----------+&#xA;|      fieldname       | ispk | fieldtype | fieldlen |&#xA;+----------------------+------+-----------+----------+&#xA;| ID                   |      | int       |        4 |&#xA;| Account_ID           |      | varchar   |       24 |&#xA;| Amount_Confirmed     |      | numeric   |        9 |&#xA;| Assignation          |      | varchar   |       24 |&#xA;| Code_Account         |      | numeric   |        9 |&#xA;| Code_ClassificationA |      | varchar   |       12 |&#xA;| Code_Cost_Center     |      | varchar   |       12 |&#xA;| Code_Currency        |      | varchar   |       12 |&#xA;| Code_Payment_Term    |      | varchar   |       12 |&#xA;| Code_Sales_Org       |      | varchar   |       12 |&#xA;| Code_Type            |      | varchar   |       12 |&#xA;| Code_Unit_Org        |      | varchar   |       12 |&#xA;| Con_Date             |      | varchar   |        8 |&#xA;| Date_Doc             |      | varchar   |        8 |&#xA;| Date_Early           |      | varchar   |        8 |&#xA;| Date_End             |      | varchar   |        8 |&#xA;| Doc_Type             |      | varchar   |       12 |&#xA;| Ind_DH               |      | varchar   |        1 |&#xA;| Ind_Oper             |      | varchar   |        1 |&#xA;| Locked               |      | varchar   |        1 |&#xA;| Num_Doc_ID           |      | varchar   |       24 |&#xA;| Num_Inv_ID           |      | varchar   |       24 |&#xA;| Num_Invoice          |      | numeric   |        9 |&#xA;| Num_Pos              |      |