      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 110_erp_imp_sellers_users 3.2.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_sellers&#xA;TRUNCATE TABLE ERP_IMP_core_users&#xA;SELECT * FROM ERP_IMP_sellers ORDER BY id DESC&#xA;SELECT * FROM ERP_IMP_core_users ORDER BY id DESC&#xA;UPDATE ERP_IMP_sellers SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_sellers insert:'&#xA;INSERT INTO ERP_IMP_sellers &#xA;(Code, Code_ofClient, Code_Type, Description, First_Name, Last_Name&#xA;,Source_Create, Transfer_Status, Status_SAP, Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(INT,PERNR) AS Code, &#xA;PERNR AS Code_ofClient, &#xA;NULL AS Code_Type, &#xA;NAME1+' ('+CONVERT(VARCHAR,CONVERT(INT,PERNR))+')' AS Description, &#xA;NAME1 AS First_Name, &#xA;NULL AS Last_Name, &#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_FAGENT.STATUS,'') = '', 0,IIF(ERP_FAGENT.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FAGENT&#xA;&#xA;UNION&#xA;&#xA;SELECT DISTINCT CONVERT(INT,PERNR) AS Code, &#xA;PERNR AS Code_ofClient, &#xA;NULL AS Code_Type, &#xA;'DESC. '+' ('+PERNR+')' AS Description,&#xA;'NAME1. '+PERNR AS First_Name, &#xA;'LAST1. '+PERNR AS Last_Name, &#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_KNVPBIS.STATUS,'') = '', 0,IIF(ERP_FAGENT.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_KNVPBIS&#xA;WHERE 1=1&#xA;AND PERNR NOT IN&#xA;(&#xA;    SELECT PERNR&#xA;    FROM ERP_FAGENT&#xA;&#xA;    UNION&#xA;    &#xA;    -- parche que evita que se cambie con una descripcion un nombre bien formado que ya existe&#xA;    -- y que no nos vuelven a enviar por ser una carga de mantenimiento&#xA;    SELECT Code_ofClient&#xA;    FROM sellers&#xA;)&#xA;&#xA;-- =====================================================================&#xA;--                      ERP_IMP_core_users&#xA;-- =====================================================================&#xA;PRINT 'ERP_IMP_core_users insert:'&#xA;INSERT INTO ERP_IMP_core_users &#xA;(Code, Description, Active, First_Name, Data_Box, &#xA;Source_Create, Transfer_Status, Status_SAP, Transfer_Date)&#xA;&#xA;SELECT DISTINCT ZCEMPLE AS Code, &#xA;ZNEMPLE AS Description,&#xA;'1' as Active,&#xA;ZNEMPLE AS First_Name,&#xA;ZCEMPLE AS Data_Box,&#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_FAGEEM.STATUS,'')='',0,IIF(ERP_FAGENT.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FAGEEM&#xA;&#xA;/*&#xA;ERP_FAGEEM  ENLACE ENTRE CODIGO AGENTE Y EMPLEADO&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;|  campo  |          descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| PERNR   | Código de agente              |     1 | NUMC       |        8 |         0 | FAGENT         |&#xA;| VKORG   | Organización de ventas        |     2 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG   | Canal de distribución         |     3 | CHAR       |        2 |         0 | TVTW           |&#xA;| ZCEMPLE | Código de empleado            |     4 | DEC        |        4 |         0 |                |&#xA;| ZNEMPLE | Nombre del empleado           |       | CHAR       |       45 |         0 |                |&#xA;| ZFECINI | Fecha de inicio               |       | DATS       |        8 |         0 |                |&#xA;| ZFECFIN | Fecha de fin                  |       | DATS       |        8 |         0 |                |&#xA;| LIFNR   | Código de acreedor del agente |       | CHAR       |       10 |         0 |                |&#xA;| SAKNR   | Cuenta de mayor               |       | CHAR       |       10 |         0 |                |&#xA;| KOSTL   | Centro de coste               |       | CHAR       |       10 |         0 |                |&#xA;| ZCGASOL | Tipo de gasolina por defecto  |       | CHAR       |        1 |         0 | FGASOL         |&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_FAGENT AGENTES&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |      descripcion       | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| PERNR | Código de agente       |     1 | NUMC       |        8 |         0 | FAGENT         |&#xA;| VKORG | Organización de ventas |       | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG | Canal de distribución  |       | CHAR       |        2 |         0 | TVTW           |&#xA;| SPART | Sector                 |       | CHAR       |        2 |         0 | TSPA           |&#xA;| NAME1 | Nombre del agente      |       | CHAR       |       35 |         0 |                |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_KNVPBIS  CLIENTES - AGENTES DE VENTAS&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |         descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;| KUNNR | Código del cliente           |     1 | CHAR       |       10 |         0 | KNA1           |&#xA;| VKORG | Organización de ventas       |     2 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG | Canal de distribución        |     3 | CHAR       |        2 |         0 | TVTW           |&#xA;| SPART | Sector                       |     4 | CHAR       |        2 |         0 | TSPA           |&#xA;| PARZA | Contador número interlocutor |     5 | NUMC       |        3 |         0 |                |&#xA;| PERNR | Código del agente            |       | NUMC       |        8 |         0 | FAGENT         |&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;*/&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
