          SQLTask:SqlStatementSource="--170_erp_imp_prj_fagedh 1.0.0&#xA;-- dias habiles por agente&#xA;&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_prj_fagedh&#xA;SELECT * FROM ERP_IMP_prj_fagedh ORDER BY id DESC&#xA;UPDATE ERP_IMP_prj_fagedh SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_prj_fagedh insert: '&#xA;INSERT INTO ERP_IMP_prj_fagedh&#xA;(Code_ofClientSeller,Code_Unit_Org,Code_Sales_Org,ZANYRT,ZRUTAV,&#xA;ZDIASHAB,&#xA;Source_Create, Transfer_Status, Status_SAP, Transfer_Date)&#xA;&#xA;SELECT DISTINCT ERP_FAGEDH.PERNR AS Code_ofClientSeller&#xA;,sel_orgs.Code_Unit_Org,sel_orgs.Code_Sales_Org&#xA;,ERP_FAGEDH.ZANYRT,ERP_FAGEDH.ZRUTAV&#xA;,ERP_FAGEDH.ZDIASHAB,&#xA;&#xA;'0' AS  Source_Create,IIF(ISNULL(ERP_FAGEDH.STATUS,'')='',0,IIF(ERP_FAGEDH.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FAGEDH&#xA;INNER JOIN&#xA;(&#xA;    -- según la carga de erp_imp_sales_organization&#xA;    SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;    --,s.SPART --sector se quita&#xA;    ,souo.BUKRS AS Code_Unit_Org&#xA;    --,u.ZCEMPLE AS Code_User&#xA;    ,s.PERNR --AS Code_Seller&#xA;    --,'' AS status&#xA;    FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;    INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;    ON u.VKORG = s.VKORG --code_unit_org&#xA;    AND u.VTWEG = s.VTWEG --canal dist&#xA;    AND u.PERNR = s.PERNR  --code_seller&#xA;    INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;    ON s.VKORG = souo.VKORG&#xA;    AND souo.SPRAS = 'S'&#xA;) AS sel_orgs&#xA;ON ERP_FAGEDH.PERNR = sel_orgs.PERNR&#xA;&#xA;/*&#xA;ERP_FAGEDH&#xA;+----------+-----------------------------------+-------+------------+----------+-----------+----------------+&#xA;|  campo   |            descripcion            | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+----------+------------------