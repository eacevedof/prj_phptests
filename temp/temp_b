        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 610_erp_imp_pricing_secuence.sql 2.0.0&#xA;/**&#xA;TRUNCATE TABLE ERP_IMP_pricing_secuence&#xA;UPDATE ERP_IMP_pricing_secuence SET Transfer_Status=100 &#xA;SELECT * FROM ERP_IMP_pricing_secuence&#xA;**/&#xA;PRINT 'ERP_IMP_pricing_secuence insert:'&#xA;INSERT INTO ERP_IMP_pricing_secuence &#xA;(Secuence,Description,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT KSCHL AS Secuence, ZTXTCON+' ('+KSCHL+')' AS Description,&#xA;'0' AS Source_Create,&#xA;IIF(ISNULL(ERP_CLASECON.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_CLASECON&#xA;WHERE 1=1&#xA;-- !!! parche hasta que haya bonificaciones&#xA;AND KSCHL NOT IN &#xA;(&#xA;    'ZBOC','ZBOM','ZPEC','ZPEM'&#xA;    ,'ZPUC'--&gt; esta se divide en 2&#xA;)&#xA;&#xA;UNION&#xA;&#xA;SELECT 'ZPUC1' AS Secuence, 'PRCIO ARTICULO 1 (ZPUC acc)' AS Description,&#xA;'0' AS Source_Create,&#xA; 0 AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;&#xA;UNION &#xA;&#xA;SELECT 'ZPUC2' AS Secuence, 'PRCIO ARTICULO 2 (ZPUC acchier)' AS Description,&#xA;'0' AS Source_Create,&#xA; 0 AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;&#xA;UNION&#xA;&#xA;SELECT 'MWST' AS Secuence, 'TASAS E IMPUESTOS (MWST)' AS Description,&#xA;'0' AS Source_Create,&#xA; 0 AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;&#xA;UNION&#xA;&#xA;SELECT 'ZREC' AS Secuence, 'RECARGO DE EQUIVALENCIA (ZREC)' AS Description,&#xA;'0' AS Source_Create,&#xA; 0 AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;&#xA;UNION&#xA;&#xA;SELECT 'ZZCANON' AS Secuence, 'CANON PRODUCTOS (ZZCANON)' AS Description,&#xA;'0' AS Source_Create,&#xA; 0 AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;&#xA;/*&#xA;+-----------+-----------+-------+------------+&#xA;| Prioridad | Secuencia | Tabla | Agrupación |&#xA;+-----------+-----------+-------+------------+&#xA;|         1 | ZPUC1     |       |          1 |&#xA;|         1 | ZDPC      |       |          2 |&#xA;|         1 | ZDPM      |       |          2 |&#xA;&#xA;|         2 | ZPUC2     |       |          1 |&#xA;|         2 | ZDPC      |       |          2 |&#xA;|         2 | ZDPM      |       |          2 |&#xA;|         2 | ZDCC      |       |          3 |&#xA;|         2 | ZDCM      |       |          3 |&#xA;|         2 | ZDIT      |       |          3 |&#xA;&#xA;|         3 | ZPUM      |       |          1 |&#xA;|         3 | ZDPC      |       |          2 |&#xA;|         3 | ZDPM      |       |          2 |&#xA;|         3 | ZDCC      |       |          3 |&#xA;|         3 | ZDCM      |       |          3 |&#xA;|         3 | ZDIT      |       |          3 |&#xA;|         3 | ZDAC      |       |          4 |&#xA;|         3 | ZDAM      |       |          4 |&#xA;+-----------+-----------+-------+------------+&#xA;&#xA;ZBOC-- NO HAY NADA EN BONIFICACIONES&#xA;ZBOM&#xA;    ZDAC&#xA;    ZDAM&#xA;    ZDCC&#xA;    ZDCM&#xA;    ZDIT&#xA;    ZDPC&#xA;    ZDPM&#xA;ZPEC -- PRECIO LOTE &#xA;ZPEM&#xA;    ZPUC ZPUC1 ZPUC2-- EN 2&#xA;    ZPUM&#xA;&#xA;CLASECON    CONDICIONES A UTILIZAR EN TERMINAL&#x9;&#xA;Se ha eliminado Organización/Canal porque es lo mismo para todas las áreas de ventas + &#xA;cambio orden campos&#xA;&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;|  campo  |                   descripcion                    | clave | tipo_datos | longitud |&#xA;+--