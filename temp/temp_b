    <DTS:Executable
      DTS:refId="Package\ERP_IMP_products_pack                   430"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{dda45844-2c14-4ccc-a019-a0aec4248e0c}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="ERP_IMP_products_pack                   430"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="2">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 430_erp_imp_products_pack 5.2.0&#xA;&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_products_pack&#xA;SELECT * FROM ERP_IMP_products_pack ORDER BY id DESC&#xA;UPDATE ERP_IMP_products_pack SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_products_pack insert: '&#xA;&#xA;INSERT INTO ERP_IMP_products_pack &#xA;(STLNR,Code_ofClient,Code_ofClient_Comp,Quantity,Unit_Type_Sales,WERKS,Type_Sale,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT A.STLNR,&#xA;CONVERT(VARCHAR(24),A.MATNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(24),A.IDNRK) AS Code_ofClient_Comp,&#xA;CONVERT(NUMERIC(11,3),A.MENGE) AS Quantity,&#xA;CONVERT(VARCHAR(4),A.MEINS) AS Unit_Type_Sales,&#xA;CONVERT(VARCHAR(4),A.WERKS) AS WERKS,&#xA;CONVERT(VARCHAR(1),A.SORTF) AS Type_Sale,&#xA;&#xA;'0' AS  Source_Create,&#xA;IIF(ISNULL(A.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM &#xA;(&#xA;    SELECT DISTINCT ERP_MAST.[Status], MATNR,IDNRK&#xA;    ,IIF(WERKS='',NULL,WERKS) AS WERKS -- centro de exp por defecto&#xA;    ,STLAN -- utilizacion en la lista de materiales&#xA;    ,ERP_MAST.STLNR -- codigo en la lista de materiales&#xA;    --,REPLACE(REPLACE(MENGE,'.',''),',','.') AS MENGE&#xA;    ,MENGE -- qty&#xA;    ,MEINS -- unidad de medida &#xA;    --,IIF(LTRIM(RTRIM(SORTF))='',0,1) AS SORTF -- Indicador de componente sin cargo &#xA;    ,IIF(LTRIM(RTRIM(SORTF))='',1,0) AS SORTF -- Indicador de componente sin cargo&#xA;     -- En productos pack el campo &quot;Sin cargo hay que calcularlos al revés. Si viene valor es un &quot;0&quot; sino &quot;1&quot;.&#xA;    FROM ERP_MAST -- Materiales cabecera&#xA;    INNER JOIN ERP_STPO -- Detalle de lista de materiales (componentes)&#xA;    ON ERP_MAST.STLNR = ERP_STPO.STLNR&#xA;    WHERE 1=1&#xA;    -- AND IDNRK='AL00654C'&#xA;    -- AND MATNR='AL000372A'&#xA;) AS A&#xA;&#xA;-- productos repetidos&#xA;DECLARE @oTableRep TABLE([idn][int]IDENTITY(1,1) NOT NULL,id INT)&#xA;&#xA;PRINT 'ids de duplicados'&#xA;INSERT INTO @oTableRep (id)&#xA;SELECT id &#xA;FROM ERP_IMP_products_pack&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0 &#xA;AND EXISTS &#xA;(&#xA;    -- repetidos&#xA;    SELECT *&#xA;    FROM&#xA;    (&#xA;        SELECT DISTINCT Code_ofClient,Code_ofClient_Comp,WERKS,Type_Sale&#xA;        FROM ERP_IMP_products_pack&#xA;        WHERE 1=1&#xA;        AND Transfer_Status = 0 &#xA;        AND ISNULL(Code_ofClient,'')!=''&#xA;        AND ISNULL(Code_ofClient_Comp,'')!=''&#xA;        AND ISNULL(WERKS,'')!=''&#xA;        GROUP BY Code_ofClient,Code_ofClient_Comp,WERKS,Type_Sale&#xA;        HAVING COUNT(id)&gt;1&#xA;    ) AS t&#xA;    WHERE 1=1&#xA;    AND t.Code_ofClient = ERP_IMP_products_pack.Code_ofClient&#xA;    AND t.Code_ofClient_Comp = ERP_IMP_products_pack.Code_ofClient_Comp&#xA;    AND t.WERKS = ERP_IMP_products_pack.WERKS&#xA;    AND t.Type_Sale = ERP_IMP_products_pack.Type_Sale&#xA;)&#xA;&#xA;-- unifico los duplicados sumando sus cantidades&#xA;PRINT 'packs duplicados unificados'&#xA;INSERT INTO ERP_IMP_products_pack &#xA;(STLNR,Code_ofClient,Code_ofClient_Comp,Quantity,Unit_Type_Sales&#xA;,WERKS,Type_Sale&#xA;,Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT MAX(STLNR) AS STLNR,Code_ofClient,Code_ofClient_Comp&#xA;,SUM(Quantity) AS Quantity,MAX(Unit_Type_Sales) AS Unit_Type_Sales&#xA;,WERKS, Type_Sale&#xA;,MAX(Source_Create) Source_Create,MAX(Transfer_status) Transfer_status&#xA;,MAX(Status_SAP) Status_SAP,MAX(Transfer_Date) Transfer_Date&#xA;FROM ERP_IMP_products_pack&#xA;WHERE 1=1&#xA;AND id IN&#xA;(&#xA;    SELECT id FROM @oTableRep&#xA;)&#xA;GROUP BY Code_ofClient,Code_ofClient_Comp,WERKS,Type_Sale&#xA;&#xA;PRINT 'se borra packs repetidos'&#xA;-- borro los repetidos&#xA;DELETE FROM ERP_IMP_products_pack WHERE id IN (SELECT id FROM @oTableRep)&#xA;&#xA;/*&#xA;-- HIJO: CL500BCH01&#xA;select * from ERP_IMP_products_pack where code_ofClient='000000000000084021'&#xA;&#xA;RP101&#xA;RP102&#xA;*&#xA;SELECT * FROM ERP_IMP_products_pack where Code_ofClient_Comp='000000000000084021'&#xA;&#xA;HIJOS 000000000000084021&#xA;RP10101&#xA;FALTARIA - CL500BCH01, se marca con type_sale=0 y en amount_per=000000000000084021&#xA;&#xA;SELECT * FROM ERP_IMP_products_pack where Code_ofClient='RP101'&#xA;&#xA;ERP_MAST  MATERIALES - CABECERA LISTA MATERIALES&#xA;+-------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |              descripcion              | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| MATNR | Código del material                   |     1 | CHAR       |       18 |         0 | MARA           |&#xA;| WERKS | Centro de expedición por defecto      |     2 | CHAR       |        4 |         0 | T001W          |&#xA;| STLAN | Utilización de la lista de materiales |     3 | CHAR       |        1 |         0 |                | no se importa, siempre viene un 5&#xA;| STLNR | Código de la lista de materiales      |     4 | CHAR       |        8 |         0 |                |&#xA;+-------+---------------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_STPO    MATERIALES - DETALE LLISTA MATERIALES&#xA;+-------+------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |                  descripcion                   | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| STLNR | Código de la lista de materiales               |     1 | CHAR       |        8 |         0 |                |&#xA;| STLKN | Número de nodo de la posición de la lista      |       | NUMC       |        8 |         0 |                |&#xA;| IDNRK | Componente de la lista                         |       | CHAR       |       18 |         0 | MARA           |&#xA;| MEINS | Unidad de medida de la lista                   |       | CHAR       |        3 |         0 |                |&#xA;| MENGE | Unidades del componente según unidad de medida |       | DEC        |       13 |         3 |                |&#xA;| SORTF | Indicador de componente sin cargo              |       | CHAR       |        1 |         0 |                |&#xA;+-------+------------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_IMP_products_pack&#xA;+--------------------+------+-----------+----------+&#xA;|     fieldname      | ispk | fieldtype | fieldlen |&#xA;+--------------------+------+-----------+----------+&#xA;| ID                 |      | int       |        4 |&#xA;| STLNR              |      | varchar   |        8 |&#xA;| Code_Product       |      | numeric   |        9 |&#xA;| Code_ofClient      |      | varchar   |       24 |&#xA;| Code_Product_Comp  |      | numeric   |        9 |&#xA;| Code_ofClient_Comp |      | varchar   |       24 |&#xA;| Quantity           |      | numeric   |     11,3 |&#xA;| Unit_Type_Sales    |      | varchar   |        4 |&#xA;| Type_Sale          |      | varchar   |        1 |&#xA;| Amount_per         |      | numeric   |      6,3 |&#xA;| WERKS              |      | varchar   |        4 |&#xA;+--------------------+------+-----------+----------+&#xA;&#xA;products_packs&#xA;+-----------------+------+-----------+----------+&#xA;|    fieldname    | ispk | fieldtype | fieldlen |&#xA;+-----------------+------+-----------+----------+&#xA;| Code            | Y    | numeric   |        9 |&#xA;| Code_Component  | Y    | numeric   |        9 |&#xA;| WERKS           | Y    | varchar   |        4 |&#xA;| Quantity        |      | numeric   |     11,3 |&#xA;| Unit_Type_Sales |      | varchar   |        4 |&#xA;| Type_Sale       |      | char      |        1 |&#xA;| Amount_per      |      | numeric   |      6,3 |&#xA;+-----------------+------+-----------+----------+&#xA;&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
