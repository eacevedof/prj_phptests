    SELECT * 
    FROM
    (
        SELECT SUBSTRING(linea,1,1) AS Status,
LTRIM(RTRIM(SUBSTRING(linea,2,11))) AS ZATRIB,
LTRIM(RTRIM(SUBSTRING(linea,13,7))) AS NUMORD
--,id
FROM ERP_auxiliar 
        ) AS t
        WHERE 1=1
        --AND</property>
