          SQLTask:SqlStatementSource="-- 260_erp_imp_accounts_tax 3.1.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_accounts_tax&#xA;SELECT * FROM ERP_IMP_accounts_tax ORDER BY id DESC&#xA;UPDATE ERP_IMP_accounts_tax SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;-- ojo con esto hay que darle una vuelta&#xA;PRINT 'ERP_IMP_accounts_tax insert:'&#xA;INSERT INTO ERP_IMP_accounts_tax &#xA;(Code_ofClient,Code_Country,Type,TaxClassification,Transfered,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_KNA1.KUNNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.LAND1) AS Code_Country,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.TATYP1) AS Type,&#xA;CONVERT(VARCHAR(24),ERP_KNA1.TAXKD1) AS TaxClassification,&#xA;'1' AS Transfered, &#xA;'0' AS  Source_Create,IIF(ISNULL(ERP_KNA1.STATUS,'')='',0,IIF(ERP_KNA1.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM &#xA;(&#xA;    SELECT STATUS,KUNNR, LAND1,TATYP1,TAXKD1+TATYP1 AS TAXKD1&#xA;    FROM ERP_KNA1 &#xA;    WHERE LAND1!='' AND TATYP1!='' AND TAXKD1!=''&#xA;&#xA;    UNION&#xA;&#xA;    SELECT STATUS,KUNNR, LAND1,TATYP2,TAXKD2+TATYP2 AS TAXKD2&#xA;    FROM ERP_KNA1 &#xA;    WHERE LAND1!='' AND TATYP2!='' AND TAXKD2!=''&#xA;) AS ERP_KNA1 --CLIENTES - DATOS GENERALES&#xA;WHERE 1=1&#xA;&#xA;/*&#xA;UPDATE       prj_accounts_tax&#xA;SET           TaxClassification = TaxClassification + Type&#xA;&#xA;select * from erp_FTIIVA --TIPOS DE IVA&#xA;select * from ERP_KNA1&#xA;select * from erp_TSKDT --IMPUESTOS&#xA;&#xA;- IVA -&gt; PRJ_DES_ARRAY con type SMWS&#xA;- Recargo Equivalencia -&gt;  PRJ_DES_ARRAY con type SZRE&#xA;&#xA;TATYP1: Condición comercial IVA&#xA;TATYP2: Condición comercial recargo equivalencia&#xA;&#xA;TAXKD1: Clasificación fiscal IVA&#xA;TAXKD2: Clasificación fiscal recargo equivalencia&#xA;&#xA;&#xA;SELECT KUNNR,LAND1,TATYP1,TATYP2,TAXKD1,TAXKD2 &#xA;FR