        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 240_erp_imp_accounts_organization 5.0.0&#xA;&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_accounts_organization&#xA;SELECT * FROM ERP_IMP_accounts_organization ORDER BY id DESC&#xA;UPDATE ERP_IMP_accounts_organization SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_accounts_organization insert' &#xA;INSERT INTO ERP_IMP_accounts_organization &#xA;(&#xA;Code_ofClientAccount,Code_Unit_Org,Code_Sales_Org,Code_Channel,Code_ofClientSeller,&#xA;Code_Hierarchy1,Code_ClassificationC,Code_ClassificationD,Via_Payment,Code_Payment,Code_Price_List,&#xA;Code_Currency,Sales_Office,Incoterms1,Incoterms2,Priority,Num_CopyFac,&#xA;Num_CopyAlb,Code_ofClientOld,Allow_Alcohol,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_KNVV.KUNNR) AS Code_ofClientAccount,&#xA;--CONVERT(VARCHAR(12),ERP_KNB1.BUKRS) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),SELLERORG.BUKRS) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.VKORG+ERP_KNVV.VTWEG) AS Code_Sales_Org,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.VTWEG) AS Code_Channel,&#xA;CONVERT(VARCHAR(24),ERP_KNVPBIS.PERNR) AS Code_ofClientSeller,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.KDGRP) AS Code_Hierarchy1,-- Grupo de clientes&#xA;CONVERT(VARCHAR(12),ERP_KNVV.VSBED) AS Code_ClassificationC,--Condición de expedición prj_dar(500)&#xA;CONVERT(VARCHAR(12),ERP_KNVV.SPART) AS Code_ClassificationD,--Sector prj_dar(501)&#xA;-- CONVERT(VARCHAR(12),ERP_KNB1.ZWELS) AS Via_Payment,&#xA;NULL AS Via_Payment,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.ZTERM) AS Code_Payment,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.KONDA) AS Code_Price_List,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.WAERS) AS Code_Currency,&#xA;CONVERT(VARCHAR(12),ERP_KNVV.BZIRK) AS Sales_Office,&#xA;CONVERT(VARCHAR(3),ERP_KNVV.INCO1) AS Incoterms1,&#xA;CONVERT(VARCHAR(30),ERP_KNVV.INCO2) AS Incoterms2,&#xA;CONVERT(VARCHAR(1),ERP_KNVV.LPRIO) AS Priority,&#xA;CONVERT(INT,ERP_KNVV.DOANZF) AS Num_CopyFac,&#xA;CONVERT(INT,ERP_KNVV.DOANZA) AS Num_CopyAlb, --ERROR de conversion alfanum a entero&#xA;-- CONVERT(VARCHAR(24),ERP_KNB1.ALTKN) AS Code_ofClientOld,&#xA;NULL AS Code_ofClientOld,&#xA;-- Cuando tiene ‘X’ es que el redondeo está desactivado, es decir, en estos casos no hay que redondear.&#xA;-- mail: fwd Campo Redondeo 19/02/2018&#xA;CONVERT(VARCHAR(1),IIF(ERP_KNVV.RDOFF='X',0,1)) AS Allow_Alcohol, -- 04/01/2018 añadido, flag se aplica redondeo(1) o no(0)&#xA;&#xA;'0' AS Source_Create, IIF(ISNULL(ERP_KNVV.STATUS,'')='',0,IIF(ERP_KNVV.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_KNVV -- relacion cliente/salesorg&#xA;INNER JOIN ERP_KNVPBIS --relacion cliente/salesorg/seller&#xA;ON ERP_KNVPBIS.KUNNR = ERP_KNVV.KUNNR  -- code_account&#xA;AND ERP_KNVPBIS.VKORG = ERP_KNVV.VKORG -- code_sales_org&#xA;AND ERP_KNVPBIS.VTWEG = ERP_KNVV.VTWEG  -- Canal de distribución &#xA;AND ERP_KNVPBIS.SPART = ERP_KNVV.SPART  -- Sector&#xA;INNER JOIN &#xA;(&#xA;    -- vendedores y todas sus organizaciones&#xA;    SELECT DISTINCT souo.BUKRS&#xA;    ,s.VKORG,s.VTWEG &#xA;    --,s.VKORG+s.VTWEG AS VKORGTWEG&#xA;    ,SPART&#xA;    ,s.PERNR &#xA;    FROM ERP_TVKO as souo&#xA;    INNER JOIN ERP_FAGENT AS s&#xA;    ON souo.VKORG = s.VKORG&#xA;    WHERE 1=1 &#xA;    AND SPRAS='S'&#xA;) AS SELLERORG --relacion seller/salesorg/sector&#xA;ON SELLERORG.VKORG = ERP_KNVV.VKORG -- code_sales_org&#xA;AND SELLERORG.VTWEG = ERP_KNVV.VTWEG  -- Canal de distribución &#xA;AND SELLERORG.SPART = ERP_KNVV.SPART  -- Sector&#xA;AND SELLERORG.PERNR = ERP_KNVPBIS.PERNR  -- Seller&#xA;WHERE 1=1&#xA;-- AND ERP_KNVV.KUNNR = '0000062002'&#xA;&#xA;UNION&#xA;&#xA;-- Los transportistas se tratan como cuentas&#xA;SELECT KUNN2 AS Code_ofClientAccount,BUKRS AS Code_Unit_Org&#xA;,VKORG+VTWEG AS Code_Sales_Org,VTWEG AS Code_Channel,PERNR AS Code_ofClientSeller&#xA;,NULL AS Code_Hierarchy1, NULL AS Code_ClassificationC,NULL AS Code_ClassificationD, NULL AS Via_Payment&#xA;,NULL AS Code_Payment, NULL AS Code_Price_List,NULL AS Code_Currency, NULL AS Sales_Office&#xA;,NULL AS Incoterms1, NULL AS Incoterms2,NULL AS Priority, NULL AS Num_CopyFac&#xA;,NULL AS Num_CopyAlb, NULL AS Code_ofClientOld, NULL AS Allow_Alcohol&#xA;,'0' AS Source_Create, IIF(ISNULL(Status,'')='',0,IIF(ERP_KNVV.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM&#xA;(&#xA;    -- transportistas&#xA;    SELECT DISTINCT ERP_KNVP.Status, ERP_KNVP.KUNNR,ERP_KNVP.VKORG,ERP_KNVP.VTWEG,ERP_KNVP.SPART&#xA;    ,KUNN2,PARVW,uos.PERNR,uos.BUKRS&#xA;    FROM ERP_KNVP --interlocutores&#xA;    INNER JOIN&#xA;    (&#xA;        SELECT DISTINCT ERP_KNVPBIS.KUNNR,ERP_KNVPBIS.VKORG&#xA;        ,ERP_KNVPBIS.VTWEG,ERP_KNVPBIS.SPART,ERP_KNVPBIS.PERNR,BUKRS&#xA;        FROM ERP_KNVPBIS --cliente/seller/salesorg&#xA;        INNER JOIN &#xA;        (&#xA;            SELECT DISTINCT KUNNR,BUKRS&#xA;            FROM ERP_KNB1 --cliente/uorg&#xA;            WHERE 1=1&#xA;        ) AS ERP_KNB1&#xA;        ON ERP_KNVPBIS.KUNNR = ERP_KNB1.KUNNR&#xA;        INNER JOIN &#xA;        (&#xA;            SELECT DISTINCT VKORG,VTWEG,PERNR,SPART&#xA;            FROM ERP_FAGENT&#xA;        ) AS ERP_FAGENT&#xA;        ON ERP_KNVPBIS.VKORG = ERP_FAGENT.VKORG&#xA;        AND ERP_KNVPBIS.VTWEG = ERP_FAGENT.VTWEG&#xA;        AND ERP_KNVPBIS.SPART = ERP_FAGENT.SPART&#xA;        AND ERP_KNVPBIS.PERNR = ERP_FAGENT.PERNR&#xA;    ) AS uos&#xA;    ON ERP_KNVP.KUNNR = uos.KUNNR&#xA;    AND ERP_KNVP.VKORG = uos.VKORG&#xA;    AND ERP_KNVP.VTWEG = uos.VTWEG&#xA;    AND ERP_KNVP.SPART = uos.SPART&#xA;    WHERE 1=1&#xA;    AND PARVW='TR'&#xA;) AS T&#xA;WHERE 1=1&#xA;&#xA;PRINT 'PARCHE: ERP_IMP_accounts_organization actualiza Codigo antiguo y Via de pago' &#xA;UPDATE ERP_IMP_accounts_organization&#xA;SET Via_Payment = ZWELS,&#xA;Code_ofClientOld = ALTKN&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT KUNNR,BUKRS,ALTKN,ZWELS&#xA;    FROM ERP_KNB1 --cliente/uorg&#xA;    WHERE 1=1&#xA;) AS ERP_KNB1&#xA;WHERE 1=1&#xA;AND Transfer_Status = 0&#xA;AND ERP_KNB1.KUNNR = ERP_IMP_accounts_organization.Code_ofClientAccount&#xA;AND ERP_KNB1.BUKRS = ERP_IMP_accounts_organization.Code_Unit_Org&#xA;&#xA;&#xA;PRINT 'PARCHE: ERP_IMP_accounts_organization delete duplicados' &#xA;UPDATE ERP_IMP_accounts_organization&#xA;SET Transfer_Status = 222&#xA;WHERE id in&#xA;(&#xA;    -- duplicados&#xA;    SELECT id&#xA;    FROM ERP_IMP_accounts_organization as imp&#xA;    WHERE 1=1&#xA;    AND Transfer_Status = 0&#xA;    AND EXISTS &#xA;    (&#xA;        SELECT *&#xA;        FROM&#xA;        (&#x9;&#xA;            -- DUPLICADOS&#xA;            SELECT count(id) as i,Code_Unit_Org,Code_ofClientAccount,Code_Sales_Org,Code_ofClientSeller&#xA;            FROM ERP_IMP_accounts_organization&#xA;            WHERE Transfer_Status = 0&#xA;            GROUP BY Code_ofClientAccount,Code_Unit_Org,Code_Sales_Org,Code_ofClientSeller&#xA;            HAVING COUNT(id)&gt;1&#xA;        ) a&#xA;        WHERE a.Code_ofClientAccount = imp.Code_ofClientAccount&#xA;        AND a.Code_Unit_Org = imp.Code_Unit_Org&#xA;        AND a.Code_Sales_Org = imp.Code_Sales_Org&#xA;        AND a.Code_ofClientSeller = imp.Code_ofClientSeller&#xA;        --and i=3&#xA;    )&#xA;)&#xA;&#xA;&#xA;/*&#xA;-- vendedores PERNR = vendedor&#xA;select * from ERP_FAGENT where PERNR='00000047'&#xA;-- usuarios - vendedores&#xA;select * from ERP_FAGEEM where PERNR='00000047'&#xA;-- cuentas - vendedores&#xA;select * from ERP_KNVPBIS where PERNR='00000047'&#xA;&#xA;ERP_KNVP CLIENTES - INTERLOCUTORES&#xA;&#xA;PARVW: WE=Destinatario mercancía, RE=Destinatario factura, RG=Responsable pago&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |         descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;| KUNNR | Código del cliente           |     1 | CHAR       |       10 |         0 | KNA1           |&#xA;| VKORG | Organización de ventas       |     2 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG | Canal de distribución        |     3 | CHAR       |        2 |         0 | TVTW           |&#xA;| SPART | Sector                       |     4 | CHAR       |        2 |         0 | TSPA           |&#xA;| PARVW | Función de interlocutor      |     5 | CHAR       |        2 |         0 |                |&#xA;| PARZA | Contador número interlocutor |     6 | NUMC       |        3 |         0 |                |&#xA;| KUNN2 | Código del interlocutor      |       | CHAR       |       10 |         0 | KNA1           |&#xA;| DEFPA | Interlocutor por defecto     |       | CHAR       |        1 |         0 |                |&#xA;+-------+------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_FTRANS  AGENCIAS DE TRANSPORTE&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| campo |       descripcion        | clave | tipo_datos | longitud |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| LIFNR | Código del transportista |     1 | CHAR       |       10 |&#xA;| NAME1 | Nombre del transportista |     2 | CHAR       |       35 |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;&#xA;ERP_TVKO&#x9;ZSDTV636&#x9;ORGANIZACIONES DE VENTAS&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;| campo |             descripcion              | clave | tipo_datos | longitud |&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;| SPRAS | Idioma de la descripción             |     1 | CHAR       |        1 |&#xA;| VKORG | Organización de ventas               |     2 | CHAR       |        4 |&#xA;| VTEXT | Descripción                          |       | CHAR       |       20 |&#xA;| BUKRS | Sociedad a la que pertenece          |       | CHAR       |        4 |&#xA;| MOREI | Variante de reglamentación de viajes |       | CHAR       |        2 |&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;&#xA;ERP_KNVPBIS CLIENTES - AGENTES DE VENTAS&#xA;+-------+------------------------------+-------+------------+----------+&#xA;| campo |         descripcion          | clave | tipo_datos | longitud |&#xA;+-------+------------------------------+-------+------------+----------+&#xA;| KUNNR | Código del cliente           |     1 | CHAR       |       10 |&#xA;| VKORG | Organización de ventas       |     2 | CHAR       |        4 |&#xA;| VTWEG | Canal de distribución        |     3 | CHAR       |        2 |&#xA;| SPART | Sector                       |     4 | CHAR       |        2 |&#xA;| PARZA | Contador número interlocutor |     5 | NUMC       |        3 |&#xA;| PERNR | Código del agente            |       | NUMC       |        8 |&#xA;+-------+------------------------------+-------+------------+----------+&#xA;&#xA;ERP_KNB1 CLIENTES - DATOS POR SOCIEDAD&#xA;+-------+----------------------------+-------+------------+----------+&#xA;| campo |        descripcion         | clave | tipo_datos | longitud |&#xA;+-------+----------------------------+-------+------------+----------+&#xA;| KUNNR | Código del cliente         |     1 | CHAR       |       10 |&#xA;| BUKRS | Código de la sociedad      |     2 | CHAR       |        4 |&#xA;| ZWELS | Vías de pago               |       | CHAR       |       10 |&#xA;| URLID | Acuerdo de vacaciones      |       | CHAR       |        4 |&#xA;| ALTKN | Código anterior de cliente |       | CHAR       |       10 |&#xA;+-------+----------------------------+-------+------------+----------+&#xA;&#xA;ERP_KNVV CLIENTES - DATOS POR ÁREA DE VENTAS&#xA;+--------+-------------------------------+-------+------------+----------+&#xA;| campo  |          descripcion          | clave | tipo_datos | longitud |&#xA;+--------+-------------------------------+-------+------------+----------+&#xA;| KUNNR  | Código del cliente            |     1 | CHAR       |       10 |&#xA;| VKORG  | Organización de ventas        |     2 | CHAR       |        4 |&#xA;| VTWEG  | Canal de distribución         |     3 | CHAR       |        2 |&#xA;| SPART  | Sector                        |     4 | CHAR       |        2 |&#xA;| ERDAT  | Fecha de creación             |       | DATS       |        8 |&#xA;| KDGRP  | Grupo de clientes             |       | CHAR       |        2 |&#xA;| KONDA  | Grupo de precios              |       | CHAR       |        2 |&#xA;| LPRIO  | Prioridad de entrega          |       | CHAR       |        2 |&#xA;| PLTYP  | Clasificación Demand Planning |       | CHAR       |        2 |&#xA;| VSBED  | Condición de expedición       |       | CHAR       |        2 |&#xA;| WAERS  | Moneda del cliente            |       | CUKY       |        5 |&#xA;| BOKRE  | Relevante para rappel         |       | CHAR       |        1 |&#xA;| INCO1  | Parte 1 Incoterm              |       | CHAR       |        3 |&#xA;| INCO2  | Parte 2 Incoterm              |       | CHAR       |       28 |&#xA;| ZTERM  | Condición de pago             |       | CHAR       |        4 |&#xA;| DOANZF | Número de copias para factura |       | DEC        |        3 |&#xA;| DOANZA | Número de copias para albarán |       | DEC        |        3 |&#xA;| LOEVM  | Marca para borrado            |       | CHAR       |        1 |&#xA;| BZIRK  | Zona de ventas                |       | CHAR       |        6 |&#xA;+--------+-------------------------------+-------+------------+----------+&#xA;&#xA;ERP_IMP_accounts_organization&#xA;+----------------------+------+-----------+----------+&#xA;|      fieldname       | ispk | fieldtype | fieldlen |&#xA;+----------------------+------+-----------+----------+&#xA;| ID              