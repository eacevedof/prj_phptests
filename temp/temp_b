          SQLTask:SqlStatementSource="-- 285_erp_imp_prj_accounts_incidence 1.0.1&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_prj_accounts_incidence&#xA;SELECT * FROM ERP_IMP_prj_accounts_incidence ORDER BY id DESC&#xA;UPDATE ERP_IMP_prj_accounts_incidence SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_prj_accounts_incidence insert:'&#xA;&#xA;INSERT INTO ERP_IMP_prj_accounts_incidence &#xA;(Code_ofClient,Code_Unit_Org,Code_Sales_Org&#xA;,BELNR,GJAHR,BUZEI,TIPINC,IMPNO&#xA;,Source_Create,Transfer_status,Status_SAP,Transfer_Date&#xA;)&#xA;&#xA;SELECT ERP_FCLIIN.KUNNR,uoso.Code_Unit_Org,uoso.Code_Unit_Org&#xA;,BELNR,GJAHR,BUZEI,'TIPO_INC' TIPINC,IMPNO,&#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_FCLIIN.STATUS,'')='',0,IIF(ERP_FCLIIN.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FCLIIN&#xA;INNER JOIN&#xA;(&#xA;&#x9;SELECT DISTINCT BUKRS AS Code_Unit_Org&#xA;&#x9;,ERP_TVKO.VKORG+ERP_FAGENT.VTWEG AS Code_Sales_Org&#xA;&#x9;FROM ERP_TVKO&#xA;&#x9;INNER JOIN &#xA;&#x9;(&#xA;&#x9;&#x9;-- !!! parche para que cargue los canales que usan los vendedores&#xA;&#x9;&#x9;SELECT DISTINCT VKORG,VTWEG&#xA;&#x9;&#x9;FROM ERP_FAGENT&#xA;&#x9;) AS ERP_FAGENT&#xA;&#x9;ON ERP_TVKO.VKORG = ERP_FAGENT.VKORG&#xA;&#x9;WHERE 1=1&#xA;&#x9;AND SPRAS='S'&#xA;) AS uoso&#xA;ON ERP_FCLIIN.BUKRS = uoso.Code_Unit_Org&#xA;&#xA;&#xA;&#xA;&#xA;/*&#xA;ERP_FCLIIN    CLIENTES - INCIDENCIAS COBRO&#xA;+-------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |          descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| BUKRS | Código de la sociedad         |     1 | CHAR       |        4 |         0 |                |&#xA;| KUNNR | Código del cliente            |  