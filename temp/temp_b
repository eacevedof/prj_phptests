        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 400_erp_imp_products 3.0.0&#xA;&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_products&#xA;SELECT * FROM ERP_IMP_products ORDER BY id DESC&#xA;UPDATE ERP_IMP_products SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_products clean cr lf: '&#xA;&#xA;-- UPDATE ERP_MARA SET VOLUM = '0' WHERE VOLUM='0,000'&#xA;-- UPDATE ERP_MARM SET UMREZ = '0' WHERE UMREZ='0,000'&#xA;-- UPDATE ERP_MARM SET BRGEW = '0' WHERE BRGEW='0,000'&#xA;-- UPDATE ERP_MARA SET NTGEW = '0' WHERE NTGEW='0,000'&#xA;&#xA;PRINT 'ERP_IMP_products insert: '&#xA;INSERT INTO ERP_IMP_products &#xA;(Code_ofClient,Code_ClassificationA,Code_ClassificationC,Factor_Conversion,Unit_Type_Sales,Date_Create,Date_Modify,Weight,Weight_Net&#xA;,Unit_Type_Weight,Volume,Description,&#xA;Unit_Type_Volumen,Code_AgrupationA1,Code_Hierarchy_All,Code_Hierarchy1,Code_Hierarchy2,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_MARA.MATNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(12),ERP_MARA.MATKL) AS Code_ClassificationA, --Grupo de artículos&#xA;-- CONVERT(VARCHAR(12),ERP_FATRIB.ATNAM) AS Code_ClassificationB, -- (Tipo encendedor 12/02/2018) Tipo articulo, 20/11/2017 pedro lo tiene que revisar&#xA;CONVERT(VARCHAR(12),ERP_MARA.MTART) AS Code_ClassificationC, -- Tipo de material &#xA;-- CONVERT(VARCHAR(12),ERP_FATRIB.ATNAM) AS Code_ClassificationD, -- Canal de ventas 12/02/2018&#xA;-- CONVERT(NUMERIC(11,3),REPLACE(REPLACE(ERP_MARM.UMREZ,'.',''),',','.')) AS Factor_Conversion,&#xA;1 AS Factor_Conversion,&#xA;CONVERT(VARCHAR(4),ERP_MARA.MEINS) AS Unit_Type_Sales, -- Unidad de medida de venta type_unit&#xA;CONVERT(VARCHAR(8),ERP_MARA.ERSDA) AS Date_Create,&#xA;CONVERT(VARCHAR(8),ERP_MARA.LAEDA) AS Date_Modify,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(ERP_MARA.BRGEW,'.',''),',','.')) AS Weight,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(ERP_MARA.NTGEW,'.',''),',','.')) AS Weight_Net,&#xA;CONVERT(VARCHAR(4),ERP_MARA.GEWEI) AS Unit_Type_Weight, -- Unidad de peso products_array(601)&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(ERP_MARA.VOLUM,'.',''),',','.')) AS Volume,&#xA;CONVERT(VARCHAR(50),ISNULL(ERP_MAKT.MAKTX,ERP_MARA.MATNR)) AS Description,&#xA;CONVERT(VARCHAR(4),ERP_MARA.VOLEH) AS Unit_Type_Volumen, -- Unidad de medida del volumen type_unit&#xA;CONVERT(VARCHAR(12),ERP_MARA.MATKL) AS Code_AgrupationA1, --  marcacom = tbMara(&quot;matkl&quot;)  products_array(602)&#xA;CONVERT(VARCHAR(60),ERP_MARA.PRDHA) AS Code_Hierarchy_All,&#xA;CONVERT(VARCHAR(12),RIGHT(LEFT(RTRIM(LTRIM(ERP_MARA.PRDHA)),7),3)) AS Code_Hierarchy1, --marcaest = Right(Left(tbMara(&quot;PRDHA&quot;), 7), 3)&#xA;CONVERT(VARCHAR(12),RIGHT(LEFT(RTRIM(LTRIM(ERP_MARA.PRDHA)),10),3)) AS Code_Hierarchy2,--grupoest = Right(Left(tbMara(&quot;PRDHA&quot;), 10), 3)&#xA;'0' AS Source_Create,&#xA;IIF(ISNULL(ERP_MARA.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_MARA --MATERIALES - DATOS GENERALES&#xA;LEFT JOIN ERP_MAKT&#xA;ON ERP_MARA.MATNR = ERP_MAKT.MATNR&#xA;AND ERP_MAKT.SPRAS='S'&#xA;&#xA;PRINT 'ERP_IMP_products UPDATE ZZTIPENC'&#xA;-- Actualizo el tipo de encendedor&#xA;UPDATE ERP_IMP_products&#xA;SET Code_ClassificationB = T.ATWRT&#xA;FROM &#xA;(&#xA;    SELECT MATNR,ATWRT&#xA;    FROM ERP_FATRIB&#xA;    WHERE 1=1&#xA;    AND ATNAM='ZZTIPENC'--tipo encendedor&#xA;) T&#xA;WHERE 1=1&#xA;AND ERP_IMP_products.Transfer_Status=0&#xA;AND ERP_IMP_products.Code_ofClient = T.MATNR&#xA;&#xA;PRINT 'ERP_IMP_products UPDATE ZZCANVEN'&#xA;-- Actualizo el canal de ventas &#xA;UPDATE ERP_IMP_products&#xA;SET Code_ClassificationD = T.ATWRT&#xA;FROM &#xA;(&#xA;    SELECT MATNR,ATWRT&#xA;    FROM ERP_FATRIB&#xA;    WHERE 1=1&#xA;    AND ATNAM='ZZCANVEN'--canal de ventas&#xA;) T&#xA;WHERE 1=1&#xA;AND ERP_IMP_products.Transfer_Status=0&#xA;AND ERP_IMP_products.Code_ofClient = T.MATNR&#xA;&#xA;/*&#xA;las medidas van en otra tabla&#xA;LEFT JOIN ERP_MARM -- MATERIALES - UNIDADES DE MEDIDA&#xA;ON ERP_MARA.MATNR = ERP_MARM.MATNR&#xA;*/&#xA;&#xA;/*&#xA;ERP_MAKT MATERIALES - DESCRIPCION SEGUN IDIOMA&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| campo |       descripcion        | clave | tipo_datos | longitud |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| MATNR | Código del material      |     1 | CHAR       |       18 |&#xA;| SPRAS | Idioma                   |     2 | CHAR       |        1 |&#xA;| MAKTX | Descripción del material |       | CHAR       |       40 |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;&#xA;ERP_MARA --MATERIALES - DATOS GENERALES&#xA;+-------+---------------------------------+-------+------------+----------+&#xA;| campo |           descripcion           | clave | tipo_datos | longitud |&#xA;+-------+---------------------------------+-------+------------+----------+&#xA;| MATNR | Código del material             |     1 | CHAR       |       18 |&#xA;| MTART | Tipo de material                |       | CHAR       |        4 |&#xA;| PRDHA | Jerarquía del material          |       | CHAR       |       18 |&#xA;| MATKL | Grupo de artículos              |       | CHAR       |        9 |&#xA;| ERSDA | Fecha de creación               |       | DATS       |        8 |&#xA;| LAEDA | Fecha de la última modificación |       | DATS       |        8 |&#xA;| MEINS | Unidad de medida de venta       |       | CHAR       |        3 |&#xA;| BRGEW | Peso bruto                      |       | CHAR       |       15 |&#xA;| NTGEW | Peso neto                       |       | CHAR       |       15 |&#xA;| GEWEI | Unidad de medida del peso       |       | CHAR       |        3 |&#xA;| VOLUM | Volumen                         |       | CHAR       |       15 |&#xA;| VOLEH | Unidad de medida del volumen    |       | CHAR       |        3 |&#xA;+-------+---------------------------------+-------+------------+----------+&#xA;&#xA;ERP_MARM -- MATERIALES - UNIDADES DE MEDIDA&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;| campo |                      descripcion                      | clave | tipo_datos | longitud |&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;| MATNR | Código del material                                   |     1 | CHAR       |       18 |&#xA;| MEINH | Unidad de medida alternativa                          |     2 | CHAR       |        3 |&#xA;| UMREZ | Número de unidades de la unidad de venta              |       | DEC        |        6 |&#xA;| UMREN | Número de unidades de la unidad de medida alternativa |       | DEC        |        6 |&#xA;| EAN11 | Código de barras de la unidad de medida               |       | CHAR       |       18 |&#xA;| LAENG | Longitud                                              |       | DEC        |       15 |&#xA;| BREIT | Ancho                                                 |       | DEC        |       15 |&#xA;| HOEHE | Alto                                                  |       | DEC        |       15 |&#xA;| MEABM | Unidad de medida para longitud/ancho/altura           |       | CHAR       |        3 |&#xA;| VOLUM | Volumen                                               |       | DEC        |       15 |&#xA;| VOLEH | Unidad de volumen                                     |       | CHAR       |        3 |&#xA;| BRGEW | Peso bruto                                            |       | DEC        |       15 |&#xA;| GEWEI | Unidad de peso                                        |       | CHAR       |        3 |&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
