          SQLTask:SqlStatementSource="-- 460_erp_imp_prj_fcabrap 1.0.0&#xA;&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_prj_fcabrap&#xA;SELECT * FROM ERP_IMP_prj_fcabrap ORDER BY id DESC&#xA;UPDATE ERP_IMP_prj_fcabrap SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_prj_fcabrap insert: '&#xA;INSERT INTO ERP_IMP_prj_fcabrap &#xA;(Code_ofClientSeller,Code_Unit_Org,Code_Sales_Org,ZFECRAP,ZANYRT,ZRUTAV&#xA;,ZDIASHAB,ZDIASTRA,ZVISDIA,ZPEDDIA,ZPEDDIA2,ZCOMIDAEN,ZCOMIDACON,ZPERNOCTAEN,ZCDIETA,ZVALDIE,ZWAERKD&#xA;,ZCGASOL,ZKMSCIU,ZKMSCAR,ZVALGAS,ZWAERKG,ZCPRCLDS,ZNPRCLDS,ZWAERKR,ZFECPRO,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT &#xA;so.PERNR,so.Code_Unit_Org,so.Code_Sales_Org,ZFECRAP,ZANYRT,ZRUTAV&#xA;,ZDIASHAB,ZDIASTRA,ZVISDIA,ZPEDDIA,ZPEDDIA2,ZCOMIDAEN,ZCOMIDACON,ZPERNOCTAEN,ZCDIETA,ZVALDIE,ZWAERKD&#xA;,ZCGASOL,ZKMSCIU,ZKMSCAR,ZVALGAS,ZWAERKG,ZCPRCLDS,ZNPRCLDS,ZWAERKR,ZFECPRO,&#xA;'0' AS  Source_Create,&#xA;IIF(ISNULL(ERP_FCABRAP.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FCABRAP&#xA;INNER JOIN&#xA;(&#xA;    -- según la carga de erp_imp_sales_organization&#xA;    SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;    --,s.SPART --sector se quita&#xA;    ,souo.BUKRS AS Code_Unit_Org&#xA;    --,u.ZCEMPLE AS Code_User&#xA;    ,s.PERNR --AS Code_Seller&#xA;    --,'' AS status&#xA;    FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;    INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;    ON u.VKORG = s.VKORG --code_unit_org&#xA;    AND u.VTWEG = s.VTWEG --canal dist&#xA;    AND u.PERNR = s.PERNR  --code_seller&#xA;    INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;    ON s.VKORG = souo.VKORG&#xA;    AND souo.SPRAS = 'S'&#xA;) so&#xA;ON VKORG+VTWEG = so.Code_Sales_Org&#xA;AND ERP_FCABRAP.PERNR = so.PERNR&#xA;&#xA;/*&#xA;ERP_FCABRAP&#xA;+----