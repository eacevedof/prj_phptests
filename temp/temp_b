          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 440_erp_imp_products_organization 5.0.1&#xA;&#xA;/*&#xA;-- TRUNCATE TABLE ERP_IMP_products_organization&#xA;SELECT * FROM ERP_IMP_products_organization ORDER BY id DESC&#xA;UPDATE ERP_IMP_products_organization SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'PARCHE update ERP_MVKE.SPART=01 (encendedores)' &#xA;UPDATE ERP_MVKE SET SPART = '01' WHERE LTRIM(RTRIM(SPART))=''&#xA;&#xA;PRINT 'ERP_IMP_products_organization insert: '&#xA;INSERT INTO ERP_IMP_products_organization &#xA;(Code_Sector,Code_ofClient,Code_Unit_Org,Code_Sales_Org,&#xA;Code_ClassificationD,Code_ClassificationE,Group_Position,Group_Stadist,Locked,&#xA;Code_ClassificationM,AUMNG,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT CONVERT(VARCHAR(12),ERP_MVKE.SPART) AS Sector,&#xA;CONVERT(VARCHAR(24),ERP_MVKE.MATNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(12),ERP_TVKO.BUKRS) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),ERP_MVKE.VKORG+VTWEG) AS Code_Sales_Org,&#xA;--CONVERT(VARCHAR(),ERP_PRJ_PRODUCTS_SECTOR.Code_Class1) AS ,&#xA;CONVERT(VARCHAR(12),ERP_MVKE.KONDM) AS Code_ClassificationD, --Grupo de materiales prd_ar(500)&#xA;CONVERT(VARCHAR(12),ERP_MVKE.DWERK) AS Code_ClassificationE, --Centro de expedición por defecto prd_ar(505)&#xA;CONVERT(VARCHAR(12),ERP_MVKE.MTPOS) AS Group_Position,&#xA;CONVERT(VARCHAR(12),ERP_MVKE.VERSG) AS Group_Stadist,&#xA;CONVERT(VARCHAR(2),IIF(ERP_MVKE.PRAT1='X','1',0)) AS Locked, -- Venta en terminal &#xA;CONVERT(VARCHAR(4),ERP_MVKE.RDPRF) AS Code_ClassificationM, -- Unidad de medida para redondeo en pedidos prd_ar(506)&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(ERP_MVKE.AUMNG,'.',''),',','.')) AS AUMNG,&#xA;'0' AS  Source_Create,&#xA;IIF(ISNULL(ERP_MVKE.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_MVKE --MATERIALES - DATOS POR ÁREA DE VENTAS&#xA;INNER JOIN &#xA;(&#xA;    SELECT DISTINCT VKORG,BUKRS&#xA;    FROM ERP_TVKO&#xA;    WHERE 1=1&#xA;    AND SPRAS='S'    &#xA;) AS ERP_TVKO&#xA;ON ERP_MVKE.VKORG = ERP_TVKO.VKORG&#xA;WHERE 1=1&#xA;-- !!! parche para que cargue los canales que usan los vendedores&#xA;AND ERP_MVKE.VTWEG IN &#xA;(&#xA;    SELECT DISTINCT VTWEG &#xA;    FROM ERP_FAGENT        &#xA;)&#xA;&#xA;/*&#xA;la tabla MARM, con MEINH = MVKE-RDPRF, de cara a obtener el valor de UMREZ, que&#xA;corresponde a la cantidad a utilizar para el redondeo. En el supuesto de que no se&#xA;encontrara información en MARM habría que interpretar que RDPRF no tiene&#xA;*/&#xA;PRINT 'ERP_IMP_products_organization update REDONDEO'&#xA;UPDATE ERP_IMP_products_organization&#xA;SET Factor_Conversion = t.UMREZ&#xA;FROM &#xA;(&#xA;    --DATO PARA REDONDEO Factor_Conversion_New &#xA;    SELECT um.MATNR,MEINH,UMREZ,RDPRF,udef.Code_Sales_Org&#xA;    FROM&#xA;    (&#xA;        -- MEINH: Unidad de medida alternativa &#xA;        -- UMREZ: Número de unidades de la unidad de venta&#xA;        SELECT DISTINCT MATNR,MEINH,UMREZ &#xA;        FROM ERP_MARM -- MATERIALES - UNIDADES DE MEDIDA&#xA;        --ORDER BY 1,2,3&#xA;    ) AS um -- units measure&#xA;    INNER JOIN&#xA;    (&#xA;        -- RDFRP: Unidad de medida para redondeo en pedidos&#xA;        SELECT DISTINCT MATNR,RDPRF,VKORG+VTWEG AS Code_Sales_Org &#xA;        FROM ERP_MVKE -- MATERIALES - DATOS POR ÁREA DE VENTAS&#xA;        WHERE RDPRF!=''&#x9;&#xA;    ) AS udef -- unidad por defecto&#xA;    ON um.MATNR = udef.MATNR&#xA;    AND um.MEINH = udef.RDPRF&#xA;    --AND um.MATNR = 'CL2A216H'&#xA;) t&#xA;WHERE 1=1&#xA;AND ERP_IMP_products_organization.Code_ofClient = t.MATNR&#xA;AND ERP_IMP_products_organization.Code_Sales_Org = t.Code_Sales_Org&#xA;AND ERP_IMP_products_organization.Code_ClassificationM = t.RDPRF&#xA;AND Transfer_status=0&#xA;&#xA;&#xA;/*&#xA;ERP_MVKE    MATERIALES - DATOS POR ÁREA DE VENTAS&#xA;+-------+-------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |                descripcion                | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+-------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;| MATNR | Código del material                       |     1 | CHAR       |       18 | NULL      | MARA           |&#xA;| VKORG | Organización de ventas                    |     2 | CHAR       |        4 | NULL      | TVKO           |&#xA;| VTWEG | Canal de distribución                     |     3 | CHAR       |        2 | NULL      | TVTW           |&#xA;| MTPOS | Grupo de tipos de posición                |       | CHAR       |        4 | NULL      |                |&#xA;| VERSG | Grupo de estadísticas del material        |       | CHAR       |        1 | NULL      |                |&#xA;| KONDM | Grupo de materiales                       |       | CHAR       |        2 | NULL      |                |&#xA;| PRAT1 | Venta en terminal                         |       | CHAR       |        1 | NULL      |                |&#xA;| PRAT2 | Retener pedido recibido en central        |       | CHAR       |        1 | NULL      |                |&#xA;| SPART | Sector                                    |       | CHAR       |        2 | NULL      | TSPA           |&#xA;| DWERK | Centro de expedición por defecto          |       | CHAR       |        4 | NULL      | T001W          |&#xA;| AUMNG | Cantidad mínima de pedido                 |       | DEC        |       13 | 3         |                |&#xA;| RDPRF | Unidad de medida para redondeo en pedidos |       | CHAR       |        4 | NULL      |                |&#xA;+-------+-------------------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ZMATMNP - MATERIALES QUE AFECTAN AL MINIMO PEDIDO&#xA;+--------+-------------------------------------------------------------+-------+------------+----------+&#xA;| campo  |                         descripcion                         | clave | tipo_datos | longitud |&#xA;+--------+-------------------------------------------------------------+-------+------------+----------+&#xA;| VKORG  | Organización de ventas                                      |     1 | CHAR       |        4 |&#xA;| VTWEG  | Canal de distribución                                       |     2 | CHAR       |        2 |&#xA;| SPART  | Sector                                                      |     3 | CHAR       |        2 |&#xA;| MATNR  | Código del material                                         |     4 | CHAR       |       18 |&#xA;| WAERS  | Moneda                                                      |       | CUKY       |        5 |&#xA;| IMPMIN | Importe mínimo del pedido permitido al añadir este material |       | CURR       |       11 |&#xA;+--------+-------------------------------------------------------------+-------+------------+----------+&#xA;&#xA;+----------------------+------+-----------+----------+&#xA;|      fieldname       | ispk | fieldtype | fieldlen |&#xA;+----------------------+------+-----------+----------+&#xA;| ID                   |      | int       |        4 |&#xA;| Code_Product         |      | numeric   |        9 |&#xA;| Code_Sector          |      | varchar   |        2 |&#xA;| Code_ofClient        |      | varchar   |       24 |&#xA;| Code_Unit_Org        |      | varchar   |       12 |&#xA;| Code_Sales_Org       |      | varchar   |       12 |&#xA;| Code_Status          |      | varchar   |       12 |&#xA;| Locked               |      | varchar   |        2 |&#xA;| Factor_Conversion    |      | numeric   |        9 |&#xA;| Unit_Box             |      | numeric   |        5 |&#xA;| Unit_Pallet          |      | numeric   |        5 |&#xA;| Unit_Type_Sales      |      | varchar   |        4 |&#xA;| Unit_Type_Inv        |      | varchar   |        4 |&#xA;| Code_ClassificationD |      | varchar   |       12 |&#xA;| Code_ClassificationE |      | varchar   |       12 |&#xA;| TaxClassification    |      | varchar   |       24 |&#xA;| Group_Position       |      | varchar   |       12 |&#xA;| Group_Stadist        |      | varchar   |       12 |&#xA;| AUMNG                |      | numeric   |        9 |&#xA;| Code_ClassificationM |      | varchar   |        4 |&#xA;+----------------------+------+-----------+----------+&#xA;&#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
