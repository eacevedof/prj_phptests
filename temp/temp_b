          SQLTask:SqlStatementSource="-- 820_ERP_IMP_prj_hist_orders_lines.sql 3.0.0&#xA;/**&#xA;TRUNCATE TABLE ERP_IMP_prj_hist_orders_lines&#xA;UPDATE ERP_IMP_prj_hist_orders_lines SET Transfer_Status=100 &#xA;SELECT * FROM ERP_IMP_prj_hist_orders_lines&#xA;**/&#xA;PRINT 'ERP_IMP_prj_hist_orders_lines insert:'&#xA;INSERT INTO ERP_IMP_prj_hist_orders_lines &#xA;(Order_Num_ofClient,Num_Line,Code_ofClientProduct,Quantity,Price,Net_Amount,&#xA;Total_Discount,Total_Amount,Order_Date,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT&#xA;CONVERT(VARCHAR(24),VBELN) AS Order_Num_ofClient,&#xA;-- PARCHE POSNR. Esto genera duplicados&#xA;--CONVERT(NUMERIC(4,0),RIGHT(POSNR,4)) AS Num_Line,--POSNR,&#xA;CONVERT(NUMERIC(6,0),RIGHT(POSNR,6)) AS Num_Line,--POSNR,&#xA;CONVERT(VARCHAR(24),MATNR) AS Code_ofClientProduct,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(LSMENG,'.',''),',','.')) AS Quantity,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(NETPR,'.',''),',','.')) AS Price,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IMPNET,'.',''),',','.')) AS Net_Amount,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IMPDTO,'.',''),',','.')) AS Total_Discount,&#xA;CONVERT(NUMERIC(11,3),REPLACE(REPLACE(IMPNET,'.',''),',','.')) AS Total_Amount,&#xA;CONVERT(VARCHAR(8),EDATU) AS Order_Date,&#xA;&#xA;'0' AS Source_Create, IIF(ISNULL(ERP_VBAP.STATUS,'')='',0,IIF(ERP_VBAP.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM  ERP_VBAP&#xA;ORDER BY 1,2&#xA;&#xA;/*&#xA;VBAP&#x9;&#xA;Order_Num_ofClient&#x9;1&#x9;VBELN&#x9;Documento de ventas&#x9;1&#x9;CHAR&#x9;10&#x9;0&#x9;VBAK&#x9;&#xA;Num_Line&#x9;2&#x9;POSNR&#x9;Posición del documento de ventas&#x9;2&#x9;CHAR&#x9;6&#x9;0&#x9;&#x9;&#xA;Code_ofClientProduct&#x9;3&#x9;MATNR&#x9;Código del material&#x9;&#x9;CHAR&#x9;18&#x9;0&#x9;MARA&#x9;&#xA;Quantity&#x9;4&#x9;LSMENG&#x9;Cantidad&#x9;&#x9;QUAN&#x9;13&#x9;0&#x9;&#x9;&#x