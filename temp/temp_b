          SQLTask:SqlStatementSource="--160_erp_imp_prj_users_organ_seller 3.0.1&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_prj_users_organ_seller&#xA;DELETE FROM ERP_IMP_prj_users_organ_seller&#xA;SELECT * FROM ERP_IMP_prj_users_organ_seller&#xA;*/&#xA;PRINT 'ERP_IMP_prj_users_organ_seller insert:'&#xA;INSERT INTO ERP_IMP_prj_users_organ_seller &#xA;(Code_Unit_Org, Code_Sales_Org,Code_ofClientUser, Code_ofClientSeller,&#xA;VTWEG, LIFNR, SAKNR, KOSTL, ZFECINI, ZFECFIN, ZCGASOL&#xA;,Source_Create, Transfer_Status, Status_SAP, Transfer_Date)&#xA;&#xA;SELECT DISTINCT Code_Unit_Org,Code_Sales_Org,Code_User AS Code_ofClientUser,Code_Seller AS Code_ofClientSeller,&#xA;VTWEG, LIFNR, SAKNR, KOSTL, ZFECINI, ZFECFIN, ZCGASOL,&#xA;'0' AS Source_Create, IIF(ISNULL(uos.status,'')='',0,9) AS Transfer_status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT u.VKORG+u.VTWEG AS Code_Sales_Org&#xA;    --,s.SPART --sector se quita&#xA;    ,souo.BUKRS AS Code_Unit_Org&#xA;    ,u.ZCEMPLE AS Code_User&#xA;    ,s.PERNR AS Code_Seller&#xA;    ,u.VTWEG, LIFNR, SAKNR, KOSTL, ZFECINI, ZFECFIN, ZCGASOL    &#xA;    ,'' AS status&#xA;    FROM ERP_FAGEEM AS u  --relacion usuario/vendedor/salesorg&#xA;    INNER JOIN ERP_FAGENT AS  s --relacion vendedor/salesorg&#xA;    ON u.VKORG = s.VKORG --code_unit_org&#xA;    AND u.VTWEG = s.VTWEG --canal dist&#xA;    AND u.PERNR = s.PERNR  --code_seller&#xA;    INNER JOIN ERP_TVKO AS souo --relacion unitorg/salesorg&#xA;    ON s.VKORG = souo.VKORG&#xA;    AND souo.SPRAS = 'S'&#xA;    &#xA;    UNION&#xA;    &#xA;    -- usuario generico&#xA;    SELECT DISTINCT acsel.VKORG+acsel.VTWEG AS Code_Sales_Org &#xA;    ,souo.BUKRS AS Code_Unit_Org&#xA;    ,'9999' AS Code_User,'9999' AS Code_Seller&#xA;    ,acsel.VTWEG AS VTWEG, NULL AS LIFNR, NULL AS SAKNR,NULL AS KOSTL&#xA;    ,NULL AS ZFECINI,NULL AS ZFECFIN,NULL AS ZCGASOL    &#xA;    ,'' AS Status&#xA;    FROM ERP_KNVPBIS AS acsel  --relacion cliente/salesorg/vendedor&#xA;    INNER JOIN ERP_TVKO as souo&#xA;    ON acsel.VKORG = souo.VKORG&#xA;    AND souo.SPRAS = 'S'&#xA;    WHERE 1=1&#xA;    -- !!! parche para que cargue los canales que usan los vendedores&#xA;    AND acsel.VTWEG IN &#xA;    (&#xA;        SELECT DISTINCT VTWEG &#xA;        FROM ERP_FAGENT        &#xA;    )&#xA;&#xA;    UNION&#xA;&#xA;    -- vendedores cuentas con vendedores que no nos han pasado&#xA;    SELECT DISTINCT acsel.VKORG+VTWEG AS Code_Sales_Org&#xA;    ,souo.BUKRS AS Code_Unit_Org&#xA;    ,'9999' AS Code_User,PERNR AS Code_Seller&#xA;    ,acsel.VTWEG AS VTWEG, NULL AS LIFNR, NULL AS SAKNR,NULL AS KOSTL&#xA;    ,NULL AS ZFECINI,NULL AS ZFECFIN,NULL AS ZCGASOL  &#xA;    ,'' AS Status&#xA;    FROM ERP_KNVPBIS AS acsel&#xA;    INNER JOIN ERP_TVKO AS souo&#xA;    ON acsel.VKORG = souo.VKORG&#xA;    WHERE 1=1&#xA;    AND acsel.PERNR NOT IN&#xA;    (&#xA;        SELECT PERNR&#xA;        FROM ERP_FAGENT&#xA;    )&#xA;    -- !!! parche para que cargue los canales que usan los vendedores&#xA;    AND acsel.VTWEG IN &#xA;    (&#xA;        SELECT DISTINCT VTWEG &#xA;        FROM ERP_FAGENT        &#xA;    )    &#xA;)&#xA;AS uos&#xA;&#xA;PRINT 'update ERP_IMP_prj_users_organ_seller code_user y code_seller a 9999 '&#xA;UPDATE ERP_IMP_prj_users_organ_seller&#xA;SET Code_Seller='9999'&#xA;,Code_User='9999'&#xA;WHERE 1=1&#xA;AND Transfer_status='0'&#xA;AND Code_ofClientSeller = '9999'&#xA;&#xA;/*&#xA;ERP_TVKO&#x9;ZSDTV636&#x9;ORGANIZACIONES DE VENTAS&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;| campo |             descripcion              | clave | tipo_datos | longitud |&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;| SPRAS | Idioma de la descripción             |     1 | CHAR       |        1 |&#xA;| VKORG | Organización de ventas               |     2 | CHAR       |        4 |&#xA;| VTEXT | Descripción                          |       | CHAR       |       20 |&#xA;| BUKRS | Sociedad a la que pertenece          |       | CHAR       |        4 |&#xA;| MOREI | Variante de reglamentación de viajes |       | CHAR       |        2 |&#xA;+-------+--------------------------------------+-------+------------+----------+&#xA;&#xA;ERP_FAGENT ZSDTV652&#x9;AGENTES&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |      descripcion       | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;| PERNR | Código de agente       |     1 | NUMC       |        8 |         0 | FAGENT         |&#xA;| VKORG | Organización de ventas |       | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG | Canal de distribución  |       | CHAR       |        2 |         0 | TVTW           |&#xA;| SPART | Sector                 |       | CHAR       |        2 |         0 | TSPA           |&#xA;| NAME1 | Nombre del agente      |       | CHAR       |       35 |         0 |                |&#xA;+-------+------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_FAGEEM ENLACE ENTRE CODIGO AGENTE Y EMPLEADO&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;|  campo  |          descripcion          | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;| PERNR   | Código de agente              |     1 | NUMC       |        8 |         0 | FAGENT         |&#xA;| VKORG   | Organización de ventas        |     2 | CHAR       |        4 |         0 | TVKO           |&#xA;| VTWEG   | Canal de distribución         |     3 | CHAR       |        2 |         0 | TVTW           |&#xA;| ZCEMPLE | Código de empleado            |     4 | DEC        |        4 |         0 |                |&#xA;| ZNEMPLE | Nombre del empleado           |       | CHAR       |       45 |         0 |                |&#xA;| ZFECINI | Fecha de inicio               |       | DATS       |        8 |         0 |                |&#xA;| ZFECFIN | Fecha de fin                  |       | DATS       |        8 |         0 |                |&#xA;| LIFNR   | Código de acreedor del agente |       | CHAR       |       10 |         0 |                |&#xA;| SAKNR   | Cuenta de mayor               |       | CHAR       |       10 |         0 |                |&#xA;| KOSTL   | Centro de coste               |       | CHAR       |       10 |         0 |                |&#xA;| ZCGASOL | Tipo de gasolina por defecto  |       | CHAR       |        1 |         0 | FGASOL         |&#xA;+---------+-------------------------------+-------+------------+----------+-----------+----------------+&#xA;&#xA;ERP_KNVPBIS CLIENTES - AGENTES DE VENTAS&#xA;+-------+------------------------------+-------+------------+----------+&#xA;| campo |         descripcion          | clave | tipo_datos | longitud |&#xA;+-------+------------------------------+-------+------------+----------+&#xA;| KUNNR | Código del cliente           |     1 | CHAR       |       10 |&#xA;| VKORG | Organización de ventas       |     2 | CHAR       |        4 |&#xA;| VTWEG | Canal de distribución        |     3 | CHAR       |        2 |&#xA;| SPART | Sector                       |     4 | CHAR       |        2 |&#xA;| PARZA | Contador número interlocutor |     5 | NUMC       |        3 |&#xA;| PERNR | Código del agente            |       | NUMC       |        8 |&#xA;+-------+------------------------------+-------+------------+----------+&#xA;&#xA;ERP_IMP_prj_users_organ_seller&#xA;+---------------------+------+-----------+----------+&#xA;|      fieldname      | ispk | fieldtype | fieldlen |&#xA;+---------------------+------+-----------+----------+&#xA;| ID                  |      | int    