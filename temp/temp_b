ode2)&#xA;        VALUES(@sSecuence,'ZBQ57','N',57,&#xA;        '17','47')&#xA;&#xA;        --marca&#xA;        --01:so,17:prdh1&#xA;        INSERT INTO ERP_IMP_pricing_structure         &#xA;        (Secuence,Code_Table,Code_Type,Orden,&#xA;        Code1)&#xA;        VALUES(@sSecuence,'ZBQ58','N',58,&#xA;        '17')&#xA;    END&#x9;--fin material&#xA;&#x9;&#xA;    FETCH NEXT FROM oCursorField INTO @sSecuence,@zbq0,@zbq1,@zbq2,@zbq3,@zbq4,@zbq5&#xA;END&#xA;CLOSE oCursorField&#xA;DEALLOCATE oCursorField&#xA;&#xA;/*&#xA;DTO --&gt; 0&#xA;DTO ADI --&gt; 0&#xA;CANON --&gt; 0 (Aquí habría que multiplicar el Canon por la cantidad para que sea ese total el que se le suma al Importe Neto )&#xA;&#xA;DTO PP --&gt; 1 (Esta secuencia aún no está creada pero no nos afectará nada. &#xA;Ya veré cuando la añado y te pasamos la variable. De momento no la aplicaremos)&#xA;IVA --&gt; 1&#xA;REQ --&gt; 1&#xA;*/&#xA;PRINT 'UPDATE On_Base 0 Dtos'&#xA;UPDATE ERP_IMP_pricing_structure&#xA;SET On_Base = 0&#xA;WHERE 1=1&#xA;AND Transfer_Status IS NULL&#xA;AND Secuence IN &#xA;(&#xA;    'ZDPC','ZDPM' -- DTO PRECIO&#xA;    ,'ZDCC','ZDCM','ZDIT' -- DESCUENTO&#xA;    ,'ZDAC','ZDAM' -- DTO ADICIONAL&#xA;)&#xA;&#xA;PRINT 'UPDATE On_Base=0 Precios'&#xA;/*&#xA;1º La secuencia de precio debe tener un onBase igual a 0, &#xA;si no no funciona correctamente todo el calculo del precio, descuentos y tasas.&#xA;Respecto a lo del resumen, lo del descuento que dices que no lo muestra bien en el &#xA;bruto-descuento es debido a que no está bien el onBase en las secuencias de precio,&#xA;tiene que ser 0. &#xA;UPDATE ERP_IMP_pricing_structure&#xA;SET On_Base = 0&#xA;WHERE 1=1&#xA;AND Transfer_Status IS NULL&#xA;AND Secuence IN &#xA;(   &#xA;    'ZPUC1' -- PRECIO Cuenta&#xA;    ,'ZPUC2' -- PRECIO Hierarcy&#xA;    ,'ZPUM' -- PRECIO producto&#xA;)&#xA;*/&#xA;&#xA;-- mejoro la anterior con IN &#xA;UPDATE ERP_IMP_pricing_structure &#xA;SET On_Base = 0&#xA;WHERE Transfer_Status IS NULL&#xA;AND Secuence LIKE 'ZP%'&#xA;&#xA;--UPDATE ERP_IMP_pricing_structure SET On_Base=1 WHERE Secuence IN ('ZPUC1','ZPUC2','ZPUM')&#xA;UPDATE ERP_IMP_pricing_structure SET On_Base=0 WHERE Secuence IN ('ZDPC','ZDPM')&#xA;-- 06/03/2018 lo cambio a consecuencia de este email: Otro ejemplo de que suma los Dtos&#xA;UPDATE ERP_IMP_pricing_structure SET On_Base=0 WHERE Secuence IN ('ZDCC','ZDCM','ZDIT')&#xA;UPDATE ERP_IMP_pricing_structure SET On_Base=0 WHERE Secuence IN ('ZDAC','ZDAM')&#xA;--UPDATE ERP_IMP_pricing_structure SET On_Base=0 WHERE Secuence IN ('ZZCANON')&#xA;&#xA;PRINT 'UPDATE Transfer_Status,Transfer_Date'&#xA;UPDATE ERP_IMP_pricing_structure&#xA;SET Source_Create='0'&#xA;,Transfer_Status=0&#xA;,Status_SAP='0'&#xA;,Transfer_Date=@sToday&#xA;WHERE Transfer_Status IS NULL&#xA;&#xA;-- Agrego impuestos, recargos y cannon&#xA;-- estos code_table tambien hay que agregarlos en erp_imp_pricing_tables_loop&#xA;-- ZZCANON&#xA;-- 30/11/2017 el canon debe ir como primer impuesto y se aplica sobre el total (on_base=1)&#xA;INSERT INTO ERP_IMP_pricing_structure         &#xA;(Secuence,Code_Table,Orden,Code_Type,Code1,Code2,Code3,On_Base&#xA;,Source_Create,Transfer_Status,Status_SAP,Transfer_Date)&#xA;VALUES('ZZCANON','ZBQ600',600,'N','02','46',NULL,'0'&#xA;,'0',0,'0',@sToday)&#x9;&#xA;&#xA;-- SECUENCIA MWST 02:pais,27:imp cliente,28:imp.producto&#xA;INSERT INTO ERP_IMP_pricing_structure         &#xA;(Secuence,Code_Table,Orden,Code_Type,Code1,Code2,Code3,On_Base&#xA;,Source_Create,Transfer_Status,Status_SAP,Transfer_Date)&#xA;VALUES('MWST','ZBQ700',700,'N','02','27','28','1'&#xA;,'0',0,'0',@sToday)&#x9;&#xA;&#xA;-- ZREC&#xA;-- 02:pais,44:customer recargo,45:products recargo&#xA;INSERT INTO ERP_IMP_pricing_structure         &#xA;(Secuence,Code_Table,Orden,Code_Type,Code1,Code2,Code3,On_Base&#xA;,Source_Create,Transfer_Status,Status_SAP,Transfer_Date)&#xA;VALUES('ZREC','ZBQ800',800,'N','02','44','45','1'&#xA;,'0',0,'0',@sToday)&#x9;&#xA;&#xA;/*&#xA;CARGAR ESTAS CONSTANTES:&#xA;+----------+------------+-------+-------+-------+-------+&#xA;| Secuence | Code_Table | Orden | Code1 | Code2 | Code3 |&#xA;+----------+------------+-------+-------+-------+-------+&#xA;| ZZCANON  | ZBQ300     |     1 |    02 |    28 | NULL  |&#xA;| ZREC     | ZBQ200     |     1 |    02 |    44 | 45    |&#xA;| MWST     | ZBQ100     |     1 |    02 |    27 | 28    |&#xA;+----------+------------+-------+-------+-------+-------+&#xA;&#xA;CLASECON    CONDICIONES A UTILIZAR EN TERMINAL&#x9;&#xA;Se ha eliminado Organización/Canal porque es lo mismo para todas las áreas de ventas + &#xA;cambio orden campos&#xA;&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;|  campo  |                   descripcion                    | clave | tipo_datos | longitud |&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;| ZTPCOND | Tipo de condición comercial en programa terminal |     1 | CHAR       |        1 |&#xA;| ZNUMORD | Número de orden                                  |     2 | CHAR       |        1 |&#xA;| ZTXTCON | Texto tipo condición                             |       | CHAR       |       20 |&#xA;| KSCHL   | Clase de condición                               |       | CHAR       |        4 |&#xA;| ZBQ0    | Mat.Sup.                                         |       | CHAR       |        1 |&#xA;| ZBQ1    | Org./Canal/Clte/Marca                            |       | CHAR       |        1 |&#xA;| ZBQ2    | Org./Canal/Jrarquía/Marca                        |       | CHAR       |        1 |&#xA;| ZBQ3    | Org./Canal/Marca                                 |       | CHAR       |        1 |&#xA;| ZBQ4    | Org./Marca                                       |       | CHAR       |        1 |&#xA;| ZBQ5    | Material                                         |       | CHAR       |        1 |&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;&#xA;prj_pricing_structure&#xA