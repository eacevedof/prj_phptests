          SQLTask:SqlStatementSource="-- 612_erp_imp_pricing_priority.sql 6.0.0&#xA;/*&#xA;-- solo se carga 1 vez (een keer)&#xA;-- TRUNCATE TABLE ERP_IMP_pricing_priority&#xA;SELECT * FROM ERP_IMP_pricing_priority ORDER BY id DESC&#xA;*/&#xA;PRINT 'ERP_IMP_pricing_priority insert:'&#xA;INSERT INTO ERP_IMP_pricing_priority &#xA;(Code_Trademark,Orden,Secuence,Code_Type,Priority,Agrupation,Type_Based,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT Code_Trademark,Orden,Secuence,Code_Type,Priority,Agrupation,Type_Based,&#xA;'0' AS Source_Create,&#xA;Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM &#xA;(&#xA;    SELECT Code_Trademark &#xA;    FROM ERP_IMP_pricing_trademark&#xA;    WHERE 1=1&#xA;    AND Transfer_Status=0&#xA;&#xA;    UNION&#xA;&#xA;    SELECT Code_Trademark&#xA;    FROM prj_pricing_trademark&#xA;) AS Td&#xA;CROSS JOIN&#xA;(&#xA;    -- 2º Las secuencias de descuento siempre deben ir por detrás de las de precio. &#xA;    --Type_Based 0: cantidad, 1:porcentaje&#xA;&#xA;    --PRIORIDAD 1 ==============================================&#xA;    SELECT 1 AS Priority, 1 AS Orden,'ZPUC1' AS Secuence,'1' AS Agrupation,&#xA;    'P' AS Code_Type, '0' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 1 AS Priority, 2 AS Orden,'ZDIT' AS Secuence,'2' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;/*&#xA;    SELECT 1 AS Priority, 2 AS Orden,'ZDPC' AS Secuence,'2' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 1 AS Priority, 3 AS Orden,'ZDPM' AS Secuence,'3' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;*/&#xA;    UNION  &#xA;&#xA;    -- PRIORIDAD 2 ================================================&#xA;    SELECT 2 AS Priority, 4 AS Orden,'ZPUC2' AS Secuence,'1' AS Agrupation,&#xA;    'P' AS Code_Type, '0' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 2 AS Priority, 5 AS Orden,'ZDIT' AS Secuence,'2' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;/*&#xA;    SELECT 2 AS Priority, 5 AS Orden,'ZDPC' AS Secuence,'2' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 2 AS Priority, 6 AS Orden,'ZDPM' AS Secuence,'3' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION&#xA;&#xA;    SELECT 2 AS Priority, 7 AS Orden,'ZDCC' AS Secuence,'4' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 2 AS Priority, 8 AS Orden,'ZDCM' AS Secuence,'5' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 2 AS Priority, 9 AS Orden,'ZDIT' AS Secuence,'6' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;*/&#xA;    UNION&#xA;&#xA;    -- PRIORIDAD 3 ==================================================&#xA;    SELECT 3 AS Priority, 10 AS Orden,'ZPUM' AS Secuence,'1' AS Agrupation,&#xA;    'P' AS Code_Type, '0' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 11 AS Orden,'ZDPC' AS Secuence,'2' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 12 AS Orden,'ZDPM' AS Secuence,'3' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    UNION&#xA;&#xA;    SELECT 3 AS Priority, 13 AS Orden,'ZDCC' AS Secuence,'4' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 14 AS Orden,'ZDCM' AS Secuence,'5' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 15 AS Orden,'ZDIT' AS Secuence,'6' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 16 AS Orden,'ZDAC' AS Secuence,'7' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION  &#xA;&#xA;    SELECT 3 AS Priority, 17 AS Orden,'ZDAM' AS Secuence,'8' AS Agrupation,&#xA;    'D' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status    &#xA;    &#xA;    -- IMPUESTOS Y TASAS&#xA;    -- Type_Based: 1: porcentaje 0:cantidad&#xA;    UNION&#xA;    &#xA;    -- 30/11/2017 el canon debe ir como primer impuesto y se aplica sobre el total (on_base=1)&#xA;    -- 05/12/2017 los impuestos se deben multiplicar por prioridad&#xA;    SELECT 1 AS Priority, 601 AS Orden,'ZZCANON' AS Secuence,'60' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 1 AS Priority, 701 AS Orden,'MWST' AS Secuence,'70' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 1 AS Priority, 801 AS Orden,'ZREC' AS Secuence,'80' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;&#xA;    UNION&#xA;&#xA;    SELECT 2 AS Priority, 602 AS Orden,'ZZCANON' AS Secuence,'60' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 2 AS Priority, 702 AS Orden,'MWST' AS Secuence,'70' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 2 AS Priority, 802 AS Orden,'ZREC' AS Secuence,'80' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status    &#xA;&#xA;    UNION&#xA;&#xA;    SELECT 3 AS Priority, 603 AS Orden,'ZZCANON' AS Secuence,'60' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 3 AS Priority, 703 AS Orden,'MWST' AS Secuence,'70' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status&#xA;    &#xA;    UNION&#xA;    &#xA;    SELECT 3 AS Priority, 803 AS Orden,'ZREC' AS Secuence,'80' AS Agrupation,&#xA;    'T' AS Code_Type, '1' AS Type_Based,0  AS Transfer_Status        &#xA;&#xA;) AS C&#xA;&#xA;/*&#xA;select * from ERP_CLASECON&#xA;+---------+---------+----------------+-------+------+------+------+------+------+------+&#xA;| ZTPCOND | ZNUMORD |    ZTXTCON     | KSCHL | ZBQ0 | ZBQ1 | ZBQ2 | ZBQ3 | ZBQ4 | ZBQ5 |&#xA;+---------+---------+----------------+-------+------+------+------+------+------+------+&#xA;|       1 |       1 | Dto. Precio    | ZDPC  | S    | S    | S    | N    | N    | N    |&#xA;|       1 |       2 | Dto. Precio    | ZDPM  | S    | N    | N    | S    | S    | S    |&#xA;|       2 |       1 | Descuento      | ZDCC  | N    | S    | S    | N    | N    | N    |&#xA;|       2 |       2 | Descuento      | ZDCM  | N    | N    | N    | S    | S    | S    |&#xA;|       2 |       3 | Descuento      | ZDIT  | N    | S    | S    | N    | N    | N    |&#xA;|       3 |       1 | Dto. Adicional | ZDAC  | N    | S    | S    | N    | N    | N    |&#xA;|       3 |       2 | Dto. Adicional | ZDAM  | N    | N    | N    | S    | S    | S    |&#xA;|       4 |       1 | Bonificación   | ZBOC  | N    | S    | S    | N    | N    | N    |&#xA;|       4 |       2 | Bonificación   | ZBOM  | N    | N    | N    | S    | S    | S    |&#xA;|       5 |       1 | Precio. Art    | ZPUC  | N    | S    | S    | N    | N    | N    |&#xA;|       5 |       2 | Precio. Art    | ZPUM  | N    | N    | N    | S    | S    | S    |&#xA;|       6 |       1 | Lote/Expositor | ZPEC  | S    | S    | S    | N    | N    | N    |&#xA;|       6 |       2 | Lote/Expositor | ZPEM  | S    | N    | N    | S    | S    | S    |&#xA;+---------+---------+----------------+-------+------+------+------+------+------+------+&#xA;&#xA;CLASECON    CONDICIONES A UTILIZAR EN TERMINAL&#x9;&#xA;Se ha eliminado Organización/Canal porque es lo mismo para todas las áreas de ventas + &#xA;cambio orden campos&#xA;&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;|  campo  |                   descripcion                    | clave | tipo_datos | longitud |&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;| ZTPCOND | Tipo de condición comercial en programa terminal |     1 | CHAR       |        1 |&#xA;| ZNUMORD | Número de orden                                  |     2 | CHAR       |        1 |&#xA;| ZTXTCON | Texto tipo condición                             |       | CHAR       |       20 |&#xA;| KSCHL   | Clase de condición                               |       | CHAR       |        4 |&#xA;| ZBQ0    | Mat.Sup.                                         |       | CHAR       |        1 |&#xA;| ZBQ1    | Org./Canal/Clte/Marca                            |       | CHAR       |        1 |&#xA;| ZBQ2    | Org./Canal/Jrarquía/Marca                        |       | CHAR       |        1 |&#xA;| ZBQ3    | Org./Canal/Marca                                 |       | CHAR       |        1 |&#xA;| ZBQ4    | Org./Marca                                       |       | CHAR       |        1 |&#xA;| ZBQ5    | Material                                         |       | CHAR       |        1 |&#xA;+---------+--------------------------------------------------+-------+------------+----------+&#xA;&#xA;prj_pricing_priority&#xA;+----------------+------+-----------+----------+&#xA;|   fieldname    | ispk | fieldtype | fieldlen |&#xA;+----------------+------+-----------+----------+&#xA;| Code_Trademark | Y    | varchar   |       50 |&#xA;| Priority       |      | numeric   |        5 |&#xA;| Orden          | Y    | numeric   |        5 |    --ERP_CLASECON.ZNUMORD&#xA;| Secuence       | Y    | varchar   |       10 |    --ERP_CLASECON.KSCHL&#xA;| Code_Type      | Y    | varchar   |        1 |    --prj_pricing_secuence_type&#xA;| Agrupation     |      | varchar   |        2 |    --??? 1..9&#xA;| Type_Based     |      | char      |        1 |    --0: cantidad, 1%&#xA;+----------------+------+-----------+----------+&#xA;&#xA;prj_pricing_secuence_type &#xA;+------+-------------+&#xA;| Code | Description |&#xA;+------+-------------+&#xA;| C    | Cost        |&#xA;| D    | Discounts   |&#xA;| M    | Manual      |&#xA;| P    | Price       |&#xA;| T    | Taxes       |&#xA;+------+-------------+&#xA;*/&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
