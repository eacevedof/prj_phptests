        <SQLTask:SqlTaskData
          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 410_erp_imp_products_measure 3.0.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_products_measure&#xA;SELECT * FROM ERP_IMP_products_measure ORDER BY id DESC&#xA;UPDATE ERP_IMP_products_measure SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_products_measure insert: '&#xA;&#xA;INSERT INTO ERP_IMP_products_measure &#xA;(Code_ofClient,Unit_Measure,Factor_Conversion,EAN,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_MARM.MATNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(4),ERP_MARM.MEINH) AS Unit_Measure,&#xA;CONVERT(NUMERIC(11,3),ERP_MARM.UMREN) AS Factor_Conversion,&#xA;CONVERT(VARCHAR(18),IIF(LTRIM(RTRIM(ERP_MARM.EAN11))='',NULL,LTRIM(RTRIM(ERP_MARM.EAN11)))) AS EAN,&#xA;'0' AS Source_Create,&#xA;IIF(ISNULL(ERP_MARM.STATUS,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_MARM --MATERIALES - UNIDADES DE MEDIDA&#xA;&#xA;/*&#xA;https://mail.google.com/mail/u/0/#sent/161e1083aee27203&#xA;A mostrar en el detalle.&#xA;+----------------------+---------------------------------+&#xA;| EAN del material     | MARM-EAN11 cuando MEINH = ‘ST’  |&#xA;| DUN14 de la caja     | MARM-EAN11 cuando MEINH = ‘CS’  |&#xA;| Unidades por subcaja | MARM-UMREZ cuando MEINH = ‘SCA’ |&#xA;| Unidades por caja    | MARM-UMREZ cuando MEINH = ‘CS’  |&#xA;| Unidades por palet   | MARM-UMREZ cuando MEINH = ‘PAL’ |&#xA;+----------------------+---------------------------------+&#xA;&#xA;+---------------------------+-----------------------+------+------------------------------------------------------------------------+&#xA;|         tablename         |       fieldname       | ispk |                                 origen                                 |&#xA;+---------------------------+-----------------------+------+------------------------------------------------------------------------+&#xA;| products                  | Unit_Type_Sales       |      | -- ERP_MARA.MEINS                                                      |&#xA;| products                  | Unit_Type_Inv         |      | -- ERP_MARA.VOLEH                                                      |&#xA;&#xA;| products_measure          | Code_Product          | Y    |                                                                        |&#xA;| products_measure          | Unit_Measure          | Y    | --ERP_MARM.MEINH Unidad de medida alternativa                          |&#xA;&#xA;| products_organization     | Code_Product          | Y    |                                                                        |&#xA;| products_organization     | Code_Unit_Org         | Y    |                                                                        |&#xA;| products_organization     | Code_Sales_Org        | Y    |                                                                        |&#xA;| products_organization     | Unit_Type_Sales       |      | -- ERP_MARA.MEINS Unidad de medida de venta                            |&#xA;| products_organization     | Unit_Type_Inv         |      | -- ERP_MARA.VOLEH Unidad de medida del volumen                         |&#xA;&#xA;| prj_products_organization | Code_Product          | Y    |                                                                        |&#xA;| prj_products_organization | Code_Unit_Org         | Y    |                                                                        |&#xA;| prj_products_organization | Code_Sales_Org        | Y    |                                                                        |&#xA;| prj_products_organization | Code_ClassificationM  |      | --ERP_MVKE.RDPRF Unidad de medida para redondeo en pedidos prd_ar(506) |&#xA;| prj_products_organization | Factor_Conversion_New |      | --ERP_MARM.UMREZ Número de unidades de la unidad de venta              |&#xA;+---------------------------+-----------------------+------+------------------------------------------------------------------------+&#xA;&#xA;ERP_MARM    MATERIALES - UNIDADES DE MEDIDA&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;| campo |                      descripcion                      | clave | tipo_datos | longitud |&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;| MATNR | Código del material                                   |     1 | CHAR       |       18 |&#xA;| MEINH | Unidad de medida alternativa                          |     2 | CHAR       |        3 |&#xA;| UMREZ | Número de unidades de la unidad de venta              |       | DEC        |        6 |&#xA;| UMREN | Número de unidades de la unidad de medida alternativa |       | DEC        |        6 |&#xA;| EAN11 | Código de barras de la unidad de medida               |       | CHAR       |       18 |&#xA;| LAENG | Longitud                                              |       | DEC        |       15 |&#xA;| BREIT | Ancho                                                 |       | DEC        |       15 |&#xA;| HOEHE | Alto                                                  |       | DEC        |       15 |&#xA;| MEABM | Unidad de medida para longitud/ancho/altura           |       | CHAR       |        3 |&#xA;| VOLUM | Volumen                                               |       | DEC        |       15 |&#xA;| VOLEH | Unidad de volumen                                     |       | CHAR       |        3 |&#xA;| BRGEW | Peso bruto                                            |       | DEC        |       15 |&#xA;| GEWEI | Unidad de peso                                        |       | CHAR       |        3 |&#xA;+-------+-------------------------------------------------------+-------+------------+----------+&#xA;&#xA;ERP_IMP_products_measure&#xA;+-------------------+------+-----------+----------+&#xA;|     fieldname     | ispk | fieldtype | fieldlen |&#xA;+-------------------+------+-----------+----------+&#xA;| ID                |      | int       |        4 |&#xA;| Code_Product      |      | numeric   |        9 |&#xA;| Code_ofClient     |      | varchar   |       24 |&#xA;| Unit_Measure      |      | varchar   |        4 |&#xA;| Factor_Conversion |      | numeric   |        9 |&#xA;+-------------------+------+-----------+----------+&#xA;&#xA;products_measure&#xA;+---------------------+------+-----------+----------+&#xA;|      fieldname      | ispk | fieldtype | fieldlen |&#xA;+---------------------+------+-----------+----------+&#xA;| Code_Product        |  Y   | numeric   |        9 |&#xA;| Unit_Measure        |  Y   | varchar   |        4 |&#xA;| Factor_Conversion   |      | numeric   |        9 |&#xA;| EAN                 |      | varchar   |       18 |&#xA;| P_Lengh             |      | numeric   |        9 |&#xA;| P_Width             |      | numeric   |        9 |&#xA;| P_Height            |      | numeric   |        9 |&#xA;| Unit_Type_Measure   |      | varchar   |        4 |&#xA;| Weight              |      | numeric   |        9 |&#xA;| Unit_Measure_Weight |      | varchar   |        4 |&#xA;| Product_Id          |      | varchar   |       24 |&#xA;+---------------------+------+-----------+----------+&#xA;&#xA;*/&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
