          SQLTask:SqlStatementSource="-- 615_erp_imp_pricing_tables_esc.sql 4.0.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_pricing_tables_esc&#xA;SELECT * FROM ERP_IMP_pricing_tables_esc&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_pricing_tables_esc insert:'&#xA;&#xA;INSERT INTO ERP_IMP_pricing_tables_esc&#xA;(Secuence,Code_Table,Quantity,ValueKey,&#xA;Code1,Code2,Code3,Code4,Code5,Code6,Code7,Code8,&#xA;Value,Base,Code_Currency,Unit_Measure,&#xA;Date_Ini,Date_Fin,Price_Min,Price_Max,Id_ERP,Del_Pos,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT ipt.Secuence,Code_Table,tesc.Quantity,ValueKey,&#xA;Code1,Code2,Code3,Code4,Code5,Code6,Code7,Code8,&#xA;tesc.Value,Base,Code_Currency,Unit_Measure,&#xA;Date_Ini,Date_Fin,Price_Min,Price_Max,ipt.Id_ERP,ipt.Del_Pos,&#xA;'0' AS Source_Create,&#xA;IIF(ISNULL(tesc.Status,'')='',0,9) AS Transfer_status,&#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_IMP_pricing_tables AS ipt&#xA;INNER JOIN&#xA;(&#xA;    SELECT TOP 900000 KNUMH AS Id_ERP,KLFN1 AS N,&#xA;    LTRIM(RTRIM(REPLACE(REPLACE(KBTER,'.',''),',','.'))) AS Value,&#xA;    KSTBM AS Quantity,Status&#xA;    FROM ERP_FCOND2&#xA;    ORDER BY KNUMH,KLFN1&#xA;) AS tesc&#xA;ON ipt.Id_ERP = tesc.Id_ERP&#xA;WHERE 1=1&#xA;AND ipt.Transfer_status = 0&#xA;&#xA;PRINT 'ERP_IMP_pricing_tables_esc update structure tipo escalado'&#xA;UPDATE ERP_IMP_pricing_structure&#xA;SET Code_Type = 'E'&#xA;FROM&#xA;(&#xA;    SELECT DISTINCT Secuence,Code_Table&#xA;    FROM ERP_IMP_pricing_tables_esc&#xA;    WHERE 1=1&#xA;    AND Transfer_status = 0&#xA;) A&#xA;WHERE 1=1&#xA;AND Transfer_Status=0&#xA;AND ERP_IMP_pricing_structure.Secuence = A.Secuence&#xA;AND ERP_IMP_pricing_structure.Code_Table = A.Code_Table&#xA;&#xA;PRINT 'ERP_IMP_pricing_tables_esc elimino DELETE en tables las que estan en escalado:'&#xA;-- no se debe eliminar de pricing tables pq puede que sea el mismo dto tanto para escalado como sin&#xA;/*&#xA;DELETE ERP_IMP_pricing_tables&#xA;FROM ERP_IMP_pricing_tables AS ipt&#xA;INNER JOIN &#xA;(&#xA;    SELECT DISTINCT Id_Erp &#xA;    FROM ERP_IMP_pricing_tables_esc&#xA;    WHERE Transfer_Status=0&#xA;) AS tesc&#xA;ON ipt.Id_Erp = tesc.Id_Erp&#xA;AND ipt.Transfer_Status=0&#xA;*/&#xA;&#xA;/* &#xA;FCOND2 - CONDICIONES - ESCALADOS&#xA;Nueva: Es como FKONM pero con otro nombre&#xA;&#xA;+-------+-----------------------------------------+-------+------------+----------+-----------+&#xA;| campo |               descripcion               | clave | tipo_datos | longitud | decimales |&#xA;+-------+-----------------------------------------+-------+------------+----------+-----------+&#xA;| KNUMH | Número de registro de condición         |     1 | CHAR       |       10 |         0 |&#xA;| KLFN1 | Número actual de línea en escala        |     2 | NUMC       |        4 |         0 |&#xA;| KSTBM | Cantidad de la condición base de escala |       | QUAN       |       15 |         3 |&#xA;| KBTER | Importe o porcentaje de la condición    |       | CURR       |       11 |         2 |&#xA;+-------+-----------------------------------------+-------+------------+----------+-----------+&#xA;&#xA;prj_pricing_tables_esc&#xA;+---------------+------+-----------+----------+&#xA;|   fieldname   | ispk | fieldtype | fieldlen |&#xA;+---------------+------+-----------+----------+&#xA;| Secuence      | Y    | varchar   |       10 |&#xA;| Code_Table    | Y    | varchar   |       12 |&#xA;| Quantity      | Y    | numeric   |        9 |&#xA;| ValueKey      | Y    | varchar   |      100 |&#xA;| Code1         |      | varchar   |       30 |&#xA;| Code2         |      | varchar   |       30 |&#xA;| Code3         |      | varchar   |       30 |&#xA;| Code4         |      | varchar   |       30 |&#xA;| Code5         |      | varchar   |       30 |&#xA;| Code6         |      | varchar   |       30 |&#xA;| Code7         |      | varchar   |       30 |&#xA;| Code8         |      | varchar   |       30 |&#xA;| Value         |      | numeric   |        9 |&#xA;| Base          |      | numeric   |        9 |&#xA;| Code_Currency |      | varchar   |       12 |&#xA;| Unit_Measure  |      | varchar   |        4 |&#xA;| Date_Ini      | Y    | varchar   |        8 |&#xA;| Date_Fin      | Y    | varchar   |        8 |&#xA;| Price_Min     |      | numeric   |        9 |&#xA;| Price_Max     |      | numeric   |        9 |&#xA;| Id_ERP        |      | varchar   |       10 |&#xA;+---------------+------+-----------+----------+&#xA;&#xA;prj_pricing_structure.Code_Type -- E: Escalado, N: Normal&#xA;*/&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
