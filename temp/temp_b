          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 210_erp_imp_accounts 3.1.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_accounts&#xA;SELECT * FROM ERP_IMP_accounts ORDER BY id DESC&#xA;UPDATE ERP_IMP_accounts SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;PRINT 'ERP_IMP_accounts insert:'&#xA;INSERT INTO ERP_IMP_accounts &#xA;(Code_ofClient,Name1,Name2,Name3,Name4,NIF,Code_Country,Code_Province,Code_Population,&#xA;ZIP,Address1,Address2,Province,Population,Hamlet,Phone1,Phone2,Fax,Web,E_mail,Code_Type&#xA;,Code_ClassificationB,Code_ClassificationC,Code_ClassificationD,Date_New,&#xA;Language_tr,House_nr,&#xA;--,Country_Bank,Pwd_Bank,CCC,Digito_Control,IBAN&#xA;--,Code_ClassificationA,Limit_Credit,Used_Credit,Risk,Blocked_Credit hay que crear una para los creditos&#xA;Iva,Class_Iva,Requi,Class_Requi,Telebox,Source_Create,Transfer_status,Status_SAP,&#xA;Transfer_Date&#xA;)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_KNA1.KUNNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.NAME1) AS Name1,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.NAME2) AS Name2,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.NAME3) AS Name3,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.NAME4) AS Name4,&#xA;CONVERT(VARCHAR(30),ERP_KNA1.STCD1) AS NIF,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.LAND1) AS Code_Country,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.REGIO) AS Code_Province,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.CITYC) AS Code_Population,&#xA;CONVERT(VARCHAR(20),ERP_KNA1.PSTLZ) AS ZIP,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.STRAS) AS Address1,&#xA;CONVERT(VARCHAR(75),ERP_KNA1.PFACH) AS Address2,&#xA;CONVERT(VARCHAR(50),ERP_KNA1.ORT02) AS Province, -- igual que hamlet&#xA;CONVERT(VARCHAR(50),ERP_KNA1.ORT01) AS Population,&#xA;CONVERT(VARCHAR(70),ERP_KNA1.ORT02) AS Hamlet,&#xA;CONVERT(VARCHAR(30),ERP_KNA1.TELF1) AS Phone1,&#xA;CONVERT(VARCHAR(30),ERP_KNA1.TELF2) AS Phone2,&#xA;CONVERT(VARCHAR(30),ERP_KNA1.TELFX) AS Fax,&#xA;CONVERT(VARCHAR(70),ERP_KNA1.TELMV) AS Web,&#xA;CONVERT(VARCHAR(70),ERP_KNA1.EMAIL) AS E_mail,&#xA;CONVERT(VARCHAR(12),ERP_KNA1.KTOKD) AS Code_Type,-- Grupo de cuentas deudor&#xA;-- Code_ClassificationB No se usa en accounts&#xA;CONVERT(VARCHAR(12),ERP_KNA1.LZONE) AS Code_ClassificationB, -- Zona de transporte acc_array(503)&#xA;CONVERT(VARCHAR(12),ERP_KNA1.ANRED) AS Code_ClassificationC, -- Tratamiento acc_array(502)&#xA;CONVERT(VARCHAR(12),ERP_KNA1.SORTL) AS Code_ClassificationD, -- Clasificacion acc_array(504)&#xA;CONVERT(VARCHAR(8),ERP_KNA1.ERDAT) AS Date_New,&#xA;CONVERT(VARCHAR(4),ERP_KNA1.SPRAS) AS Language_tr,&#xA;CONVERT(VARCHAR(10),ERP_KNA1.PSTL2) AS House_nr,&#xA;&#xA;/*&#xA;pasa a  prj_accounts_bank&#xA;CONVERT(VARCHAR(12),ERP_KNBK.BANKS) AS Country_Bank,&#xA;CONVERT(VARCHAR(12),ERP_KNBK.BANKL) AS Pwd_Bank,&#xA;CONVERT(VARCHAR(18),ERP_KNBK.BANKN) AS CCC,&#xA;CONVERT(VARCHAR(2),ERP_KNBK.BKONT) AS Digito_Control,&#xA;CONVERT(VARCHAR(34),ERP_KNBK.IBAN) AS IBAN,&#xA;&#xA;pasa a prj_accounts_credit&#xA;CONVERT(VARCHAR(12),ERP_FCLIDF.CTLPC) AS Code_ClassificationA, -- Clase de riesgo (solvencia) acc_array(500) &#xA;CONVERT(NUMERIC(11,2),ERP_FCLIDF.KLIMK) AS Limit_Credit,&#xA;CONVERT(NUMERIC(11,2),ERP_FCLIDF.OBLIG) AS Used_Credit, &#xA;CONVERT(VARCHAR(1),ERP_FCLIDF.CRBLB) AS Blocked_Credit,&#xA;*/&#xA;CONVERT(VARCHAR(12),ERP_KNA1.TATYP1) AS Iva,   -- ??? pedro tiene que consultarlo&#xA;CONVERT(VARCHAR(12),ERP_KNA1.TAXKD1) AS Class_Iva,  -- ??? pedro tiene que consultarlo&#xA;CONVERT(VARCHAR(12),ERP_KNA1.TATYP2) AS Requi,  -- ??? pedro tiene que consultarlo&#xA;CONVERT(VARCHAR(12),ERP_KNA1.TAXKD2) AS Class_Requi, -- ??? pedro tiene que consultarlo&#xA;CONVERT(VARCHAR(15),ERP_KNA1.TELBX) AS Telebox,&#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_KNA1.STATUS,'')='',0,IIF(ERP_KNA1.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_KNA1 --CLIENTES - DATOS GENERALES&#xA;/*&#xA;LEFT JOIN ERP_KNBK --CLIENTES - DATOS BANCARIOS&#xA;ON ERP_KNA1.KUNNR = ERP_KNBK.KUNNR&#xA;*/&#xA;/*&#xA;LEFT JOIN &#xA;( &#xA;    SELECT KUNNR,&#xA;    REPLACE(REPLACE(KLIMK,'.',''),',','.') AS KLIMK,&#xA;    REPLACE(REPLACE(OBLIG,'.',''),',','.') AS OBLIG&#xA;    ,CTLPC,CRBLB&#xA;    FROM ERP_FCLIDF  --CLIENTES - DATOS FINANCIEROS&#xA;) AS ERP_FCLIDF&#xA;ON ERP_KNA1.KUNNR = ERP_FCLIDF.KUNNR&#xA;*/&#xA;UNION &#xA;&#xA;-- TRANSPORTISTAS&#xA;SELECT LIFNR AS Code_ofClient,&#xA;NAME1 AS Name1,&#xA;NULL AS Name2,&#xA;NULL AS Name3,&#xA;NULL AS Name4,&#xA;NULL AS NIF,&#xA;NULL AS Code_Country,&#xA;NULL AS Code_Province,&#xA;NULL AS Code_Population,&#xA;NULL AS ZIP,&#xA;NULL AS Address1,&#xA;NULL AS Address2,&#xA;NULL AS Province, -- igual que hamlet&#xA;NULL AS Population,&#xA;NULL AS Hamlet,&#xA;NULL AS Phone1,&#xA;NULL AS Phone2,&#xA;NULL AS Fax,&#xA;NULL AS Web,&#xA;NULL AS E_mail,&#xA;'TR' AS Code_Type,-- Grupo de cuentas deudor&#xA;NULL AS Code_ClassificationB, -- Zona de transporte acc_array(503)&#xA;NULL AS Code_ClassificationC, -- Tratamiento acc_array(502)&#xA;NULL AS Code_ClassificationD, --Clasificacion acc_array(504)&#xA;NULL AS Date_New,&#xA;NULL AS Language_tr,&#xA;NULL AS House_nr,&#xA;NULL AS Iva,   -- ??? pedro tiene que consultarlo&#xA;NULL AS Class_Iva,  -- ??? pedro tiene que consultarlo&#xA;NULL AS Requi,  -- ??? pedro tiene que consultarlo&#xA;NULL AS Class_Requi, -- ??? pedro tiene que consultarlo&#xA;NULL AS Telebox,&#xA;'0' AS  Source_Create, &#xA;IIF(ISNULL(ERP_FTRANS.STATUS,'')='',0,IIF(ERP_KNA1.STATUS='T',0,9)) AS Transfer_Status, &#xA;'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-',''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FTRANS &#xA;&#xA;PRINT 'ERP_IMP_accounts update Code_ClassC - Tratamientos'&#xA;UPDATE ERP_IMP_accounts&#xA;SET Code_ClassificationC = Tratamiento.Code_ClassificationC&#xA;FROM &#xA;(&#xA;    SELECT A.Code_ofClient,B.Code AS Code_ClassificationC&#xA;    FROM&#xA;    (&#xA;        SELECT DISTINCT Code_ofClient,Code_ClassificationC &#xA;        FROM ERP_IMP_accounts&#xA;        WHERE 1=1&#xA;        AND Transfer_Status=0&#xA;        AND Code_ClassificationC!=''&#xA;    ) A&#xA;    LEFT JOIN&#xA;    (&#xA;        SELECT Code,Description&#xA;        FROM accounts_array&#xA;        WHERE Type='502'&#xA;        UNION&#xA;        SELECT Code,Description&#xA;        FROM accounts_array_tr&#xA;        WHERE Type='502'&#xA;    ) B&#xA;    ON A.Code_ClassificationC = B.Description&#xA;) Tratamiento&#xA;WHERE 1=1&#xA;AND Transfer_Status=0&#xA;AND ERP_IMP_accounts.Code_ofClient = Tratamiento.Code_ofClient&#xA;/*&#xA;ERP_FTRANS  AGENCIAS DE TRANSPORTE&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| campo |       descripcion        | clave | tipo_datos | longitud |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;| LIFNR | Código del transportista |     1 | CHAR       |       10 |&#xA;| NAME1 | Nombre del transportista |     2 | CHAR       |       35 |&#xA;+-------+--------------------------+-------+------------+----------+&#xA;&#xA;&#xA;ERP_KNA1 CLIENTES - DATOS GENERALES&#xA;+--------+-------------------------------------------+-------+------------+----------+&#xA;| campo  |                descripcion                | clave | tipo_datos | longitud |&#xA;+--------+-------------------------------------------+-------+------------+----------+&#xA;| KUNNR  | Código del cliente                        |     1 | CHAR       |       10 |&#xA;| KTOKD  | Grupo de cuentas deudor                   |       | CHAR       |        4 |&#xA;| ANRED  | Tratamiento                               |       | CHAR       |       15 |&#xA;| SORTL  | Campo de clasificación                    |       | CHAR       |       10 |&#xA;| NAME1  | Nombre del cliente                        |       | CHAR       |       35 |&#xA;| NAME2  | Nombre del cliente                        |       | CHAR       |       35 |&#xA;| NAME3  | Nombre del cliente                        |       | CHAR       |       35 |&#xA;| NAME4  | Nombre del client