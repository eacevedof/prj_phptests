          SQLTask:SqlStatementSource="-- 230_erp_imp_prj_accounts_credit 2.1.0&#xA;/*&#xA;-- !!! revisar hay duplicados&#xA;TRUNCATE TABLE ERP_IMP_prj_accounts_credit&#xA;SELECT * FROM ERP_IMP_prj_accounts_credit ORDER BY id DESC&#xA;UPDATE ERP_IMP_prj_accounts_credit SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;&#xA;PRINT 'ERP_IMP_prj_accounts_credit insert:'&#xA;INSERT INTO ERP_IMP_prj_accounts_credit &#xA;(Code_ofClient,--Code_Account&#xA;Code_Unit_Org,&#xA;Limit_Credit,Used_Credit,Blocked_Credit,Code_ClassificationA,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(24),ERP_FCLIDF.KUNNR) AS Code_ofClient,&#xA;CONVERT(VARCHAR(12),ERP_FCLIDF.BUKRS) AS Code_Unit_Org,&#xA;CONVERT(NUMERIC(11,2),A.KLIMK) AS Limit_Credit,&#xA;CONVERT(NUMERIC(11,2),A.OBLIG) AS Used_Credit, &#xA;CONVERT(VARCHAR(1),ERP_FCLIDF.CRBLB) AS Blocked_Credit,&#xA;CONVERT(VARCHAR(12),ERP_FCLIDF.CTLPC) AS Code_ClassificationA, -- Clase de riesgo (solvencia) acc_array(500) &#xA;'0' AS  Source_Create,IIF(ISNULL(ERP_FCLIDF.STATUS,'')='',0,IIF(ERP_FCLIDF.STATUS='T',0,9)) AS Transfer_Status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_FCLIDF --CLIENTES - DATOS FINANCIEROS&#xA;INNER JOIN&#xA;(&#xA;    SELECT KUNNR,&#xA;    REPLACE(REPLACE(KLIMK,'.',''),',','.') AS KLIMK,&#xA;    REPLACE(REPLACE(OBLIG,'.',''),',','.') AS OBLIG&#xA;    ,CTLPC,CRBLB&#xA;    FROM ERP_FCLIDF  --CLIENTES - DATOS FINANCIEROS&#xA;    WHERE (ISNUMERIC(KLIMK) = 1) OR (ISNUMERIC(OBLIG) = 1)&#xA;) A&#xA;ON ERP_FCLIDF.KUNNR = A.KUNNR&#xA;&#xA;/*&#xA;(&#xA;    SELECT DISTINCT KUNNR,VKORG&#xA;    FROM ERP_KNVV -- ERP_KNVV CLIENTES - DATOS POR ÁREA DE VENTAS&#xA;) ERP_KNVV&#xA;ON ERP_KNVV.KUNNR = ERP_FCLIDF.KUNNR&#xA;*/&#xA;&#xA;&#xA;/*&#xA;-- CONSULTA PEDRO&#xA;SELECT FCLIDF.BUKRS&#xA;,accounts.Code&#xA;,CAST(REPLACE(FCLIDF.KLIMK,',','.') AS numeric) AS Expr6&#xA;,CAST(REPLACE(FCLIDF.OBLIG,',','.') AS numeric) AS Expr7&#xA;,FCLIDF.CRBLB&#xA;,FCLIDF.CTLPC&#xA;FROM &#xA;(&#xA;    SELECT Status,BUKRS,KUNNR,KLIMK,OBLIG,CTLPC,CRBLB,DIAPAR&#xA;    ,DIAPAT&#xA;    FROM ERP_FCLIDF&#xA;    WHERE (ISNUMERIC(KLIMK) = 1) OR (ISNUMERIC(OBLIG) = 1)&#xA;) AS FCLIDF&#xA;INNER JOIN accounts&#xA;ON FCLIDF.KUNNR = accounts.Code_ofClient&#xA;&#xA;ERP_FCLIDF: CLIENTES - DATOS FINANCIEROS&#xA;+--------+-----------------------------+-------+------------+----------+&#xA;| campo  |         descripcion         | clave | tipo_datos | longitud |&#xA;+--------+-----------------------------+-------+------------+----------+&#xA;| BUKRS  | Código de la sociedad       |     1 | CHAR       |        4 |&#xA;| KUNNR  | Código del cliente          |     2 | CHAR       |       10 |&#xA;| KLIMK  | Límite del crédito          |       | CURR       |       15 |&#xA;| OBLIG  | Crédito utilizado           |       | CURR       |       15 |&#xA;| CTLPC  | Clase de riesgo (solvencia) |       | CHAR       |        3 |&#xA;| CRBLB  | Bloqueado para créditos     |       | CHAR       |        1 |&#xA;| DIAPAR | Días de pago reales         |       | CHAR       |        5 |&#xA;| DIAPAT | Días de pago teóricos       |       | CHAR       |        5 |&#xA;+--------+-----------------------------+-------+------------+----------+&#xA;&#xA;ERP_KNVV CLIENTES - DATOS POR ÁREA DE VENTAS&#xA;+--------+-------------------------------+-------+------------+----------+&#xA;| campo  |          descripcion          | clave | tipo_datos | longitud |&#xA;+--------+-------------------------------+-------+------------+----------+&#xA;| KUNNR  | Código del cliente            |     1 | CHAR       |       10 |&#xA;| VKORG  | Organización de ventas        |     2 | CHAR       |        4 |&#xA;| VTWEG  | Canal de distribución         |     3 | CHAR       |        2 |&#xA;| SPART  | Sector                        |     4 | CHAR       |        2 |&#xA;| ERDAT  | Fecha de creación             |       | DATS       |        8 