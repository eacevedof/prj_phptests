          SQLTask:SqlStatementSource="-- 330_erp_imp_contacts_accounts 1.1.0&#xA;/*&#xA;TRUNCATE TABLE ERP_IMP_contacts_accounts&#xA;SELECT * FROM ERP_IMP_contacts_accounts ORDER BY id DESC&#xA;UPDATE ERP_IMP_contacts_accounts SET Transfer_Status=100 WHERE Transfer_Status=0&#xA;*/&#xA;INSERT INTO ERP_IMP_contacts_accounts &#xA;(Code_Unit_Org,Code_Sales_Org,Account_ID,Contact_ID,&#xA;Source_Create,Transfer_status,Status_SAP,Transfer_Date)&#xA;&#xA;SELECT DISTINCT CONVERT(VARCHAR(12),Code_Unit_Org) AS Code_Unit_Org,&#xA;CONVERT(VARCHAR(12),Code_Sales_Org) AS Code_Sales_Org,&#xA;CONVERT(VARCHAR(24),Code_ofClientAccount) AS Account_ID,&#xA;CONVERT(VARCHAR(24),ERP_KNVK.PARNR) AS Contact_ID,&#xA;'0' AS  Source_Create,0 AS Transfer_status,'0' AS Status_SAP, &#xA;REPLACE(REPLACE(REPLACE(CONVERT(CHAR(20),GETDATE(),120),'-', ''),':',''),' ','') AS Transfer_Date&#xA;FROM ERP_KNVK --CLIENTES - PERSONAS DE CONTACTO&#xA;INNER JOIN &#xA;(&#xA;    SELECT DISTINCT Code_ofClientAccount,Code_Unit_Org,Code_Sales_Org&#xA;    --,Code_ofClientSeller&#xA;    FROM view_help_erpaccorg&#xA;)&#xA;AS accorg&#xA;ON ERP_KNVK.KUNNR = accorg.Code_ofClientAccount&#xA;-- ORDER BY PARNR&#xA;&#xA;&#xA;/*&#xA;ERP_KNVK   CLIENTES - PERSONAS DE CONTACTO&#xA;+-------+----------------------------+-------+------------+----------+-----------+----------------+&#xA;| campo |        descripcion         | clave | tipo_datos | longitud | decimales | tabla_asociada |&#xA;+-------+----------------------------+-------+------------+----------+-----------+----------------+&#xA;| PARNR | Número persona de contacto |     1 | CHAR       |       10 |         0 |                |&#xA;| KUNNR | Código del cliente         |       | CHAR       |       10 |         0 | KNA1           |&#xA;| NAME1 | Nombre del contacto        |       | CHAR       |       35 |         0 |                |&#xA;| TELF1 | Teléfono                   |       | CHAR       |       16 |         0 |                |&#xA;| ABTNR | Departamento            