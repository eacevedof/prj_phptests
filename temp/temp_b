          SQLTask:Connection="{FBE901F1-A3F7-4A9B-900B-2CAFC81814E4}"
          SQLTask:SqlStatementSource="-- 000_all_delete_months 1.0.1&#xA;/*&#xA;SELECT DISTINCT tablename&#xA;FROM view_gettable&#xA;WHERE tablename like 'ERP_IMP_%'&#xA;ORDER BY 1&#xA;*/&#xA;DECLARE @iMonths NUMERIC(6,2)&#xA;SET @iMonths = 3&#xA;&#xA;PRINT '001_all_delete_months: '+CONVERT(VARCHAR(4),@iMonths)&#xA;&#xA;PRINT 'DELETE ERP_IMP_accounts'&#xA;DELETE FROM ERP_IMP_accounts&#xA;WHERE 1=1&#xA;AND id IN&#xA;(&#xA;    SELECT id&#xA;    FROM&#xA;    (&#xA;        SELECT id,&#xA;        DATEDIFF(MONTH,&#xA;            CONVERT(DATETIME,&#xA;                SUBSTRING(Transfer_Date,1,4) +'-'+&#xA;                SUBSTRING(Transfer_Date,5,2) +'-'+&#xA;                SUBSTRING(Transfer_Date,7,2) +' '+&#xA;                SUBSTRING(Transfer_Date,9,2) +':'+&#xA;                SUBSTRING(Transfer_Date,11,2) +':'+&#xA;                SUBSTRING(Transfer_Date,13,2) +'.000', &#xA;                120),-- converts create_date into datetime&#xA;            GETDATE()) AS nmonths&#xA;        FROM ERP_IMP_accounts&#xA;        WHERE 1=1&#xA;    ) AS ERP_IMP_accounts&#xA;    WHERE 1=1&#xA;    AND nmonths&gt;@iMonths&#xA;)&#xA;&#xA;/*&#xA;HAY QUE HACER ESTO MISMO CON:&#xA;&#xA;ERP_IMP_accounts&#xA;ERP_IMP_accounts_function&#xA;ERP_IMP_accounts_hierarchy&#xA;ERP_IMP_accounts_organization&#xA;ERP_IMP_accounts_tax&#xA;ERP_IMP_collection_pending&#xA;ERP_IMP_contacts&#xA;ERP_IMP_contacts_accounts&#xA;ERP_IMP_contacts_organizations&#xA;ERP_IMP_core_users&#xA;ERP_IMP_pricing_condition&#xA;ERP_IMP_pricing_priority&#xA;ERP_IMP_pricing_secuence&#xA;ERP_IMP_pricing_structure&#xA;ERP_IMP_pricing_tables&#xA;ERP_IMP_pricing_tables_esc&#xA;ERP_IMP_pricing_trademark&#xA;ERP_IMP_prj_accounts_bank&#xA;ERP_IMP_prj_accounts_credit&#xA;ERP_IMP_prj_users_organ_seller&#xA;ERP_IMP_products&#xA;ERP_IMP_products_measure&#xA;ERP_IMP_products_organization&#xA;ERP_IMP_products_pack&#xA;ERP_IMP_products_plant&#xA;ERP_IMP_products_tax&#xA;ERP_IMP_products_tr&#xA;ERP_IMP_promo_head_z&#xA;ERP_IMP_promo_lines_z&#x